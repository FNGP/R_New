.PROGRAM muove_da23_a15()
;Movimenti da DEPOSITO PEZZO SCARTO a PARCHEGGIO LAMIERE
;
; +-- robot in repeat	
  IF SWITCH(REPEAT ) AND NOT SIG(x_fine) THEN
;   +-- anticipa la richiesta lamiera a sollevatore
    IF NOT SIG(x_soll_pz_pre) THEN
      PULSE y_ask_pz_soll,1
    ELSE
;
;     +-- comando e controllo sollevatore alto
      SIGNAL -y_soll_dw,-y_soll_up
      PULSE y_soll_up,1
    END
  END
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #fi_scarto
  JMOVE #m23_15_01
  JMOVE #fi_parc_lam
;
  RETURN
.END
.PROGRAM muove_da23_a18()
