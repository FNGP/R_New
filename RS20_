  RETURN
.END
.PROGRAM muove_da21_a3()
;Movimenti da SOLLEVATORE a PRESSA
;
; +-- controllo sollevatore basso
  IF NOT SIG(x_soll_dw) THEN
    CALL soll_dw("BREAK","ON","OFF",0)
  END
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #fi_soll_lam
  JMOVE #fi_pressa
;
  RETURN
.END
