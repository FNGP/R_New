    POINT .pos[76] = #phome                                ;posizione libera
    POINT .#pos[76] = #phome                               ;posizione libera
    POINT .t[76] = NULL
    CALL set_tool("NULL")
    POINT .pos[77] = #phome                                ;posizione libera
    POINT .#pos[77] = #phome                               ;posizione libera
    POINT .t[77] = NULL
;
;   +-- set posizione inizio ricerca
    n_start = 1
;   +-- set posizione fine ricerca
    n_end = 77
;   +-- set distanza precedente
    dist_pre = 3000
    FOR .i = n_start TO n_end
      IF DEXT(#HERE,6)=>0 AND DEXT(.#pos[.i],6)=>0 THEN
        POINT .old_t = TOOL
        IF DISTANCE(.t[.i],.old_t)>1 THEN
          TOOL .t[.i]
        END
        IF DISTANCE(HERE,.pos[.i])<dist_pre THEN
          dist_pre = DISTANCE(HERE,.pos[.i])
          pos = .i
        END
      ELSE
;
        IF DEXT(#HERE,6)<0 AND DEXT(.#pos[.i],6)<0 THEN
          POINT .old_t = TOOL
          IF DISTANCE(.t[.i],.old_t)>1 THEN
            TOOL .t[.i]
          END
          IF DISTANCE(HERE,.pos[.i])<dist_pre THEN
            dist_pre = DISTANCE(HERE,.pos[.i])
            pos = .i
          END
        END
      END
      TWAIT 0.02
    END
;
    SPEED 20 ALWAYS
    ACCURACY 1 ALWAYS
    CASE pos OF
     VALUE 1,19,20,21,23:
      COLRON
      COLRJON
      DRAW 5,0,5
      CALL p_jmove(#phome,x_home)
;
     VALUE 2:
      COLRON
      COLRJON
      CALL p_jmove(#fi_soll_lam,x_home)
;
     VALUE 3:
      CALL p_jmove(#avv1_soll_lam1,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_soll_lam,x_home)
;
     VALUE 4:
      COLRON
      COLRJON
      CALL p_jmove(#fi_pressa,x_home)
;
     VALUE 5:
      COLRON
      COLRJON
      CALL p_jmove(#m1_4_01,x_home)
;
     VALUE 6:
      COLRON
      COLRJON
      CALL p_jmove(#m1_4_02,x_home)
      CALL p_jmove(#m1_4_01,x_home)
;
     VALUE 7:
      COLRON
      COLRJON
      CALL p_jmove(#fi_deposito,x_home)
      CALL p_jmove(#m1_4_02,x_home)
      CALL p_lmove(#m1_4_01,x_home)
;
     VALUE 8:
