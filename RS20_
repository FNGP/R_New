.PROGRAM muove_da20_a1()
;Movimenti da F.I. PER FOTO a HOME
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #fi_foto
  JMOVE #phome
;
  RETURN
.END
.PROGRAM muove_da20_a15()
;Movimenti da F.I. PER FOTO a PARCHEGGIO LAMIERE
;
; +-- robot in repeat	
  IF SWITCH(REPEAT ) AND NOT SIG(x_fine) THEN
;   +-- anticipa la richiesta lamiera a sollevatore
    IF NOT SIG(x_soll_pz_pre) THEN
      PULSE y_ask_pz_soll,1
    ELSE
;
;     +-- comando e controllo sollevatore alto
      SIGNAL -y_soll_dw,-y_soll_up
      PULSE y_soll_up,1
    END
  END
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #fi_foto
  JMOVE #fi_parc_lam
;
  RETURN
.END
.PROGRAM muove_da20_a16()
;Movimenti da F.I. per FOTO a DEPOSITO ZONA 1 RAFF.
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #fi_foto
  JMOVE #m20_16_01
  JMOVE #fi_dzona1_raff
;
  RETURN
.END
