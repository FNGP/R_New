    $var[164] = $ENCODE(/F0.1,r_ofst_z[6])                 ;offset Z posizione griglia 6
    $var[165] = $ENCODE(/F0.1,r_ofst_z[7])                 ;offset Z posizione griglia 7
    $var[166] = $ENCODE(/F0.1,r_ofst_z[8])                 ;offset Z posizione griglia 8
    $var[167] = $ENCODE(/F0.1,0)                           ;disp.
    $var[168] = $ENCODE(/F0.1,0)                           ;disp.
    $var[169] = $ENCODE(/F0.1,0)                           ;disp.
    $var[170] = $ENCODE(/F0.1,0)                           ;disp.
    $var[171] = $ENCODE(/F0.1,0)                           ;disp.
    $var[172] = $ENCODE(/F0.1,0)                           ;disp.
    $var[173] = $ENCODE(/F0.1,0)                           ;disp.
    $var[174] = $ENCODE(/F0.1,0)                           ;disp.
    $var[175] = $ENCODE(/F0.1,0)                           ;disp.
    $var[176] = $ENCODE(/F0.1,0)                           ;disp.
    $var[177] = $ENCODE(/F0.1,0)                           ;disp.
    $var[178] = $ENCODE(/F0.1,0)                           ;disp.
    $var[179] = $ENCODE(/F0.1,0)                           ;disp.
    $var[180] = $ENCODE(/F0.1,0)                           ;disp.
    $var[181] = $ENCODE(/F0.1,0)                           ;disp.
    $var[182] = $ENCODE(/F0.1,0)                           ;disp.
    $var[183] = $ENCODE(/F0.1,cut_param[6])                ;monitor speed RS5
    $var[184] = $ENCODE(/F0.1,ABS(b_inc_mandrel))          ;inclusione inserimento mandrel
    $var[185] = $ENCODE(/F0.1,cut_param[2])                ;precarica anello su mandrino (mm)
    $var[186] = $ENCODE(/F0.1,cut_param[3])                ;offset mandrino X
    $var[187] = $ENCODE(/F0.1,cut_param[4])                ;offset mandrino Y
    $var[188] = $ENCODE(/F0.1,cut_param[5])                ;offset mandrino Z
    $var[189] = $ENCODE(/F0.1,ABS(b_inc_cam_p))            ;inclusione telecamera ptfe
    $var[190] = $ENCODE(/F0.1,ABS(b_inc_cam_3d))           ;inclusione telecamera 3d
    $var[191] = $ENCODE(/F0.1,cut_param[90])               ;velocit movimento lineare di controllo su telecamera 3d
    $var[192] = $ENCODE(/F0.1,cut_param[73])               ;velocit movimento circolare di controllo su telecamera 3d
    $var[193] = $ENCODE(/F0.1,ABS(b_lavora_sca))           ;lavorazione pezzi scarto (0=NO 1=SI')
    $var[194] = $ENCODE(/F0.1,0)                           ;disp.
    $var[195] = $ENCODE(/F0.1,0)                           ;disp.
    $var[196] = $ENCODE(/F0.1,0)                           ;disp.
    $var[197] = $ENCODE(/F0.1,cut_param[27])               ;ring cutting region [0=foreward   1=backward] taglio 1
    $var[198] = $ENCODE(/F0.1,cut_param[34])               ;cutting lift distance [mm] taglio 1
    $var[199] = $ENCODE(/F0.1,cut_param[35])               ;cutting start point approach distance [mm] taglio 1
    $var[200] = $ENCODE(/F0.1,cut_param[36])               ;spindle (frame) rotation around Z axis [deg] taglio 1
    $var[201] = $ENCODE(/F0.1,cut_param[37])               ;blade angle correction [+/-deg] taglio 1
    $var[202] = $ENCODE(/F0.1,cut_param[41])               ;abilita stop mandrino per soffio taglio 1
    $var[203] = $ENCODE(/F0.1,0)                           ;disp.
    $var[204] = $ENCODE(/F0.1,cut_param[44])               ;ring cutting region [0=foreward   1=backward] taglio 2
    $var[205] = $ENCODE(/F0.1,cut_param[51])               ;cutting lift distance [mm] taglio 2
    $var[206] = $ENCODE(/F0.1,cut_param[52])               ;cutting start point approach distance [mm] taglio 2
    $var[207] = $ENCODE(/F0.1,cut_param[53])               ;spindle (frame) rotation around Z axis [deg] taglio 2
    $var[208] = $ENCODE(/F0.1,cut_param[54])               ;spindle (frame) rotation around Z axis [deg] taglio 2
    $var[209] = $ENCODE(/F0.1,cut_param[58])               ;abilita stop mandrino per soffio taglio 2
    $var[210] = $ENCODE(/F0.1,cut_param[59])               ;disp.
    $var[211] = $ENCODE(/F0.1,cut_param[61])               ;ring cutting region [0=foreward   1=backward] taglio 3
    $var[212] = $ENCODE(/F0.1,cut_param[68])               ;cutting lift distance [mm] taglio 3
    $var[213] = $ENCODE(/F0.1,cut_param[69])               ;cutting start point approach distance [mm] taglio 3
    $var[214] = $ENCODE(/F0.1,cut_param[70])               ;spindle (frame) rotation around Z axis [deg] taglio 3
    $var[215] = $ENCODE(/F0.1,cut_param[71])               ;blade angle correction [+/-deg] taglio 3
    $var[216] = $ENCODE(/F0.1,cut_param[75])               ;abilita stop mandrino per soffio taglio 3
    $var[217] = $ENCODE(/F0.1,cut_param[76])               ;disp.
    $var[218] = $ENCODE(/F0.1,cut_param[78])               ;ring cutting region [0=foreward   1=backward] taglio 4
    $var[219] = $ENCODE(/F0.1,cut_param[85])               ;cutting lift distance [mm] taglio 4
    $var[220] = $ENCODE(/F0.1,cut_param[86])               ;cutting start point approach distance [mm] taglio 4
    $var[221] = $ENCODE(/F0.1,cut_param[87])               ;spindle (frame) rotation around Z axis [deg] taglio 4
    $var[222] = $ENCODE(/F0.1,cut_param[88])               ;blade angle correction [+/-deg] taglio 4
    $var[223] = $ENCODE(/F0.1,cut_param[92])               ;abilita stop mandrino per soffio taglio 4
    $var[224] = $ENCODE(/F0.1,cut_param[91])               ;Orientamento mandrino (0=NO 1=SI)
    $var[225] = $ENCODE(/F0.1,0)                           ;disp.
    $var[226] = $ENCODE(/F0.1,cut_param[12])               ;tempo soffio dopo taglio 1 RS5
    $var[227] = $ENCODE(/F0.1,cut_param[13])               ;tempo soffio dopo taglio 2 RS5
    $var[228] = $ENCODE(/F0.1,cut_param[14])               ;tempo soffio dopo taglio 3 RS5
    $var[229] = $ENCODE(/F0.1,cut_param[15])               ;tempo soffio dopo taglio 4 RS5
; da 230 a 249 inizializzate in pc3_init_var
;
   VALUE 3:
;   +-- gruppo 3: coordinate visione
    $var[250] = $ENCODE(/F0.1,r_x_vis[1])                  ;X posizione 1
    $var[251] = $ENCODE(/F0.1,r_y_vis[1])                  ;Y posizione 1
    $var[252] = $ENCODE(/F0.1,r_ang_vis[1])                ;O posizione 1
