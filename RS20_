.PROGRAM a()
;DISCONNESSIONE DEL SOCKET TCP
;
  PROMPT "socket ",socket_id 
  IF socket_id>0 THEN
    .try = 0
    DO
      .try = .try+1
      TCP_CLOSE result,socket_id
      IF result<0 THEN
;
      END
      TWAIT 0.5
    UNTIL result==0 OR .try>4
    IF result<0 THEN
      stato_conn = -1 ;Errore
    ELSE
;
      TWAIT 5
      socket_id = 0
      stato_conn = 0 ;ri-CONNECT
    END
  END
;
  RETURN
.END
.PROGRAM aaa_align()
  POINT .tool_att = TOOL
  ALIGN
  TOOL .tool_att+RZ(90)
  ALIGN
  TOOL .tool_att+RX(90)
  ALIGN
  TOOL .tool_att+RY(90)
  ALIGN
  TOOL .tool_att
  BASE RX(90)+RZ(90)+RX(180)
.END
.PROGRAM aaa_muovihome()
  CALL muovihome
.END
.PROGRAM aaa_sw_version()
;VERSIONE SW DEL PROGRAMMA
;
; Versione: GabryPonte 1.0
; Ultime modifiche: 18/02/2015
;
;            ,---. 
;            ,.'-.   \ 
;           ( ( ,'"""""-. 
;           `,X          `. 
;           /` `           `._ 
;          (            ,   ,_\ 
;          |          ,---.,'o `. 
;          |         / o   \     ) 
;           \ ,.    (      .____, 
;            \| \    \____,'     \ 
;          '`'\  \        _,____,' 
;          \  ,--      ,-'     \ 
;            ( C     ,'         \ 
;             `--'  .'           | 
;               |   |         .O | 
;             __|    \        ,-'_ 
;            / `L     `._  _,'  ' `. 
;           /    `--.._  `',.   _\  ` 
;           `-.       /\  | `. ( ,\  \ 
;          _/  `-._  /  \ |--'  (     \ 
;         '  `-.   `'    \/\`.   `.    ) 
;               \  -hrr-    \ `.  |    | 
;
.END
.PROGRAM arresta()
;Arresto immediato robot se mancano le condizioni di sicurezza
;
  HALT
.END
.PROGRAM autostart.pc()
;Processo parallelo 1
;
  b_pc_run = TRUE
  UTIMER @r_tim_pre1 = 0.1
  UTIMER @r_tim_pre2 = 0.1
  UTIMER @r_tim_pre3 = 0.1
  UTIMER @r_time_frq_sca = 0.1
  DO
    TWAIT 0.05
;   +-- controllo pc2 in run
    CALL pc1_chk_pc2_run
;   +-- controllo robot in posizione di cambio tipo
    CALL pc1_pos_cambio
;   +-- invio stato macchina a pannello
    CALL pc1_stato_macc
;   +-- lettura variabili da pannello
    CALL pc1_read_var
;   +-- seleziona ed avvia programma robot
    CALL pc1_start_robot
;   +-- contaore robot in ciclo atomatico
    CALL pc1_tim_auto
;   +-- contaore macchina accesa
    CALL pc1_tim_on
;   +-- lettura tempo ciclo robot in pressa
    CALL pc1_tim_pressa
;   +-- lettura efficienza macchina in percentuale
    CALL pc1_tim_effic
;   +-- blocco programmi da eseguire solo manualmente		
    CALL pc1_whichtask
;   +-- muovihome in esecuzione
    CALL pc1_muovih_run
;   +-- reset absolute speed
    CALL pc1_res_abs.sp
;   +-- abilitazione ciclo soft in manuale
    CALL pc1_ciclo_soft
;   +-- reset uscite a pressa se manca automatico
    CALL pc1_res_out
;   +-- controllo batterie
    CALL pc1_chk_battery
;   +-- scrittura variabili verso kladder
    CALL pc1_write_kl
;   +-- verifica parametri inseriti
    CALL pc1_control_var
  UNTIL NOT b_pc_run
;
  RETURN
.END
.PROGRAM autostart2.pc()
;Processo parallelo 2
;
  PULSE y_res_comm,1
  SIGNAL -y_cut_read_req,-y_cut_write_req,-y_cut_ack,-y_cut_integer,-y_cut_negative
  b_res_client = FALSE
  UTIMER @pc2_t_read = 0
  chk_par = 0
  read_par = 1
  debug_pc2 = FALSE
;
  b_pc_run = TRUE
  DO
;   +-- controllo parallelo 1 in run
    CALL pc2_chk_pc1_run
;
    IF b_res_client THEN
      b_res_client = FALSE
      SIGNAL -y_cut_read_req,-y_cut_write_req,-y_cut_ack,-y_cut_integer,-y_cut_negative
      PULSE y_res_comm,1
    END
    CALL pc2_cut_client
    TWAIT 0.05
  UNTIL NOT b_pc_run
;
  RETURN
.END
.PROGRAM autostart3.pc()
;Gestione interfaccia TCP Client verso PC di SuperVisione/Visione
;
; +-- impostazioni di avvio per pc3_tcp_client
  port_sv = 50001    ;porta server SuperVisione
  addr_sv[1] = 192   ;indirizzo server SuperVisione
  addr_sv[2] = 168   ;
  addr_sv[3] = 0     ;
  addr_sv[4] = 5     ;
  sk_id_sv = 0       ;socket_ID
  st_conn_sv = 0
  UTIMER @t_tcp_sv = 0
  debug_tcp = OFF
;
; +-- inizializza le stringhe libere a s.v.
  CALL pc3_init_var
; +-- setta gruppi di parametri da trasmettere
  CALL pc3_set_gruppi
;
  WHILE TRUE DO
;   +-- gestione client TCP
    CALL pc3_tcp_client
;   +-- invio allarmi a pannello
    CALL pc3_invio_all
    TWAIT 0.05
  END
;
.END
.PROGRAM autostart5.pc()
;Parallelo 5
;
  QTOOL OFF
  CP ON
  REP_ONCE ON
  STP_ONCE OFF
  ABS.SPEED OFF
  SLOW_START OFF
  AUTOSTART.PC ON
  AUTOSTART2.PC ON
  AUTOSTART3.PC ON  ;comunicazione TCP CLIENT con PC Visione/Supervisione
  AUTOSTART4.PC OFF
  AUTOSTART5.PC ON
  PREFETCH.SIGINS OFF
  b_tim_auto = FALSE
  b_tim_on = FALSE
  b_time_pressa = FALSE
  b_pc_run = TRUE
  i_fase_start = 0
  FOR .a = 1 TO i_npos_vis
    UTIMER @r_tim_raff[.a] = 0
  END
  CALL set_io
  CALL set_var
  TWAIT 5
  MC PRIME master
;
  WHILE (1) DO
;   +-- controllo stato pc3: chiusura socket in caso di arresto forzato
    CALL pc5_chk_pc3_run
    TWAIT 1
  END
.END
.PROGRAM calc_pre_raff(.i_pos_id,.&rif,.ofst_x,.ofst_y,.i_pinza_id,.i_orient_id)
;Calcolo posizioni presa da raffreddamento
;
; +-- set orientamento presa (0-180)
  CASE .i_orient_id OF
   VALUE 0:
    .i_molt = 1
;   +-- set angolo corr pezzo
    IF r_ang_vis[.i_pos_id]>180 THEN
      .ang_corr = MAXVAL(-10,MINVAL(0,r_ang_vis[.i_pos_id]-360))
    ELSE
      .ang_corr = MAXVAL(0,MINVAL(10,r_ang_vis[.i_pos_id]))
    END
    POINT .#rif = #fi_pzona1_raff
   VALUE 180:
    .i_molt = -1
;   +-- set angolo corr pezzo
    .ang_corr = MAXVAL(-10,MINVAL(10,180-r_ang_vis[.i_pos_id]))
    POINT .#rif = #fi_pzona2_raff
  END
;
  POINT #avv_praff[.i_pos_id] = f_raff+SHIFT(.rif BY r_x_vis[.i_pos_id],r_y_vis[.i_pos_id],ny[21]+50+(i_alt_gommato-naltezzang))-TRANS(0,0,0,.i_molt*.ang_corr),.#rif
  POINT #ppraff[.i_pos_id] = f_raff+SHIFT(.rif BY r_x_vis[.i_pos_id],r_y_vis[.i_pos_id],ny[21]+(i_alt_gommato-naltezzang)+r_ofst_z[.i_pos_id])-TRANS(0,0,0,.i_molt*.ang_corr),.#rif
  POINT #all_praff[.i_pos_id] = f_raff+SHIFT(.rif BY r_x_vis[.i_pos_id],r_y_vis[.i_pos_id],ny[21]+100+(i_alt_gommato-naltezzang))-TRANS(0,0,0,.i_molt*.ang_corr),.#rif
;
  RETURN
.END
.PROGRAM cam_test()
;Ciclo di test telecamere 3d e ptfe
;il pezzo da controllare viene depositato manualmente
;sul piano inclinato in posizione pezzo da tagliare (posizione alta del piano inclinato)
;
; +-- porta il robot in posizione (PIANO INCLINATO PER PASSAGGIO ANELLI A RS05)
  i_muove_verso = 25
  CALL muovi_r
;
; +-- attendi anello da prelevare da RS5
  WAIT SIG(x_rs5_okpre)
; +-- ricezione anello da RS5
  CALL ring_receiver(.gruppo)
; +-- controllo telecamera ptfe
  CALL camera_ptfe
; +-- deposito su nastro
  CALL deposito
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
; +-- set fine ciclo a RS5
  PULSE y_rs5_fine,1
; +-- reset eventuale pezzo scarto
  b_scarto_pinclb = FALSE
;
  RETURN
.END
.PROGRAM cambiotipo()
;Porta il robot in posizione di cambio pinze
;
  CALL mov_par(20,1,100,100)
  JMOVE #phome
  JMOVE #cambio01
  JMOVE #fi_cambio
;
  RETURN
.END
.PROGRAM camera_ptfe()
;  +-- controllo telecamera ptfe (sopra mandrel)
;
; +-- set tool pinza gommato
  CALL set_tool("tpscar")
; +-- porta il robot in posizione (FUORI INGOMBRO DA TELCAMERA)
  i_muove_verso = 11
  CALL muovi_r
; +-- verifica ingombro pinza codolo
  IF NOT SIG(x_pcod_su) THEN
    CALL pinzacodsu("BREAK","ON","ON",0)
  END
; +-- avvicinamento a telecameracamera
  LMOVE #cam_ptfe_01
;
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) AND NOT SIG(x_test_cam) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
;
; +-- set valori collision detection
  COLR 460,400,400,300,100,100
  COLRJ 10,10,20,30,20,30
  CALL mov_par(10,1,100,100)
; +-- posizione controllo telecamera
  LMOVE #cam_ptfe
  BREAK
;
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) AND NOT SIG(x_test_cam) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  ELSE
;   +-- acquisizione dati da telecamera
    CALL start_cam_ptfe
  END
; +-- allontanemento da telecamera
  LMOVE #cam_ptfe_01
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
  CALL mov_par(speed_fp,1,100,100)
; +-- fuori ingombro da telecamera
  JMOVE #fi_cam_ptfe
;
  RETURN
.END
.PROGRAM chk_ciclo()
;Controllo condizioni avvio ciclo
;
  .b_start = FALSE
  DO
;   +-- avvio ciclo automatico
    IF SIG(x_auto,x_start) AND NOT .b_start THEN
      CALL chk_ciclo_auto(.b_start)
    END
;
;   +-- manuale a step
    IF SIG(x_manuale,x_step) AND NOT .b_start THEN
      CALL chk_ciclo_step(.b_start)
    END
;
;   +-- manuale cambio tipo
    IF SIG(x_manuale,x_cambio) AND NOT .b_start THEN
      CALL chk_ciclo_cambi(.b_start)
    END
;
;   +-- taglio manuale
    IF SIG(x_manuale,x_man_tag) AND NOT .b_start THEN
      CALL chk_ciclo_manta(.b_start)
    END
;
;   +-- scarico manuale pressa
    IF SIG(x_manuale,x_man_scar) AND NOT .b_start THEN
      CALL chk_ciclo_mansc(.b_start)
    END
;
;   +-- test telecamere 3d e ptfe
    IF SIG(x_manuale,x_test_cam) AND NOT .b_start THEN
      CALL chk_ciclo_test(.b_start)
    END
;
    IF NOT .b_start THEN
      TWAIT 0.2
    END
  UNTIL .b_start
;
  RETURN
.END
.PROGRAM chk_ciclo_auto(.b_start)
;Avvio ciclo automatico
;
  IF NOT .b_start THEN
    .b_start = TRUE
;
  ELSE
;
    DREG (7) = 251
;                  +--------- reset fine ciclo
;                  |    +---- attesa fine ciclo resettato
    CALL res_fine("ON","OFF")
;                  +--------- reset ciclo automatico
;                  |    +---- attesa ciclo automatico resettato
    CALL res_auto("ON","ON")
;                   +-------- reset avvio ciclo
;                   |    +--- attesa start ciclo resettato
    CALL res_start("ON","ON")
;                  +--------- set ciclo manuale
;                  |    +---- attesa ciclo manuale attivo
    CALL set_manu("ON","ON")
  END
;
  RETURN
.END
.PROGRAM chk_ciclo_cambi(.b_start)
;Ciclo manuale cambio tipo
;
  IF NOT .b_start THEN
    .b_start = TRUE
  ELSE
;
    DREG (7) = 251
;                    +-------- reset cambio tipo
;                    |    +--- attesa cambio tipo resettato
    CALL res_cambio("ON","ON")
  END
;
  RETURN
.END
.PROGRAM chk_ciclo_mansc(.b_start)
;Ciclo scarico pressa manuale
;
  IF NOT .b_start THEN
    .b_start = TRUE
  ELSE
;
    DREG (7) = 251
;                     +-------- reset scarico manuale
;                     |    +--- attesa scarico manuale resettato
    CALL res_manscar("ON","ON")
  END
;
  RETURN
.END
.PROGRAM chk_ciclo_manta(.b_start)
;Ciclo taglio manuale
;
  IF NOT .b_start THEN
    .b_start = TRUE
  ELSE
;
    DREG (7) = 251
;                    +-------- reset taglio manuale
;                    |    +--- attesa taglio manuale resettato
    CALL res_mantag("ON","ON")
  END
;
  RETURN
.END
.PROGRAM chk_ciclo_step(.b_start)
;Ciclo manuale a step
;
  IF NOT .b_start THEN
    .b_start = TRUE
  ELSE
;
    DREG (7) = 251
;                  +--------- reset fine ciclo
;                  |    +---- attesa fine ciclo resettato
    CALL res_fine("ON","OFF")
;                  +--------- reset ciclo automatico
;                  |    +---- attesa ciclo automatico resettato
    CALL res_auto("ON","ON")
;                   +-------- reset avvio ciclo
;                   |    +--- attesa start ciclo resettato
    CALL res_start("ON","ON")
;                  +--------- set ciclo manuale
;                  |    +---- attesa ciclo manuale attivo
    CALL set_manu("ON","ON")
  END
;
  RETURN
.END
.PROGRAM chk_ciclo_test(.b_start)
;Ciclo test telecamere 3d e ptfe
;
  IF NOT .b_start THEN
    .b_start = TRUE
  ELSE
;
    DREG (7) = 251
;                     +-------- reset ciclo test
;                     |    +--- attesa ciclo test resettato
    CALL res_test("ON","ON")
  END
;
  RETURN
.END
.PROGRAM chk_depo_free()
;Controllo nastro di deposito libero
;
; +-- controllo zona deposito agibile
  IF SIG(x_nsca_pzini) THEN
    PULSE y_nsca_start,1
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_nsca_pzini) DO
      IF UTIMER(.@r_tim_wait)>10 THEN
        PULSE y_nsca_start,1
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM chk_pc1_run()
;Controllo parallelo 1 in run
;
  IF NOT SIG(m_pc1_run) AND TASK(1001)<>1 AND b_pc_run THEN
    PULSE m_pc1_run,1
    PCABORT
    PCEXECUTE autostart.pc
  END
;
  RETURN
.END
.PROGRAM cicloautomatico()
;Ciclo automatico SENZA taglio
;
; +-- verifica se pinza cappuccio chiusa
  IF SIG(x_pcod_ch,-x_pcod_ap) THEN
;   +-- deposito scarto cappuccio
    CALL depo_scarto("CAPPUCCIO")
  END
; +-- reset PTFE prelevato
  b_ptfe_prel = FALSE
  DO
;   +-- se non siamo a fine ciclo
    IF NOT SIG(x_fine) THEN
;     +-- presa PFTE
      CALL prelievo_ptfe
    END
  UNTIL b_ptfe_prel OR SIG(x_fine)
; +-- reset lamiera prelevata
  b_lam_prel = FALSE
; +-- reset numero tentativi presa da sollevatore
  i_nprese = 0
  DO
;   +-- se non siamo a fine ciclo
    IF NOT SIG(x_fine) THEN
;     +-- presa lamiere da sollevatore
      CALL nastropresa
    END
  UNTIL b_lam_prel OR SIG(x_fine)
; +-- set acquisizione immagine al prossimo ingresso in pressa se non siamo in manuale
  b_cam1_acq = NOT SIG(x_manuale) AND b_cam1_on
; +-- porta il robot in posizione (PRESSA)
  i_muove_verso = 3
  CALL muovi_r
; +-- reset ciclo soft
  b_ciclosoft = FALSE
; +-- scarico/carico pressa
  CALL pressa_sca_car
; +-- reset ciclo soft
  b_ciclosoft = FALSE
; +-- set chiusura sportello telecamera/spegnimento luci
  CALL tele_off
; +-- se non abbiamo la richiesta deposito a collaudo
  IF NOT SIG(x_coll_pezz) THEN
;   +-- se sistema di visione attivo
    IF b_cam1_on AND b_cam1_ready THEN
;     +-- se non siamo in manuale
      IF NOT SIG(x_manuale) THEN
;       +-- aggiorna le pos. sulla griglia di raff.
        CALL initpunti_cam
;       +-- se ho superato il numero di max foto
        IF b_nfoto_max THEN
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
;         +-- deposito scarto
          CALL depo_scarto("GOMMATO")
;         +-- deposito scarto cappuccio
          CALL depo_scarto("CAPPUCCIO")
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
          bciclofinantic = TRUE
          RETURN
        END
;       +-- lavoro con "i_npos_vis"
        .max_pos = i_npos_vis
      ELSE
;
;       +-- lavoro con 1 gruppo
        .max_pos = 1
;       +-- set gruppo libero
        vis_stato_pos[1] = 0
      END
;     +-- se telecamera pronta
      IF b_cam1_ready THEN
;       +-- reset gruppo
        .gruppo = 0
;       +-- cerca un gruppo libero
        FOR .i = 1 TO .max_pos
          IF vis_stato_pos[.i]==0 THEN
;           +-- set gruppo 
            .gruppo = .i
;           +-- deposito su raff.
            CALL depo_raff(.gruppo)
;           +-- deposito scarto cappuccio		
            CALL depo_scarto("CAPPUCCIO")
;           +-- verifica gruppo da scartare per avvio ciclo
            IF nscarti_avv<i_nscarti_avv THEN
              gruppo_scarto[.gruppo] = TRUE
              nscarti_avv = nscarti_avv+1
            END
            .i = 10000
          END
        END
;       +-- se non ha trovato nessun gruppo dove depositare
        IF .gruppo==0 THEN
;         +-- deposito scarto
          CALL depo_scarto("GOMMATO")
;         +-- deposito scarto cappuccio		
          CALL depo_scarto("CAPPUCCIO")
        END
      ELSE
;
        DREG (7) = 40
;       +-- deposito su nastro
        CALL deposito
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
;                      +-------- set fine ciclo
;                      |    +--- attesa fine ciclo selezionato
        CALL set_fine("ON","ON")
        RETURN
      END
    ELSE
;
      DREG (7) = 40
;     +-- deposito su nastro
      CALL deposito
;     +-- deposito scarto cappuccio		
      CALL depo_scarto("CAPPUCCIO")
    END
  ELSE
;
;   +-- deposito scarto
    CALL depo_scarto("GOMMATO")
;   +-- deposito scarto cappuccio	
    CALL depo_scarto("CAPPUCCIO")
;   +-- set reset collaudo anello
    CALL reset_coll
;   +-- porta il robot in posizione (CARICO PRESSA)
    i_muove_verso = 3
    CALL muovi_r
;   +-- reset ciclo soft
    b_ciclosoft = FALSE
    RETURN
  END
; +-- se visione attiva
  IF b_cam1_on THEN
;   +-- se non sono in manuale
    IF NOT SIG(x_manuale) THEN
;     +-- lavoro con "i_npos_vis"
      .max_pos = i_npos_vis
    ELSE
;
;     +-- lavoro con 1 gruppo
      .max_pos = 1
;     +-- set old gruppo
      old_gruppo = 0
    END
;   +-- reset numero tentativi presa da griglia piena
    .n_tentativi = 0
nuova_presa1:
;   +-- se tutte le posizioni di raff. sono occupate
    .b_presa = TRUE
    FOR .i = 1 TO .max_pos
      IF vis_stato_pos[.i]==0 THEN
        .b_presa = FALSE
        .i = 10000
      END
    END
    IF .b_presa THEN
;     +-- reset gruppo
      .gruppo = 0
;     +-- cerca la lamiera da prelevare
      FOR .i = 1 TO .max_pos
        IF vis_stato_pos[.i]==1 AND old_gruppo<.i THEN
;         +-- verifica tempo raffreddamento raggiunto/ciclo manuale
          IF UTIMER(@r_tim_raff[.i])>r_tim_raff OR SIG(x_manuale) THEN
;           +-- set gruppo
            .gruppo = .i
;           +-- se ho la pinza GOMMATO chiusa
            IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;             +-- deposito scarto
              CALL depo_scarto("GOMMATO")
            END
;           +-- se pinza cappuccio chiusa
            IF SIG(x_pcod_ch,-x_pcod_ap) THEN
;             +-- deposito scarto cappuccio
              CALL depo_scarto("CAPPUCCIO")
            END
;           +-- presa da raff.
            CALL prel_raff(.gruppo)
;           +-- gruppo buono o gruppo scarto e lavora scarto abilitato
            IF (b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo] THEN
;             +-- deposito su nastro
              CALL deposito
            ELSE
;
;             +-- deposito scarto
              CALL depo_scarto("GOMMATO")
            END
;           +-- reset eventuale gruppo scarto appena prelevato
            gruppo_scarto[.gruppo] = FALSE
;           +-- verifica gruppo appena depositato da scartare per avvio ciclo
            IF nscarti_avv<i_nscarti_avv THEN
              gruppo_scarto[.gruppo] = TRUE
              nscarti_avv = nscarti_avv+1
            END
            .i = 10000
;           +-- reset speed_fp per griglia piena
            b_speed_fp02 = FALSE
          ELSE
;
            old_gruppo = old_gruppo+1
            IF old_gruppo=>i_npos_vis THEN
              old_gruppo = 0
            END
          END
        END
      END
;     +-- se non ha trovato nessun gruppo dove prelevare
      IF .gruppo==0 AND NOT SIG(x_manuale) THEN
;       +-- se max tentativi presa da griglia piena, set fineciclo
        IF .n_tentativi=>1 THEN
          DREG (7) = 259
;                        +-------- set fine ciclo
;                        |    +--- attesa fine ciclo selezionato
          CALL set_fine("ON","ON")
          RETURN
        ELSE
;
;         +-- reset old gruppo prelevato
          old_gruppo = 0
;         +-- incrementa numero tentativi presa da griglia piena
          .n_tentativi = .n_tentativi+1
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
;         +-- set apertura sportello telecamera/accensione luci
          CALL tele_on
;         +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
          i_muove_verso = 20
          CALL muovi_r
          BREAK
;         +-- start acquisizione immagine se richiesto
          PULSE y_cam1_acq,1
          b_cam1_acq = FALSE
;         +-- set prima foto per prendere in carico lo stato delle posizioni
          b_prima_foto = TRUE
;         +-- aggiorna le pos. sulla griglia di raff.
          CALL initpunti_cam
          GOTO nuova_presa1
        END
      END
    END
  END
; +-- reset ciclo soft
  b_ciclosoft = FALSE
;
  RETURN
.END
.PROGRAM cicloautotaglio()
;Ciclo automatico taglio
;
; +-- reset PTFE prelevato
  b_ptfe_prel = FALSE
  DO
;   +-- se non siamo a fine ciclo
    IF NOT SIG(x_fine) THEN
;     +-- presa PFTE
      CALL prelievo_ptfe
    END
  UNTIL b_ptfe_prel OR SIG(x_fine)
;
; +-- reset lamiera prelevata
  b_lam_prel = FALSE
; +-- reset numero tentativi presa da sollevatore
  i_nprese = 0
  DO
;   +-- se non siamo a fine ciclo
    IF NOT SIG(x_fine) THEN
;     +-- presa lamiere da sollevatore
      CALL nastropresa
    END
  UNTIL b_lam_prel OR SIG(x_fine)
; +-- set acquisizione immagine al prossimo ingresso in pressa se non siamo in manuale
  b_cam1_acq = NOT SIG(x_manuale) AND b_cam1_on
; +-- porta il robot in posizione (SCARICO PRESSA)
  i_muove_verso = 3
  CALL muovi_r
; +-- reset ciclo soft
  b_ciclosoft = FALSE
; +-- scarico/carico pressa
  CALL pressa_sca_car
; +-- set chiusura sportello telecamera/spegnimento luci
  CALL tele_off
; +-- se non abbiamo la richiesta deposito a collaudo
  IF NOT SIG(x_coll_pezz) THEN
;   +-- se non sono in manuale
    IF NOT SIG(x_manuale) THEN
;     +-- aggiorna le pos. sulla griglia di raff.
      CALL initpunti_cam
;     +-- se ho superato il numero di max foto
      IF b_nfoto_max THEN
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
        bciclofinantic = TRUE
        RETURN
      END
;     +-- lavoro con "i_npos_vis"
      .max_pos = i_npos_vis
    ELSE
;
;     +-- lavoro con 1 gruppo
      .max_pos = 1
;     +-- set gruppo libero
      vis_stato_pos[1] = 0
    END
;   +-- se telecamera pronta
    IF b_cam1_ready THEN
;       +-- reset gruppo
      .gruppo = 0
;     +-- cerca un gruppo libero
      FOR .i = 1 TO .max_pos
        IF vis_stato_pos[.i]==0 THEN
;         +-- set gruppo
          .gruppo = .i
;         +-- deposito su raff.
          CALL depo_raff(.gruppo)
;         +-- deposito scarto cappuccio
          CALL depo_scarto("CAPPUCCIO")
;         +-- verifica gruppo da scartare per avvio ciclo
          IF nscarti_avv<i_nscarti_avv THEN
            gruppo_scarto[.gruppo] = TRUE
            nscarti_avv = nscarti_avv+1
          END
          .i = 10000
        END
      END
;     +-- se non ha trovato nessun gruppo dove depositare
      IF .gruppo==0 THEN
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
      END
    ELSE
;
      DREG (7) = 40
;     +-- deposito su nastro
      CALL deposito
;     +-- deposito scarto cappuccio		
      CALL depo_scarto("CAPPUCCIO")
;                    +-------- set fine ciclo
;                    |    +--- attesa fine ciclo selezionato
      CALL set_fine("ON","ON")
      RETURN
    END
  ELSE
;
;   +-- deposito scarto
    CALL depo_scarto("GOMMATO")
;   +-- deposito scarto cappuccio		
    CALL depo_scarto("CAPPUCCIO")
;   +-- set reset collaudo anello
    CALL reset_coll
;   +-- porta il robot in posizione (CARICO PRESSA)
    i_muove_verso = 3
    CALL muovi_r
;   +-- reset ciclo soft
    b_ciclosoft = FALSE
    RETURN
  END
; +-- se non sono in manuale, lavoro con "i_npos_vis"
  IF NOT SIG(x_manuale) THEN
    .max_pos = i_npos_vis
  ELSE
;
;   +-- lavoro con 1 gruppo
    .max_pos = 1
;   +-- set old gruppo
    old_gruppo = 0
  END
;
; +-- reset numero tentativi presa da griglia piena
  .n_tentativi = 0
nuova_presa1:
; +-- verifica se tutte posizioni di raff. sono occupate
  .b_presa = TRUE
  FOR .i = 1 TO .max_pos
    IF vis_stato_pos[.i]==0 THEN
      .b_presa = FALSE
;     +-- reset speed_fp per griglia piena
      b_speed_fp02 = FALSE
      .i = 10000
    END
  END
;  
; +-- se tutte le posizioni di raff. sono occupate
  IF .b_presa THEN
;   +-- reset gruppo
    .gruppo = 0
;   +-- cerca la lamiera da prelevare
    FOR .i = 1 TO .max_pos
      IF vis_stato_pos[.i]==1 AND old_gruppo<.i THEN
;       +-- verifica tempo raffreddamento raggiunto/ciclo manuale
        IF UTIMER(@r_tim_raff[.i])>r_tim_raff OR SIG(x_manuale) THEN
;         +-- set gruppo
          .gruppo = .i
;         +-- se ho la pinza GOMMATO chiusa
          IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;           +-- deposito scarto
            CALL depo_scarto("GOMMATO")
;           +-- deposito scarto cappuccio	
            CALL depo_scarto("CAPPUCCIO")
          END
;         +-- presa da raff.
          CALL prel_raff(.gruppo)
;         +-- se taglio 1/2/3/4 attivo/i, gruppo buono o gruppo scarto e lavorazione scarti attiva
          IF ((b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) AND ((b_lavora_sca==1 AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo])) THEN
;           +-- trasferimento anelli a RS05
            CALL rs5_transfer(.gruppo)
          ELSE
;
;           +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
            IF ((b_lavora_sca==1 AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo]) THEN
;             +-- deposito su nastro
              CALL deposito
            ELSE
;
;             +-- deposito scarto
              CALL depo_scarto("GOMMATO")
;             +-- set prima foto per prendere in carico lo stato delle posizioni
              b_prima_foto = TRUE
            END
          END
;         +-- reset eventuale gruppo scarto appena prelevato
          gruppo_scarto[.gruppo] = FALSE
;         +-- verifica gruppo appena depositato da scartare per avvio ciclo
          IF nscarti_avv<i_nscarti_avv THEN
            gruppo_scarto[.gruppo] = TRUE
            nscarti_avv = nscarti_avv+1
          END
          .i = 10000
;         +-- reset speed_fp per griglia piena
          b_speed_fp02 = FALSE
        ELSE
;
          old_gruppo = old_gruppo+1
          IF old_gruppo=>i_npos_vis THEN
            old_gruppo = 0
          END
        END
      END
    END
;   +-- se non ha trovato nessun gruppo dove prelevare
    IF .gruppo==0 AND NOT SIG(x_manuale) THEN
;     +-- se max tentativi presa da griglia piena, set fineciclo
      IF .n_tentativi=>1 THEN
        DREG (7) = 259
;                      +-------- set fine ciclo
;                      |    +--- attesa fine ciclo selezionato
        CALL set_fine("ON","ON")
        RETURN
      ELSE
;
;       +-- reset old gruppo prelevato
        old_gruppo = 0
;       +-- incrementa numero tentativi presa da griglia piena
        .n_tentativi = .n_tentativi+1
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- set apertura sportello telecamera/accensione luci
        CALL tele_on
;       +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
        i_muove_verso = 20
        CALL muovi_r
        BREAK
;       +-- start acquisizione immagine se richiesto
        PULSE y_cam1_acq,1
        b_cam1_acq = FALSE
;       +-- set prima foto per prendere in carico lo stato delle posizioni
        b_prima_foto = TRUE
;       +-- aggiorna le pos. sulla griglia di raff.
        CALL initpunti_cam
        GOTO nuova_presa1
      END
    END
  END
; +-- reset ciclo soft
  b_ciclosoft = FALSE
;
  RETURN
.END
.PROGRAM ciclofinale()
;Ciclo finale
;
  IF NOT bciclofinantic THEN
;   +-- set acquisizione immagine al prossimo ingresso in pressa se non siamo in manuale
    b_cam1_acq = NOT SIG(x_manuale) AND b_cam1_on
;   +-- porta il robot in posizione (PRESSA)
    i_muove_verso = 3
    CALL muovi_r
;   +-- scarico pressa
    CALL pressascarico
    i_muove_verso = 3
    CALL muovi_r
    LMOVE #fi_pressa
;   +-- se sistema di visione attivo
    IF b_cam1_on THEN
;     +-- se non sono in manuale
      IF NOT SIG(x_manuale) THEN
;       +-- aggiorna le pos. sulla griglia di raff.
        CALL initpunti_cam
;       +-- se ho superato il numero di max foto
        IF b_nfoto_max THEN
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
;         +-- deposito scarto
          CALL depo_scarto("GOMMATO")
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
          RETURN
        END
;       +-- lavoro con "i_npos_vis"
        .max_pos = i_npos_vis
      ELSE
;
;       +-- lavoro con 1 gruppo
        .max_pos = 1
      END
nuovo_deposito:
;     +-- se telecamera pronta
      IF b_cam1_ready THEN
;       +-- reset gruppo
        .gruppo = 0
;       +-- cerca un gruppo libero
        FOR .i = 1 TO .max_pos
          IF vis_stato_pos[.i]==0 THEN
;           +-- set gruppo
            .gruppo = .i
;           +-- deposito su raff.
            CALL depo_raff(.gruppo)
;           +-- deposito scarto cappuccio		
            CALL depo_scarto("CAPPUCCIO")
;           +-- verifica gruppo da scartare per avvio ciclo
            IF nscarti_avv<i_nscarti_avv THEN
              gruppo_scarto[.gruppo] = TRUE
              nscarti_avv = nscarti_avv+1
            END
            .i = 10000
          END
        END
;       +-- se non ha trovato nessun gruppo dove depositare
        IF .gruppo==0 THEN
;         +-- deposito scarto
          CALL depo_scarto("GOMMATO")
;         +-- deposito scarto cappuccio		
          CALL depo_scarto("CAPPUCCIO")
        END
      ELSE
;
        DREG (7) = 40
;       +-- deposito su nastro
        CALL deposito
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
      END
    ELSE
;
      DREG (7) = 40
;     +-- deposito su nastro
      CALL deposito
;     +-- deposito scarto cappuccio		
      CALL depo_scarto("CAPPUCCIO")
    END
  END
;
; +-- set chiusura sportello telecamera/spegnimento luci
  CALL tele_off
;
; +-- se siamo a fine O.L. vuota la griglia
  IF SIG(x_fine_ol) THEN
;   +-- se sistema di visione attivo e telecamera pronta
    IF b_cam1_on AND b_cam1_ready THEN
;     +-- se non sono in manuale
      IF NOT SIG(x_manuale) THEN
;       +-- set apertura sportello telecamera/accensione luci
        CALL tele_on
;       +-- lavoro con "i_npos_vis"
        .max_pos = i_npos_vis
;       +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
        i_muove_verso = 20
        CALL muovi_r
;       +-- start acquisizione immagine se richiesto
        BREAK
        PULSE y_cam1_acq,1
        b_cam1_acq = FALSE
;       +-- aggiorna le pos. sulla griglia di raff.
        CALL initpunti_cam
;       +-- se ho superato il numero di max foto
        IF b_nfoto_max THEN
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
;         +-- comando apertura pinza di carico
          CALL pinzacarapre("BREAK","ON","ON",0)
;         +-- comando apertura pinza di scarico
          CALL pinzascaapre("BREAK","ON","ON",0)
;         +-- comando apertura pinza di codolo
          CALL pinzacodapre("BREAK","ON","ON",0)
;         +-- comando scarico ptfe
          CALL vuoto_off("BREAK","ON","ON",0)
          RETURN
        END
      ELSE
;
;       +-- lavoro con 1 gruppo
        .max_pos = 1
;       +-- reset old gruppo
        old_gruppo = 0
      END
;     +-- se dati pronti da telecamera o siamo in manuale
      IF SIG(m_cam1_dati) OR SIG(x_manuale) THEN
        DO
;         +-- preleva e taglia tutti i pezzi sulla griglia di raff.
          FOR .i = 1 TO .max_pos
;           +-- se posizione occupata
            IF vis_stato_pos[.i]==1 AND old_gruppo<.i THEN
;             +-- verifica tempo raffreddamento raggiunto/ciclo manuale
              IF UTIMER(@r_tim_raff[.i])>r_tim_raff OR SIG(x_manuale) THEN
;               +-- set gruppo
                .gruppo = .i
;               +-- se ho la pinza GOMMATO chiusa
                IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;                 +-- deposito scarto
                  CALL depo_scarto("GOMMATO")
                END
;	              +-- presa da raff.
                CALL prel_raff(.gruppo)
;               +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
                IF ((b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo]) THEN
;                 +-- deposito su nastro
                  CALL deposito
                ELSE
;
;                 +-- deposito scarto
                  CALL depo_scarto("GOMMATO")
                END
;               +-- reset eventuale gruppo scarto
                gruppo_scarto[.gruppo] = FALSE
              ELSE
;
                old_gruppo = old_gruppo+1
                IF old_gruppo=>i_npos_vis THEN
                  old_gruppo = 0
                END
              END
            END
          END
;         +-- set raff. vuoto
          .raff_vuoto = TRUE
          FOR .i = 1 TO .max_pos
            IF vis_stato_pos[.i]==1 THEN
;             +-- reset raff. vuoto
              .raff_vuoto = FALSE
;             +-- reset old gruppo
              old_gruppo = 0
              .i = 10000
            END
          END
        UNTIL .raff_vuoto
      END
    END
  END
;
; +-- controllo pinza lamiera aperta
  IF NOT SIG(x_pcar_ap) THEN
;   +-- deposito scarto
    CALL depo_scarto("LAMIERA")
  END
; +-- controllo pinza gommato aperta
  IF NOT SIG(x_psca_ap) THEN
;   +-- deposito scarto
    CALL depo_scarto("GOMMATO")
  END
; +-- controllo se ho ptfe in pinza
  IF SIG(x_vuoto_ptfe) THEN
;   +-- deposito scarto
    CALL depo_scarto("PTFE")
  END
; +-- controllo se ho codolo in pinza
  IF SIG(x_pcod_ch) OR NOT SIG(x_pcod_ap) THEN
;   +-- deposito scarto cappuccio
    CALL depo_scarto("CAPPUCCIO")
  END
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
;
  RETURN
.END
.PROGRAM ciclofintaglio()
;Ciclo finale con taglio attivo
;
  IF NOT bciclofinantic THEN
;   +-- set acquisizione immagine al prossimo ingresso in pressa
    b_cam1_acq = TRUE
;   +-- porta il robot in posizione (SCARICO PRESSA)
    i_muove_verso = 3
    CALL muovi_r
;   +-- scarico pressa
    CALL pressascarico
    i_muove_verso = 3
    CALL muovi_r
    LMOVE #fi_pressa
;   +-- se non sono in manuale
    IF NOT SIG(x_manuale) THEN
;     +-- aggiorna le pos. sulla griglia di raff.
      CALL initpunti_cam
;     +-- se ho superato il numero di max foto
      IF b_nfoto_max THEN
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
        RETURN
      END
;     +-- lavoro con "i_npos_vis"
      .max_pos = i_npos_vis
    ELSE
;
;     +-- lavoro con 1 gruppo
      .max_pos = 1
    END
nuovo_deposito:
;   +-- se telecamera pronta
    IF b_cam1_ready THEN
;     +-- reset gruppo
      .gruppo = 0
;     +-- cerca un gruppo libero
      FOR .i = 1 TO .max_pos
        IF vis_stato_pos[.i]==0 THEN
;      	  +-- set gruppo
          .gruppo = .i
;         +-- deposito su raff.
          CALL depo_raff(.gruppo)
;         +-- deposito scarto cappuccio		
          CALL depo_scarto("CAPPUCCIO")
;         +-- verifica pezzo da scartare per avvio ciclo
          IF nscarti_avv<i_nscarti_avv THEN
            gruppo_scarto[.gruppo] = TRUE
            nscarti_avv = nscarti_avv+1
          END
          .i = 10000
        END
      END
;     +-- se non ha trovato nessun gruppo dove depositare
      IF .gruppo==0 THEN
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
      END
    ELSE
;
      DREG (7) = 40
;     +-- deposito su nastro
      CALL deposito
;     +-- deposito scarto cappuccio		
      CALL depo_scarto("CAPPUCCIO")
;                    +-------- set fine ciclo
;                    |    +--- attesa fine ciclo selezionato
      CALL set_fine("ON","ON")
    END
  END
;
; +-- controllo pinza carico aperta
  IF NOT SIG(x_pcar_ap) THEN
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
;   +-- deposito scarto
    CALL depo_scarto("LAMIERA")
;   +-- controllo se ho ptfe in pinza
    IF SIG(x_vuoto_ptfe) THEN
;     +-- deposito scarto
      CALL depo_scarto("PTFE")
    END
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  END
; +-- controllo se ho ptfe in pinza
  IF SIG(x_vuoto_ptfe) THEN
;   +-- deposito scarto
    CALL depo_scarto("PTFE")
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  END
;
; +-- set chiusura sportello telecamera/spegnimento luci
  CALL tele_off
;
; +-- se siamo a fine O.L. vuota la griglia
  IF SIG(x_fine_ol) THEN
;   +-- se telecamera pronta
    IF b_cam1_ready THEN
;     +-- se non sono in manuale
      IF NOT SIG(x_manuale) THEN
;       +-- set apertura sportello telecamera/accensione luci
        CALL tele_on
;	      +-- lavoro con "i_npos_vis"
        .max_pos = i_npos_vis
;       +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
        i_muove_verso = 20
        CALL muovi_r
;       +-- start acquisizione immagine se richiesto
        BREAK
        PULSE y_cam1_acq,1
        b_cam1_acq = FALSE
;       +-- aggiorna le pos. sulla griglia di raff.
        CALL initpunti_cam
;       +-- se ho superato il numero di max foto
        IF b_nfoto_max THEN
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
;         +-- comando apertura pinza di carico
          CALL pinzacarapre("BREAK","ON","ON",0)
;         +-- comando apertura pinza di scarico
          CALL pinzascaapre("BREAK","ON","ON",0)
          RETURN
        END
      ELSE
;
;       +-- lavoro con 1 gruppo
        .max_pos = 1
;       +-- reset old gruppo
        old_gruppo = 0
      END
;     +-- se dati pronti da telecamera o siamo in manuale
      IF SIG(m_cam1_dati) OR SIG(x_manuale) THEN
        DO
;         +-- preleva e taglia tutti i pezzi sulla griglia di raff.
          FOR .i = 1 TO .max_pos
;           +-- se posizione occupata
            IF vis_stato_pos[.i]==1 AND old_gruppo<.i THEN
;             +-- verifica tempo raffreddamento raggiunto/ciclo manuale
              IF UTIMER(@r_tim_raff[.i])>r_tim_raff OR SIG(x_manuale) THEN
;               +-- set gruppo
                .gruppo = .i
;               +-- se ho la pinza GOMMATO chiusa
                IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;                 +-- deposito scarto
                  CALL depo_scarto("GOMMATO")
                END
;	              +-- presa da raff.
                CALL prel_raff(.gruppo)
;               +-- se taglio 1/2/3/4 attivo/i, gruppo buono o gruppo scarto e lavorazione scarti attiva
                IF (b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) AND ((b_lavora_sca==1 AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo]) THEN
;                 +-- trasferimento anelli a RS05
                  CALL rs5_transfer(.gruppo)
                ELSE
;
;                 +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
                  IF ((b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo]) THEN
;                   +-- deposito su nastro
                    CALL deposito
                  ELSE
;
;                   +-- deposito scarto
                    CALL depo_scarto("GOMMATO")
                  END
                END
;               +-- reset eventuale pezzo scarto
                gruppo_scarto[.gruppo] = FALSE
                old_gruppo = old_gruppo+1
                IF old_gruppo=>i_npos_vis THEN
                  old_gruppo = 0
                END
              END
            END
          END
;         +-- set raff. vuoto
          .raff_vuoto = TRUE
;         +-- controllo griglia vuota
          FOR .i = 1 TO .max_pos
            IF vis_stato_pos[.i]==1 THEN
;             +-- reset raff. vuoto
              .raff_vuoto = FALSE
;             +-- reset old gruppo
              old_gruppo = 0
              .i = 10000
            END
          END
        UNTIL .raff_vuoto
;
;       +-- se richiesta scarico pezzi da RS5
        IF (SIG(x_rs5_okpre) OR NOT SIG(x_rs5_nopz)) AND (b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) THEN
;         +-- porta il robot in posizione (SCAMBIO CON RS5)
          i_muove_verso = 25
          CALL muovi_r
;         +-- presa anelli da RS5
          CALL ring_receiver(.gruppo)
;         +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
          IF NOT b_scarto_pinclb OR (b_lavora_sca AND b_scarto_pinclb) THEN
;           +-- se controllo telecamera incluso
            IF b_inc_cam_p AND (NOT b_scarto_3d OR NOT b_inc_cam_3d) THEN
;             +-- controllo telecamera ptfe
              CALL camera_ptfe
            END
;           +-- controllo mandrel pronto e non scarto da camera ptfe o 3d
            IF NOT SIG(x_mandrel_nok) THEN
              IF (NOT b_scarto_cam_p OR NOT b_inc_cam_p) AND (NOT b_scarto_3d OR NOT b_inc_cam_3d) THEN
                IF b_inc_mandrel THEN
;                 +-- montaggio mandrel
                  CALL mandrel
                END
              END
            END
            IF (b_scarto_cam_p AND b_inc_cam_p) THEN
;             +-- deposito scarto
              CALL depo_scarto("GOMMATO")
            ELSE
              IF (b_scarto_3d AND b_inc_cam_3d) THEN
;               +-- deposito scarto
                CALL depo_scarto("GOMMATO")
              ELSE
;               +-- deposito su nastro
                CALL deposito
              END
            END
          ELSE
;
;           +-- deposito scarto
            CALL depo_scarto("GOMMATO")
          END
;         +-- reset eventuale pezzo scarto
          b_scarto_pinclb = FALSE
        END
      END
    END
  END
; +-- set fine ciclo a RS5
  PULSE y_rs5_fine,1
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
; +-- comando apertura pinza di carico
  CALL pinzacarapre("BREAK","ON","ON",0)
; +-- comando apertura pinza di scarico
  CALL pinzascaapre("BREAK","ON","ON",0)
;
  RETURN
.END
.PROGRAM cicloiniziale()
;Ciclo iniziale
;
; +-- set ciclo soft
  b_ciclosoft = TRUE
; +-- reset PTFE prelevato
  b_ptfe_prel = FALSE
  DO
;   +-- se non siamo a fine ciclo
    IF NOT SIG(x_fine) THEN
;
;     +-- verifica di non avere gia ptfe
      IF SIG(x_vuoto_ptfe) THEN
        CALL depo_scarto("PTFE")
      END
;
;     +-- presa PFTE
      CALL prelievo_ptfe
    END
  UNTIL b_ptfe_prel OR SIG(x_fine)
; +-- reset lamiera prelevata
  b_lam_prel = FALSE
; +-- reset numero tentativi presa da sollevatore
  i_nprese = 0
  DO
;   +-- se non siamo a fine ciclo
    IF NOT SIG(x_fine) THEN
;     +-- presa lamiere da sollevatore con nastro
      CALL nastropresa
    END
  UNTIL b_lam_prel OR SIG(x_fine)
; +-- reset pressa caricata per eventuale fine ciclo
  b_pressa_car = FALSE
; +-- se non sono a fine ciclo
  IF SIG(x_fine) GOTO fine
; +-- porta il robot in posizione (CARICO PRESSA)
  i_muove_verso = 3
  CALL muovi_r
; +-- carico pressa
  CALL pressacarico
; +-- set pressa caricata per eventuale fine ciclo
  b_pressa_car = TRUE
; +-- reset ciclo soft
  b_ciclosoft = FALSE
; +-- verifica stato teorico griglia
  CALL stato_raff_rob
; +-- numero pezzi su griglia eccessivo
  IF ngruppi=>ngruppi_vis AND b_cam1_on THEN
;   +-- reset ciclo soft
    b_ciclosoft = FALSE
;   +-- set apertura sportello telecamera/accensione luci
    CALL tele_on
;   +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
    i_muove_verso = 20
    CALL muovi_r
;   +-- start acquisizione immagine se richiesto
    BREAK
    PULSE y_cam1_acq,1
    b_cam1_acq = FALSE
;   +-- aggiorna le pos. sulla griglia di raff.
    CALL initpunti_cam
;   +-- set ciclo soft
    b_ciclosoft = TRUE
  END
;
; +-- se non sono a fine ciclo e non ho superato il numero di max foto
  IF NOT SIG(x_fine) AND NOT b_nfoto_max THEN
;   +-- verifica stato reale griglia
    CALL stato_raff_vis
;   +-- se griglia piena/oltre max pezzi da visione
    IF ngruppi=>ngruppi_vis AND b_cam1_on THEN
;     +-- reset ciclo soft
      b_ciclosoft = FALSE
;     +-- preleva le lamiere
      DO
;       +-- cerca la lamiera da prelevare
        FOR .i = 1 TO ngruppi_vis
          IF vis_stato_pos[.i]==1 AND old_gruppo<.i THEN
;           +-- verifica tempo raffreddamento raggiunto/ciclo manuale
            IF UTIMER(@r_tim_raff[.i])>r_tim_raff OR SIG(x_manuale) THEN
;             +-- set gruppo
              .gruppo = .i
;             +-- se ho la pinza GOMMATO chiusa
              IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;               +-- deposito scarto
                CALL depo_scarto("GOMMATO")
              END
;             +-- presa da griglia
              CALL prel_raff(.gruppo)
;             +-- se taglio 1/2/3/4 attivo/i, gruppo buono o gruppo scarto e taglio scarto abilitato
              IF (b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) AND ((b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo]) THEN
;               +-- trasferimento anelli a RS05
                CALL rs5_transfer(.gruppo)
              ELSE
;
;               +-- gruppo buono o gruppo scarto e taglio scarto abilitato
                IF (b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo] THEN
;                 +-- deposito su nastro
                  CALL deposito
                ELSE
;
;                 +-- deposito scarto
                  CALL depo_scarto("GOMMATO")
                END
              END
;             +-- reset eventuale gruppo di pezzi scarto appena prelevato
              gruppo_scarto[.gruppo] = FALSE
;             +-- decrementa i gruppi di anelli presenti sulla griglia
              ngruppi = ngruppi-1
              .i = 10000
            END
          ELSE
;
            old_gruppo = old_gruppo+1
            IF old_gruppo=>ngruppi_vis THEN
              old_gruppo = 0
            END
          END
        END
      UNTIL ngruppi<ngruppi_vis
    END
  ELSE
;
fine:
;   +-- set fine ciclo a RS5
    PULSE y_rs5_fine,1
;   +-- se ho prelevato il PTFE
    IF b_ptfe_prel THEN
;     +-- deposito scarto
      CALL depo_scarto("PTFE")
    END
;   +-- se ho prelevato le lamiere
    IF b_lam_prel THEN
;     +-- deposito scarto
      CALL depo_scarto("LAMIERA")
    END
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  END
;
  RETURN
.END
.PROGRAM ciclosolotaglio()
;Ciclo solo taglio
;
nuova_foto:
; +-- set apertura sportello telecamera/accensione luci
  CALL tele_on
; +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
  i_muove_verso = 20
  CALL muovi_r
; +-- start acquisizione immagine se richiesto
  BREAK
  PULSE y_cam1_acq,1
  b_cam1_acq = FALSE
; +-- aggiorna le pos. sulla griglia di raff.
  CALL initpunti_cam
; +-- se non sono in fineciclo
  IF NOT SIG(x_fine) THEN
    DO
;     +-- preleva e taglia tutti i pezzi sulla griglia di raff.
      FOR .i = 1 TO i_npos_vis
;       +-- se posizione occupata
        IF vis_stato_pos[.i]==1 AND old_gruppo<.i THEN
;         +-- verifica tempo raffreddamento raggiunto
          IF UTIMER(@r_tim_raff[.i])>r_tim_raff THEN
;           +-- set gruppo
            .gruppo = .i
;           +-- se ho la pinza GOMMATO chiusa
            IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;             +-- deposito scarto
              CALL depo_scarto("GOMMATO")
            END
;           +-- presa da raff.
            CALL prel_raff(.gruppo)
;           +-- se taglio 1/2/3/4 attivo/i, gruppo buono o gruppo scarto e lavorazione scarti attiva
            IF ((b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) AND ((b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo])) THEN
;             +-- trasferimento anelli a RS05
              CALL rs5_transfer(.gruppo)
            ELSE
;
;             +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
              IF ((b_lavora_sca AND gruppo_scarto[.gruppo]) OR NOT gruppo_scarto[.gruppo]) THEN
;               +-- deposito su nastro
                CALL deposito
              ELSE
;
;               +-- deposito scarto
                CALL depo_scarto("GOMMATO")
              END
            END
;           +-- reset eventuale gruppo scarto
            gruppo_scarto[.gruppo] = FALSE
          ELSE
;
            old_gruppo = old_gruppo+1
            IF old_gruppo=>i_npos_vis THEN
              old_gruppo = 0
            END
          END
;         +-- siamo a fineciclo
          IF SIG(x_fine) THEN
            .i = 1000
          END
        END
;       +-- set raff. vuoto
        .raff_vuoto = TRUE
;       +-- verifica griglia vuota
        FOR .ii = 1 TO i_npos_vis
          IF vis_stato_pos[.ii]==1 THEN
;           +-- reset raff. vuoto
            .raff_vuoto = FALSE
            .ii = 10000
          END
        END
      END
    UNTIL .raff_vuoto OR SIG(x_fine)
  END
;
; +-- se richiesta scarico pezzi da RS5
  IF (SIG(x_rs5_okpre) OR NOT SIG(x_rs5_nopz)) AND (b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) THEN
;   +-- porta il robot in posizione (SCAMBIO CON RS5)
    i_muove_verso = 25
    CALL muovi_r
;   +-- presa anelli da RS5
    CALL ring_receiver(.gruppo)
;   +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
    IF NOT b_scarto_pinclb OR (b_lavora_sca AND b_scarto_pinclb) THEN
;     +-- se controllo telecamera incluso
      IF b_inc_cam_p AND (NOT b_scarto_3d OR NOT b_inc_cam_3d) THEN
;       +-- controllo telecamera ptfe
        CALL camera_ptfe
      END
;     +-- controllo mandrel pronto e non scarto da camera ptfe o 3d
      IF NOT SIG(x_mandrel_nok) THEN
        IF (NOT b_scarto_cam_p OR NOT b_inc_cam_p) AND (NOT b_scarto_3d OR NOT b_inc_cam_3d) THEN
          IF b_inc_mandrel THEN
;           +-- montaggio mandrel
            CALL mandrel
          END
        END
      END
      IF (b_scarto_cam_p AND b_inc_cam_p) THEN
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
      ELSE
        IF (b_scarto_3d AND b_inc_cam_3d) THEN
;         +-- deposito scarto
          CALL depo_scarto("GOMMATO")
        ELSE
;         +-- deposito su nastro
          CALL deposito
        END
      END
    ELSE
;
;     +-- deposito scarto
      CALL depo_scarto("GOMMATO")
    END
;   +-- reset eventuale pezzo scarto
    b_scarto_pinclb = FALSE
  END
; +-- set fine ciclo a RS5
  PULSE y_rs5_fine,1
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
;
  RETURN
.END
.PROGRAM ciclotagliono()
;Ciclo senza taglio
;
; +-- se ciclo manuale attivo
  IF SIG(x_manuale,-x_auto,-x_start) THEN
    b_ciclosoft = FALSE
    CALL cicloautomatico
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  ELSE
;
;   +-- ciclo iniziale
    CALL cicloiniziale
;
;   +-- loop del ciclo automatico
    WHILE SIG(x_auto,x_start,-x_fine) DO
      CALL chk_pc1_run
      CALL cicloautomatico
      IF SIG(x_fine) THEN
        CALL ciclofinale
      END
      bciclofinantic = FALSE
;     +-- reset pressa caricata per eventuale fine ciclo
      b_pressa_car = FALSE
    END
;
;   +-- se ho caricato la pressa, e non sono entrato in ciclo automatico, eseguo il ciclo fintaglio
    IF b_pressa_car THEN
;     +-- reset pressa caricata per eventuale fine ciclo
      b_pressa_car = FALSE
      CALL ciclofinale
    END
;
;                  +-------- reset fine ciclo
;                  |    +--- attesa fine ciclo resettato
    CALL res_fine("ON","OFF")
;                  +-------- reset ciclo automatico
;                  |    +--- attesa ciclo automatico resettato
    CALL res_auto("ON","ON")
;                   +-------- reset avvio ciclo
;                   |    +--- attesa start ciclo resettato
    CALL res_start("ON","ON")
;                  +-------- set ciclo manuale
;                  |    +--- attesa ciclo manuale attivo
    CALL set_manu("ON","ON")
;
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
    BREAK
  END
;
  RETURN
.END
.PROGRAM ciclotagliosi()
;Ciclo di lavoro con taglio abilitato
;
; +-- se ciclo manuale attivo
  IF SIG(x_manuale,-x_auto,-x_start) THEN
    b_ciclosoft = FALSE
    CALL cicloautotaglio
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  ELSE
;
;   +-- ciclo iniziale
    CALL cicloiniziale
;	
;   +-- loop del ciclo automatico
    WHILE SIG(x_auto,x_start,-x_fine) DO
      CALL chk_pc1_run
      CALL cicloautotaglio
      IF SIG(x_fine) THEN
        CALL ciclofintaglio
      END
      bciclofinantic = FALSE
;     +-- reset pressa caricata per eventuale fine ciclo
      b_pressa_car = FALSE
    END
;
;   +-- se ho caricato la pressa, e non sono entrato in ciclo automatico, eseguo il ciclo fintaglio
    IF b_pressa_car THEN
;     +-- reset pressa caricata per eventuale fine ciclo
      b_pressa_car = FALSE
      CALL ciclofintaglio
    END
;
;                  +-------- reset fine ciclo
;                  |    +--- attesa fine ciclo resettato
    CALL res_fine("ON","OFF")
;                  +-------- reset ciclo automatico
;                  |    +--- attesa ciclo automatico resettato
    CALL res_auto("ON","ON")
;                   +-------- reset avvio ciclo
;                   |    +--- attesa start ciclo resettato
    CALL res_start("ON","ON")
;                  +-------- set ciclo manuale
;                  |    +--- attesa ciclo manuale attivo
    CALL set_manu("ON","ON")
;
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  END
;
  RETURN
.END
.PROGRAM depo_raff(.gruppo)
;Deposito su griglia
;
  CASE .gruppo OF
   VALUE 1:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 1)
    i_muove_verso = 16
    CALL muovi_r
;   +-- deposito su posizione 1
    CALL depo_raff1
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona1_raff
;
   VALUE 2:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 1)
    i_muove_verso = 16
    CALL muovi_r
;   +-- deposito su posizione 2
    CALL depo_raff2
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona1_raff
;
   VALUE 3:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 17
    CALL muovi_r
;   +-- deposito su posizione 3
    CALL depo_raff3
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona2_raff
;
   VALUE 4:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 17
    CALL muovi_r
;   +-- deposito su posizione 4
    CALL depo_raff4
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona2_raff
;
   VALUE 5:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 17
    CALL muovi_r
;   +-- deposito su posizione 5
    CALL depo_raff5
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona2_raff
;
   VALUE 6:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 17
    CALL muovi_r
;   +-- deposito su posizione 6
    CALL depo_raff6
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona2_raff
;
   VALUE 7:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 1)
    i_muove_verso = 16
    CALL muovi_r
;   +-- deposito su posizione 7
    CALL depo_raff7
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona1_raff
;
   VALUE 8:
;   +-- porta il robot in posizione (DEPOSITO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 17
    CALL muovi_r
;   +-- deposito su posizione 8
    CALL depo_raff8
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_dzona2_raff
   ANY :
;
;   +-- set ciclo soft
    b_ciclosoft = TRUE
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
    DREG (7) = 194
    HALT
  END
;
; +-- set stato gruppo visione
  vis_stato_pos[.gruppo] = 1
; +-- set stato gruppo robot
  rob_stato_pos[.gruppo] = 1
; +-- set timer raffreddamento
  UTIMER @r_tim_raff[.gruppo] = 0
;
  RETURN
.END
.PROGRAM depo_raff1()
;Deposito anello su posizione raffreddamento 1
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #avv_draff[1]
; +-- set valori collision detection
  COLR 460,300,300,500,200,500
  COLRJ 10,10,20,30,20,30
  LMOVE #ddraff[1]
; +-- reset step
  CALL reset_step
; +-- in modo manuale attesa freccia sinistra per modo step
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- attesa consenso step successivo		
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
; +-- comando apertura pinza gommato
  CALL pinzascaapre("BREAK","ON","ON",0)
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all_draff[1]
;
  RETURN
.END
