.END
.PROGRAM pinzascachiude(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando chiusura pinza gommato
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_psca_ap,-y_psca_ch
    PULSE y_psca_ch,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_psca_ch,-x_psca_ap) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
