.PROGRAM pinzascaapre(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando apertura pinza gommato
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_psca_ch,-y_psca_ap
    PULSE y_psca_ap,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_psca_ap,-x_psca_ch) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
