.PROGRAM mandrel()
;Inserimento mandrel
;
; +-- set tool pinza gommato
  CALL set_tool("tpscar")
; +-- porta il robot in posizione (FUORI INGOMBRO DA MANDREL)
  i_muove_verso = 22
  CALL muovi_r
; +-- sicura pinza codolo alta
  CALL pinzacodsu("BREAK","ON","ON",0)
; +-- controllo zona mandrel libera
  WAIT SIG(x_mandrel_dw,-x_mandrel_up)
; +-- avvicinamento
  LMOVE #pmandrel_01
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
; +-- set valori collision detection
  COLR 460,400,400,500,300,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(10,1,100,100)
; +-- posizione montaggio mandrel
  LMOVE #pmandrel
; +-- se non sono in manuale
  IF NOT SIG(x_manuale) OR SIG(x_man_tag) OR SIG(x_man_scar) THEN
;   +-- verifica presenza mandrel
    BREAK
    WAIT SIG(x_mandrel_ok)
;	+-- verifica se non sono in time out assenza mandrel
    IF NOT SIG(x_to_mandr) THEN
;     +-- comando salita montaggio
      CALL mandrelup("BREAK","ON","ON",0)
;     +-- comando discesa montaggio	  
      CALL mandreldw("BREAK","ON","ON",0)
    ELSE
      CALL set_fine("ON","ON")
    END
  END
; +-- in modo manuale step successivo
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
; +-- allontanamento
  WAIT SIG(x_mandrel_dw,-x_mandrel_up)
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
  LMOVE #pmandrel_01
  LMOVE #fi_mandrel
;
  RETURN
.END
.PROGRAM mandreldw(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando discesa inserimento mandrel
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_mandrel_up,-y_mandrel_dw
    PULSE y_mandrel_dw,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_mandrel_dw,-x_mandrel_up) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 14
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        SIGNAL -y_mandrel_up,-y_mandrel_dw
        PULSE y_mandrel_dw,1
      END
      TWAIT 0.05
    END
  END
;
  IF .r_tim_attesa>0 THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM mandrelup(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando salita inserimento mandrel
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_mandrel_up,-y_mandrel_dw
    PULSE y_mandrel_up,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(-x_mandrel_dw,x_mandrel_up) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 43
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        SIGNAL -y_mandrel_up,-y_mandrel_dw
        PULSE y_mandrel_up,1
      END
      TWAIT 0.05
    END
  END
;
  IF .r_tim_attesa>0 THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM master()
;Ciclo master pressa I4I5
;
; +-- controlli iniziali
  CALL inizializza
;
; +-- ciclo principale
  CALL principale
;
  RETURN
.END
.PROGRAM modificapunto(.&ppunto,.&ttool,.nnumeropunto)
;Modifica punto richiesto
;
  POINT .tool_att = TOOL
; +-- abilita pagina TEACH
  b_p_teach = 1
  .b_muovi = FALSE
; +-- invio valori off.set a pannello
  r_temp_nx = nx[.nnumeropunto]
  r_temp_ny = ny[.nnumeropunto]
  r_temp_nz = nz[.nnumeropunto]
  temp_nx = 0
  temp_ny = 0
  temp_nz = 0
;
; +-- reset step
  CALL reset_step
;
  WAIT b_teach OR SIG(x_step)
; +-- reset step
  CALL reset_step
  IF b_teach THEN
;
;   +-- abilita pulsanti pannello ESA
    SIGNAL y_abilita_esa
    DO
      TWAIT 0.05
;     +-- PULSANTE X+
      IF SIG(x_xp) THEN
        r_temp_nx = r_temp_nx+nstepoffset
        temp_nx = temp_nx+nstepoffset
        WAIT SIG(-x_xp)
        .b_muovi = TRUE
      END
;     +-- PULSANTE X-
      IF SIG(x_xm) THEN
        r_temp_nx = r_temp_nx-nstepoffset
        temp_nx = temp_nx-nstepoffset
        WAIT SIG(-x_xm)
        .b_muovi = TRUE
      END
;     +-- PULSANTE Y+
      IF SIG(x_yp) THEN
        r_temp_ny = r_temp_ny+nstepoffset
        temp_ny = temp_ny+nstepoffset
        WAIT SIG(-x_yp)
        .b_muovi = TRUE
      END
;     +-- PULSANTE Y-
      IF SIG(x_ym) THEN
        r_temp_ny = r_temp_ny-nstepoffset
        temp_ny = temp_ny-nstepoffset
        WAIT SIG(-x_ym)
        .b_muovi = TRUE
      END
;     +-- PULSANTE Z+
      IF SIG(x_zp) THEN
        r_temp_nz = r_temp_nz+nstepoffset
        temp_nz = temp_nz+nstepoffset
        WAIT SIG(-x_zp)
        .b_muovi = TRUE
      END
;     +-- PULSANTE Z-
      IF SIG(x_zm) THEN
        r_temp_nz = r_temp_nz-nstepoffset
        temp_nz = temp_nz-nstepoffset
        WAIT SIG(-x_zm)
        .b_muovi = TRUE
      END
;
;     +-- esegue il movimento
      IF .b_muovi THEN
;       +-- set TOOL
        TOOL .ttool
;       +-- scelta numero punto per applicare la frame
        CASE .nnumeropunto OF
         VALUE 1:
;         +-- posizione scarico pressa
          POINT ptemp = SHIFT(.ppunto BY temp_ny,-temp_nz,-temp_nx)
          SPEED 10
          ACCURACY 1
          LMOVE ptemp
          BREAK
         VALUE 2:
;         +-- posizione carico pressa
          POINT ptemp = SHIFT(.ppunto BY temp_ny,-temp_nz,-temp_nx)
          SPEED 10
          ACCURACY 1
          LMOVE ptemp
          BREAK
         VALUE 3:
;         +-- posizione sollevatore 1
          POINT #ptemp = f_soll+SHIFT(.ppunto BY -(nz[3]+temp_nz),-(nx[3]+temp_nx)+((i_diam_est-ndiametron)/(2*SIN(60))),ny[3]+temp_ny+(i_alt_lam-naltezzan)),#fi_soll_lam
          SPEED 10
          ACCURACY 1
          LMOVE #ptemp
          BREAK
         VALUE 4:
;         +-- posizione sollevatore 2
          POINT #ptemp = f_soll+SHIFT(.ppunto BY -(nz[4]+temp_nz),-(nx[4]+temp_nx)+((i_diam_est-ndiametron)/(2*SIN(60))),ny[4]+temp_ny+(i_alt_lam-naltezzan)),#fi_soll_lam
          SPEED 10
          ACCURACY 1
          LMOVE #ptemp
          BREAK
         ANY :
;
          DREG (7) = 74
          HALT
        END
        .b_muovi = FALSE
      END
;		  +-- antiripetizione conferma valori
      IF NOT b_conf THEN
        .b_conf = FALSE
      END
;     +-- conferma valori da pannello
      IF b_conf AND NOT .b_conf THEN
        .b_conf = TRUE
        nx[.nnumeropunto] = nx[.nnumeropunto]+temp_nx
        ny[.nnumeropunto] = ny[.nnumeropunto]+temp_ny
        nz[.nnumeropunto] = nz[.nnumeropunto]+temp_nz
;       +-- invio valori off.set a pannello
        r_temp_nx = nx[.nnumeropunto]
        r_temp_ny = ny[.nnumeropunto]
        r_temp_nz = nz[.nnumeropunto]
        temp_nx = 0
        temp_ny = 0
        temp_nz = 0
        TOOL .ttool
      END
    UNTIL NOT b_teach
  END
; +-- reset abilitazione comandi esa
  SIGNAL -y_abilita_esa
; +-- reset abilitazione pagina teach
  b_p_teach = 0
; +-- reset teach temporanei
  r_temp_nx = 0
  r_temp_ny = 0
  r_temp_nz = 0
; +-- set tool
  TOOL .tool_att
;  CALL set_tool("tpcar")
;
  RETURN
.END
.PROGRAM mov_par(.r_speed,.r_accur,.r_accel,.r_decel)
;Gestione parametri di movimento
;
  SPEED MINVAL(.r_speed,100) ALWAYS
  ACCURACY .r_accur ALWAYS
  ACCEL MINVAL(.r_accel,100) ALWAYS
  DECEL MINVAL(.r_decel,100) ALWAYS
;
  RETURN
.END
.PROGRAM muove_da11_a1()
;Movimenti da FI CONTROLLO TELECAMERA PTFE a HOME
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #m11_1_01
  JMOVE #m11_1_02
  JMOVE #m11_1_03
  JMOVE #phome
;
  RETURN
.END
