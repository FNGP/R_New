    $var[1149] = $vid_stringa1[50]+" "+$vid_stringa2[50]
;
   VALUE 8:
;	  +-- parametri visione
    $var[1150] = $par_vis[1]
    $var[1151] = $par_vis[2]
    $var[1152] = $par_vis[3]
    $var[1153] = $par_vis[4]
