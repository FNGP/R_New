    POINT .#pos[9] = #all2_soll_lam1
    POINT .t[9] = tpcar
    POINT .pos[10] = #avv1_soll_lam2
    POINT .#pos[10] = #avv1_soll_lam2
    POINT .t[10] = tpcar
    POINT .pos[11] = #pre_soll_lam2
    POINT .#pos[11] = #pre_soll_lam2
    POINT .t[11] = tpcar
    POINT .pos[12] = #all2_soll_lam2
    POINT .#pos[12] = #all2_soll_lam2
    POINT .t[12] = tpcar
    POINT .pos[13] = #ppresdep
    POINT .#pos[13] = #ppresdep
    POINT .t[13] = tpcar
