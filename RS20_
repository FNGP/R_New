.PROGRAM muovi_r()
;Movimenti da ogni stazione
;
; +-- crea il programma da eseguire
  $program = "muove_da"+$ENCODE(/F0.0,i_muove_da)+"_a"+$ENCODE(/F0.0,i_muove_verso)
; +-- verifica l'esistenza del programma
  IF EXISTPGM($program) THEN
    SCALL $program
  ELSE
;
    DREG (7) = 41
    HALT
  END
  i_muove_da = i_muove_verso
;
  RETURN
.END
.PROGRAM muovihome()
;Porta il robot nella posizione di inizio ciclo
;
; +-- set valori collision detection
  COLR 100,100,100,100,100,100
  COLRJ 30,10,10,10,10,10
  COLROFF
  COLRJOFF
  .b_pos_ok = TRUE
  FOR .i_joint = 1 TO 6
    IF ABS(DEXT(#phome,.i_joint)-DEXT(#HERE,.i_joint))>0.5 OR SIG(x_home) THEN
      .b_pos_ok = FALSE
    END
  END
; +-- se fuori dalla posizione di HOME
  IF NOT .b_pos_ok THEN
;
;   +-- set luce telecamera ON
    PULSE y_luce_tele_on,1
;
;   +-- assegnazione punti macchina per verifica posizione
    CALL set_tool("NULL")
    POINT .pos[1] = #phome
    POINT .#pos[1] = #phome
    POINT .t[1] = NULL
;
    CALL set_tool("tpcar")
    POINT .pos[2] = #fi_soll_lam
    POINT .#pos[2] = #fi_soll_lam
    POINT .t[2] = tpcar
    POINT .pos[3] = #avv1_soll_lam1
    POINT .#pos[3] = #avv1_soll_lam1
    POINT .t[3] = tpcar
    POINT .pos[4] = #fi_pressa
    POINT .#pos[4] = #fi_pressa
    POINT .t[4] = tpcar
    POINT .pos[5] = #m1_4_01
    POINT .#pos[5] = #m1_4_01
    POINT .t[5] = tpcar
    POINT .pos[6] = #m1_4_02
    POINT .#pos[6] = #m1_4_02
    POINT .t[6] = tpcar
    POINT .pos[7] = #fi_deposito
    POINT .#pos[7] = #fi_deposito
    POINT .t[7] = tpcar
    POINT .pos[8] = #pre_soll_lam1
    POINT .#pos[8] = #pre_soll_lam1
    POINT .t[8] = tpcar
