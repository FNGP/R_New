    $var[135] = $ENCODE(/F0.1,cut_param[59])               ;speed mandrino taglio 3
    $var[136] = $ENCODE(/F0.1,cut_param[62])               ;speed taglio 3
    $var[137] = $ENCODE(/F0.1,cut_param[67])               ;altezza anello per taglio 3
    $var[138] = $ENCODE(/F0.1,cut_param[63])               ;angolo taglio 3
    $var[139] = $ENCODE(/F0.1,cut_param[66])               ;profondita' taglio 3
    $var[140] = $ENCODE(/F0.1,cut_param[65])               ;diametro taglio 3
    $var[141] = $ENCODE(/F0.1,cut_param[64])               ;angolo di spoglia taglio 3
    $var[142] = $ENCODE(/F0.1,0)                           ;disp.
    $var[143] = $ENCODE(/F0.1,cut_param[76])               ;speed mandrino taglio 4
    $var[144] = $ENCODE(/F0.1,cut_param[79])               ;speed taglio 4
    $var[145] = $ENCODE(/F0.1,cut_param[84])               ;altezza anello per taglio 4
    $var[146] = $ENCODE(/F0.1,cut_param[80])               ;angolo taglio 4
    $var[147] = $ENCODE(/F0.1,cut_param[83])               ;profondita' taglio 4
    $var[148] = $ENCODE(/F0.1,cut_param[82])               ;diametro taglio 4
    $var[149] = $ENCODE(/F0.1,cut_param[81])               ;angolo di spoglia taglio 4
    $var[150] = $ENCODE(/F0.1,0)                           ;disp.
    $var[151] = $ENCODE(/F0.1,r_speed_01)                  ;speed in pressa
    $var[152] = $ENCODE(/F0.1,r_speed_02)                  ;speed fuori pressa
    $var[153] = $ENCODE(/F0.1,r_speed_03)                  ;monitor speed
    $var[154] = $ENCODE(/F0.1,ABS(b_cam1_on))              ;sistema di visione 1 abilitato
    $var[155] = $ENCODE(/F0.1,i_colt1_max)                 ;max tagli coltello 1
    $var[156] = $ENCODE(/F0.1,i_colt2_max)                 ;max tagli coltello 2
    $var[157] = $ENCODE(/F0.1,0)                           ;disp.
    $var[158] = $ENCODE(/F0.1,i_npos_vis)                  ;numero posizioni da utilizzare sulla griglia
    $var[159] = $ENCODE(/F0.1,r_ofst_z[1])                 ;offset Z posizione griglia 1
    $var[160] = $ENCODE(/F0.1,r_ofst_z[2])                 ;offset Z posizione griglia 2
    $var[161] = $ENCODE(/F0.1,r_ofst_z[3])                 ;offset Z posizione griglia 3
