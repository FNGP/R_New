      r_y_vis[3] = VAL($var[259])                          ;Y posizione 3
      r_ang_vis[3] = VAL($var[260])                        ;angolo posizione 3
      vis_stato_pos[3] = VAL($var[261])                    ;stato posizione (0=vuota 1=anello trovato 2=anomalia)
;
      r_x_vis[4] = VAL($var[262])                          ;X posizione 4
      r_y_vis[4] = VAL($var[263])                          ;Y posizione 4
      r_ang_vis[4] = VAL($var[264])                        ;angolo posizione 4
      vis_stato_pos[4] = VAL($var[265])                    ;stato posizione (0=vuota 1=anello trovato 2=anomalia)
;
      r_x_vis[5] = VAL($var[266])                          ;X posizione 5
      r_y_vis[5] = VAL($var[267])                          ;Y posizione 5
      r_ang_vis[5] = VAL($var[268])                        ;angolo posizione 5
      vis_stato_pos[5] = VAL($var[269])                    ;stato posizione (0=vuota 1=anello trovato 2=anomalia)
;
      r_x_vis[6] = VAL($var[270])                          ;X posizione 6
      r_y_vis[6] = VAL($var[271])                          ;Y posizione 6
      r_ang_vis[6] = VAL($var[272])                        ;angolo posizione 6
      vis_stato_pos[6] = VAL($var[273])                    ;stato posizione (0=vuota 1=anello trovato 2=anomalia)
;
      r_x_vis[7] = VAL($var[274])                          ;X posizione 7
      r_y_vis[7] = VAL($var[275])                          ;Y posizione 7
      r_ang_vis[7] = VAL($var[276])                        ;angolo posizione 7
      vis_stato_pos[7] = VAL($var[277])                    ;stato posizione (0=vuota 1=anello trovato 2=anomalia)
;
      r_x_vis[8] = VAL($var[278])                          ;X posizione 8
      r_y_vis[8] = VAL($var[279])                          ;Y posizione 8
      r_ang_vis[8] = VAL($var[280])                        ;angolo posizione 8
      vis_stato_pos[8] = VAL($var[281])                    ;stato posizione (0=vuota 1=anello trovato 2=anomalia)
;
      IF SIG(m_cam1_dati) THEN
        SIGNAL m_ok_gr3
      END
    END
   VALUE 8:
;   +-- parametri visione
    $par_vis[1] = $var[1150]
    $par_vis[2] = $var[1151]
    $par_vis[3] = $var[1152]
    $par_vis[4] = $var[1153]
    $par_vis[5] = $var[1154]
    $par_vis[6] = $var[1155]
    $par_vis[7] = $var[1156]
    $par_vis[8] = $var[1157]
