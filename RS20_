  END
  CALL reset_step
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all_praff[8]
;
  RETURN
.END
.PROGRAM prelievo_ptfe()
;Prelievo PTFE
;
; +-- reset ptfe prelevato
  b_ptfe_prel = FALSE
; +-- porta il robot in posizione (CASSETTO PTFE)
  i_muove_verso = 28
  CALL muovi_r
; +-- verifica ingombro pinza codolo
  IF NOT SIG(x_pcod_giu) THEN
    CALL pinzacodgiu("BREAK","ON","ON",0)
  END
;
; +-- robot in repeat	
  IF SWITCH(REPEAT ) AND NOT SIG(x_fine) THEN
;   +-- anticipa la richiesta lamiera a sollevatore
    IF NOT SIG(x_soll_pz_pre) THEN
      PULSE y_ask_pz_soll,1
    ELSE
;
;     +-- comando e controllo sollevatore alto
      SIGNAL -y_soll_dw,-y_soll_up
      PULSE y_soll_up,1
    END
  END
;
  DO
;   +-- PTFE 1
    IF SIG(x_pres_pfte1,x_ok_pre_ptfe) THEN
;     +-- reset F.I. a cassetto ptfe
      SIGNAL -y_fi_cass_ptfe
;
      CALL set_tool("tventose")
      CALL mov_par(speed_fp,100,100,100)
      JMOVE #avv_ptfe1_01
      LMOVE #avv_ptfe1_02
      IF SIG(x_manuale) THEN
;       +-- reset step
        CALL reset_step
;       +-- attesa consenso a step successivo
        WAIT SIG(x_step)
;       +-- reset step
        CALL reset_step
      END
;     +-- comando vuoto presa ptfe
      CALL vuoto_on("BREAK","ON","",0)
;     +-- ricerca ptfe
      CALL z_ricerca(#min_ptfe1,1045,50)
      CALL mov_par(speed_fp,100,100,100)
      LMOVE #all_ptfe1_01
      MVWAIT 50
      IF SIG(x_manuale) THEN
;       +-- reset step
        CALL reset_step
;       +-- attesa consenso a step successivo
        WAIT SIG(x_step)
;       +-- reset step
        CALL reset_step
      END
      CALL mov_par(speed_fp,300,100,100)
      JMOVE #fi_cass_ptfe
      MVWAIT 50
;     +-- set F.I. a cassetto ptfe
      SIGNAL y_fi_cass_ptfe
;
;     +-- controllo ptfe prelevato
      IF SIG(x_vuoto_ptfe) THEN
;       +-- set ptfe prelevato
        b_ptfe_prel = TRUE
      ELSE
;
        CALL vuoto_off("BREAK","ON","ON",0)
        DREG (7) = 7
        CALL prosegui
      END
    ELSE
;
;     +-- PTFE 2
      IF SIG(x_pres_ptfe2,x_ok_pre_ptfe) THEN
;       +-- reset F.I. a cassetto ptfe
        SIGNAL -y_fi_cass_ptfe
;
        CALL set_tool("tventose")
        CALL mov_par(speed_fp,100,100,100)
        JMOVE #avv_ptfe2_01
        LMOVE #avv_ptfe2_02
        IF SIG(x_manuale) THEN
;         +-- reset step
          CALL reset_step
;         +-- attesa consenso a step successivo
          WAIT SIG(x_step)
;         +-- reset step
          CALL reset_step
        END
;       +-- comando vuoto presa ptfe
        CALL vuoto_on("BREAK","ON","",0)
;       +-- ricerca ptfe
        CALL z_ricerca(#min_ptfe2,1045,50)
        CALL mov_par(speed_fp,100,100,100)
        LMOVE #all_ptfe2_01
        MVWAIT 50
        IF SIG(x_manuale) THEN
;         +-- reset step
          CALL reset_step
;         +-- attesa consenso a step successivo
          WAIT SIG(x_step)
;         +-- reset step
          CALL reset_step
        END
        CALL mov_par(speed_fp,300,100,100)
        JMOVE #fi_cass_ptfe
        MVWAIT 50
;       +-- set F.I. a cassetto ptfe
        SIGNAL y_fi_cass_ptfe
;
;       +-- controllo ptfe prelevato
        IF SIG(x_vuoto_ptfe) THEN
;         +-- set ptfe prelevato
          b_ptfe_prel = TRUE
        ELSE
;
          CALL vuoto_off("BREAK","ON","ON",0)
          DREG (7) = 291
          CALL prosegui
        END
      ELSE
;       +-- avviso ptfe assente 
        UTIMER .@r_tim_wait = 0
        IF NOT SIG(x_pres_pfte1,x_pres_ptfe2) OR NOT SIG(x_ok_pre_ptfe) THEN
          DREG (4) = 275
          PULSE y_ptfe_nok,1
;         +-- allarme time out ptfe assente		  
          WHILE ((NOT SIG(x_pres_pfte1) AND NOT SIG(x_pres_ptfe2)) OR NOT SIG(x_ok_pre_ptfe)) AND NOT SIG(x_fine) DO
            IF UTIMER(.@r_tim_wait)>300 THEN
              UTIMER .@r_tim_wait = 0
              DREG (7) = 278
              CALL set_fine("ON","ON")
            END
            TWAIT 0.02
          END
        END
      END
    END
  UNTIL b_ptfe_prel OR SIG(x_fine)
;
  RETURN
.END
.PROGRAM pressa_sca_car()
;Scarico/carico pressa
;
; +-- set valori collision detection
  COLR 500,130,130,450,450,450
  COLRJ 30,10,10,30,15,30
; +-- set tool
  CALL set_tool("tpcar")
; +-- set tempo robot in pressa
  UTIMER @r_tim_rob_pr = 0
; +-- set timer per gestione speed fuori pressa
  UTIMER @r_tim_speed = 0
  CALL mov_par(r_speed_01,300,50,50)
  LMOVE #avv_prpresa_01
  CALL mov_par(r_speed_01,300,300,300)
; +-- comando apertura pinza gommato  
  CALL pinzascaapre("BREAK","ON","ON",0)
; +-- comando pinza cappuccio
  CALL pinzacodgiu("BREAK","ON","ON",0)
  CALL pinzacodapre("BREAK","ON","ON",0)
; +-- reset comando ad estrattori
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
    SIGNAL -y_arb_est_ind,-y_arb_est_ava
  END
; +-- attesa consensi da arburg
  WAIT SIG(x_arb_ap,-x_arb_ch,x_arb_est_ind,-x_arb_est_ava)
;
  IF UTIMER(@r_tim_speed)=>10 THEN
    r_speed_auto = MINVAL(100,MAXVAL(40,r_speed_auto-2))
  ELSE
;
    IF UTIMER(@r_tim_speed)=>5 THEN
      r_speed_auto = MINVAL(100,MAXVAL(40,r_speed_auto-1))
    ELSE
;
      IF UTIMER(@r_tim_speed)<2 THEN
        r_speed_auto = MAXVAL(40,MINVAL(100,r_speed_auto+2))
      END
    END
  END
;
; +-- se sistema di visione attivo
  IF b_cam1_on THEN
;   +-- set apertura sportello telecamera/accensione luci
    CALL tele_on
  END
; +-- muovi in pressa blocco pressa tool scarico
  SIGNAL -y_arb_ok_ch,-y_arb_st_free
  CALL mov_par(r_speed_01,30,300,300)
  LMOVE #avv_prpresa_02
;
  IF SIG(x_auto) AND NOT SIG(x_manuale) AND NOT b_ciclosoft THEN
    SIGNAL y_arb_est_ava;-y_arb_est_ind
  END
  ACCURACY 5
  LMOVE #pprespresa
; +-- a POS4 disabilitazione modo step
  CALL reset_step
; +-- estrattore avanti
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
;   +-- comando estrattore avanti
;       dopo 5 secondi esce ed arresta il ciclo
    SIGNAL y_arb_est_ava;-y_arb_est_ind
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_arb_est_ava,-x_arb_est_ind) DO
;
      IF UTIMER(.@r_tim_wait)>10 THEN
        SAENVCHKON
        SAEND
        DREG (7) = 50
        CALL pinzascaapre("BREAK","ON","ON",0)
        CALL pinzacodapre("BREAK","ON","ON",0)
;       +-- attesa estrattore indietro
        WAIT SIG(x_arb_est_ind,-x_arb_est_ava)
        CALL mov_par(10,1,100,100)
        LMOVE #all_prpresa_01
        LMOVE #all_prpresa_11
        LMOVE #all_prpresa_02
        LMOVE #fi_pressa
        JMOVE #phome
        HALT
      END
      TWAIT 0.05
    END
;   +-- resetto comando estrattori
    SIGNAL -y_arb_est_ava,-y_arb_est_ind
;   +-- comando chiusura pinza cappuccio
    CALL pinzacodchiude("BREAK","ON","OFF",0.3)
;   +-- comando salita pinza cappuccio
    CALL pinzacodsu("BREAK","ON","OFF",0.3)
;   +-- comando chiusura pinza gommato
    CALL pinzascachiude("BREAK","ON","ON",0)
;   +-- record posizione prima dell'estrazione
    HERE pos_sca_att[1]
;
;   +-- siamo in automatico
    SATXYZ 0,1,1
    SADEGREE 10,25,25,25,25,25
    SALENGTH 10,10,10,10,30,30
    SAGAIN 100,90,90,60,40,40
    SAGRAV 14,0,0,150
    SAENVCHKOFF
    SASTART
  ELSE
;
;   +-- modifica POS4
    CALL modificapunto(&pprespresa,&tpscar,1)
;   +-- crea le nuove posizioni		
    CALL initpunti
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo, attendo anche estrattore basso
    WAIT SIG(x_step,x_arb_est_ind,-x_arb_est_ava)
;   +-- reset step
    CALL reset_step
  END
; +-- rientro gli estrattori in auto
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
    SIGNAL -y_arb_est_ava,y_arb_est_ind
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_arb_est_ind,-x_arb_est_ava) DO
      IF UTIMER(.@r_tim_wait)>15 THEN
        DREG (7) = 51
        CALL prosegui
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.05
    END
    SAGAIN 100,100,100,100,100,100
    SAENVCHKON
    SAEND
    SIGNAL -y_arb_est_ava,-y_arb_est_ind
  END
; +-- controllo ulteriore sulla posizione degli estrattori
  WAIT SIG(x_arb_est_ind,-x_arb_est_ava)
;
; +-- record posizione dopo estrazione
  HERE pos_sca_att[2]
;
; +-- aggiorna freq, scarico pressa
  i_time_frq_sca = UTIMER(@r_time_frq_sca)
  UTIMER @r_time_frq_sca = 0
  POINT point = pprespresa
  CALL mov_par(r_speed_01,10,300,300)
  ACCEL 10
  LMOVE #all_prpresa_01
;
;
;
;
; +-- carico
; +-- se non siamo a fine ciclo
  IF NOT SIG(x_fine) THEN
;   +-- set tool
    CALL set_tool("tpcar")
    LMOVE #avv_prdep_02
;
;   +-- se sistema di visione attivo start acquisizione immagine	
    IF b_cam1_on AND b_cam1_acq THEN
      MVWAIT 50
      PULSE y_cam1_acq,1
      b_cam1_acq = FALSE
    END
;
    ACCURACY 0.1
    LMOVE #ppresdep
    TWAIT 0.2
    MVWAIT 1.5 MM
;   +-- disabilitazione modo step
    CALL reset_step
    IF NOT SIG(x_manuale) THEN
;     +-- reset lamiera prelevata
      b_lam_prel = FALSE
;     +-- comando apertura pinza lamiera
      CALL pinzacarapre("BREAK","ON","ON",0)
;     +-- reset comando vuoto per rilascio pfte
      CALL vuoto_off("","ON","ON",0)
    ELSE
;
;     +-- modifica POS5
      CALL modificapunto(&ppresdep,&tpcar,2)
;     +-- crea le nuove posizioni		
      CALL initpunti
;     +-- reset step
      CALL reset_step
;     +-- attesa consenso a step successivo
      WAIT SIG(x_step)
;     +-- reset step
      CALL reset_step
    END
;
    CALL mov_par(r_speed_01,30,300,300)
    LMOVE #all_prdep_01
    CALL mov_par(r_speed_01,300,300,300)
    LMOVE #all_prdep_02
    LMOVE #fi_pressa
;   +-- abilito chiusura pressa, sono fuori ingombro
;   !!!!! Pericolo !!!!! non variare MVWAIT rischio collisione
    MVWAIT 10
    SIGNAL y_arb_ok_ch,y_arb_st_free
    IF SIG(x_manuale) THEN
;     +-- reset step
      CALL reset_step
;     +-- attesa consenso a step successivo
      WAIT SIG(x_step)
;     +-- reset step
      CALL reset_step
    ELSE
;
;     +-- attesa chiusura pressa
      WAIT SIG(-x_arb_ap)
    END
;   +-- set valori collision detection
    COLR 500,500,500,500,500,500
    COLRJ 50,50,50,50,50,50
  ELSE
;
    CALL mov_par(r_speed_01,300,300,300)
;   +-- se sistema di visione attivo start acquisizione immagine	
    IF b_cam1_on AND b_cam1_acq THEN
      MVWAIT 50
      PULSE y_cam1_acq,1
      b_cam1_acq = FALSE
    END
    LMOVE #all_prpresa_02
    LMOVE #fi_pressa
    bciclofinantic = TRUE
  END
; +-- se non sono in manuale, incrementa i pezzi scaricati
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
    i_pezzi_tot = i_pezzi_tot+1
    i_pezzi_parz = i_pezzi_parz+1
  END
; +-- verifica corsa estrazione fuori range
  IF SIG(x_auto) THEN
    IF DISTANCE(pos_sca_att[1],pos_sca_att[2])>35 THEN
      DREG (7) = 257
;                     +-------- set fine ciclo
;                     |    +--- attesa fine ciclo selezionato
      CALL set_fine("ON","ON")
    END
  END
;
  RETURN
.END
.PROGRAM pressacarico()
;Carico pressa
;
; +-- set valori collision detection
  COLR 460,400,400,500,300,500
  COLRJ 10,10,20,30,20,30
; +-- set tool pinza lamiera
  CALL set_tool("tpcar")
; +-- se ciclo manuale attivo
  IF SIG(x_manuale) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
  CALL mov_par(r_speed_01,300,300,300)
  JMOVE #avv_prdep_01
; +-- attesa consensi da Arburg
  WAIT SIG(x_arb_ap,-x_arb_ch,x_arb_est_ind,-x_arb_est_ava)
; +-- muovo in pressa e blocco la pressa
  SIGNAL -y_arb_ok_ch,-y_arb_st_free
  LMOVE #avv_prdep_02
  ACCURACY 0.1
  LMOVE #ppresdep
  TWAIT 0.2
  MVWAIT 1.5 MM
; +-- disabilitazione modo step
  CALL reset_step
  IF NOT SIG(x_manuale) THEN
;   +-- reset lamiera prelevata
    b_lam_prel = FALSE
;   +-- comando apertura pinza lamiera
    CALL pinzacarapre("BREAK","ON","ON",0)
;   +-- reset comando vuoto per rilascio pfte
    CALL vuoto_off("","ON","ON",0)
  ELSE
;
;   +-- modifica POS5
    CALL modificapunto(&ppresdep,&tpcar,2)
;   +-- crea le nuove posizioni		
    CALL initpunti
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
;
  CALL mov_par(r_speed_01,30,300,300)
  LMOVE #all_prdep_01
  CALL mov_par(r_speed_01,300,300,300)
  LMOVE #all_prdep_02
  LMOVE #fi_pressa
; +-- abilito chiusura pressa, sono fuori ingombro
; !!!!! Pericolo !!!!! non variare MVWAIT rischio collisione
;  MVWAIT 100
  MVWAIT 10
  SIGNAL y_arb_ok_ch,y_arb_st_free
  IF SIG(x_manuale) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  ELSE
;
;   +-- attesa chiusura pressa
    WAIT SIG(-x_arb_ap)
  END
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
;
  RETURN
.END
.PROGRAM pressascarico()
;Scarico pressa
;
; +-- set valori collision detection
  COLR 500,130,130,450,450,450
  COLRJ 30,10,10,30,15,30
; +-- set tool
  CALL set_tool("tpcar")
; +-- set tempo robot in pressa
  UTIMER @r_tim_rob_pr = 0
; +-- set timer per gestione speed fuori pressa
  UTIMER @r_tim_speed = 0
  CALL mov_par(r_speed_01,300,50,50)
  LMOVE #avv_prpresa_01
  CALL mov_par(r_speed_01,300,300,300)
; +-- comando apertura pinza gommato
  CALL pinzascaapre("BREAK","ON","ON",0)
; +-- comando pinza cappuccio
  CALL pinzacodgiu("BREAK","ON","ON",0)
  CALL pinzacodapre("BREAK","ON","ON",0)
; +-- reset comando ad estrattori
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
    SIGNAL -y_arb_est_ind,-y_arb_est_ava
  END
; +-- attesa consensi da arburg
  WAIT SIG(x_arb_ap,-x_arb_ch,x_arb_est_ind,-x_arb_est_ava)
;
  IF UTIMER(@r_tim_speed)=>10 THEN
    r_speed_auto = MINVAL(100,MAXVAL(40,r_speed_auto-2))
  ELSE
;
    IF UTIMER(@r_tim_speed)=>5 THEN
      r_speed_auto = MINVAL(100,MAXVAL(40,r_speed_auto-1))
    ELSE
;
      IF UTIMER(@r_tim_speed)<2 THEN
        r_speed_auto = MAXVAL(40,MINVAL(100,r_speed_auto+2))
      END
    END
  END
;
; +-- se sistema di visione attivo
  IF b_cam1_on THEN
;   +-- set apertura sportello telecamera/accensione luci
    CALL tele_on
  END
; +-- muovi in pressa blocco pressa tool scarico
  SIGNAL -y_arb_ok_ch,-y_arb_st_free
  CALL mov_par(r_speed_01,30,300,300)
  LMOVE #avv_prpresa_02
;
  IF SIG(x_auto) AND NOT SIG(x_manuale) AND NOT b_ciclosoft THEN
    SIGNAL y_arb_est_ava;-y_arb_est_ind
  END
  ACCURACY 5
  LMOVE #pprespresa
; +-- a POS4 disabilitazione modo step
  CALL reset_step
; +-- estrattore avanti
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
;   +-- comando estrattore avanti
;       dopo 5 secondi esce ed arresta il ciclo
    SIGNAL y_arb_est_ava;-y_arb_est_ind
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_arb_est_ava,-x_arb_est_ind) DO
;
      IF UTIMER(.@r_tim_wait)>10 THEN
        SAENVCHKON
        SAEND
        DREG (7) = 50
        CALL pinzascaapre("BREAK","ON","ON",0)
        CALL pinzacodapre("BREAK","ON","ON",0)
;       +-- attesa estrattore indietro
        WAIT SIG(x_arb_est_ind,-x_arb_est_ava)
        CALL mov_par(10,1,100,100)
        LMOVE #all_prpresa_01
        LMOVE #all_prpresa_11
        LMOVE #all_prpresa_02
        LMOVE #fi_pressa
        JMOVE #phome
        HALT
      END
      TWAIT 0.05
    END
;   +-- resetto comando estrattori
    SIGNAL -y_arb_est_ava,-y_arb_est_ind
;   +-- comando chiusura pinza cappuccio
    CALL pinzacodchiude("BREAK","ON","OFF",0.3)
;   +-- comando salita pinza cappuccio
    CALL pinzacodsu("BREAK","ON","OFF",0.3)
;   +-- comando chiusura pinza gommato
    CALL pinzascachiude("BREAK","ON","ON",0)
;   +-- record posizione prima dell'estrazione
    HERE pos_sca_att[1]
;
;   +-- siamo in automatico
    SATXYZ 0,1,1
    SADEGREE 10,25,25,25,25,25
    SALENGTH 10,10,10,10,30,30
    SAGAIN 100,90,90,60,40,40
    SAGRAV 14,0,0,150
    SAENVCHKOFF
    SASTART
  ELSE
;
;   +-- modifica POS4
    CALL modificapunto(&pprespresa,&tpscar,1)
;   +-- crea le nuove posizioni		
    CALL initpunti
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo, attendo anche estrattore basso
    WAIT SIG(x_step,x_arb_est_ind,-x_arb_est_ava)
;   +-- reset step
    CALL reset_step
  END
; +-- rientro gli estrattori in auto
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
    SIGNAL -y_arb_est_ava,y_arb_est_ind
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_arb_est_ind,-x_arb_est_ava) DO
      IF UTIMER(.@r_tim_wait)>15 THEN
        DREG (7) = 51
        CALL prosegui
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.05
    END
    SAGAIN 100,100,100,100,100,100
    SAENVCHKON
    SAEND
    SIGNAL -y_arb_est_ava,-y_arb_est_ind
  END
; +-- controllo ulteriore sulla posizione degli estrattori
  WAIT SIG(x_arb_est_ind,-x_arb_est_ava)
;
; +-- record posizione dopo estrazione
  HERE pos_sca_att[2]
;
; +-- aggiorna freq, scarico pressa
  i_time_frq_sca = UTIMER(@r_time_frq_sca)
  UTIMER @r_time_frq_sca = 0
  POINT point = pprespresa
  CALL mov_par(r_speed_01,10,300,300)
  ACCEL 10
  LMOVE #all_prpresa_01
  CALL mov_par(r_speed_01,300,300,300)
  LMOVE #all_prpresa_02
  LMOVE #fi_pressa
;
; +-- se non sono in manuale, incrementa i pezzi scaricati
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) THEN
    i_pezzi_tot = i_pezzi_tot+1
    i_pezzi_parz = i_pezzi_parz+1
  END
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
;
; +-- start acquisizione immagine se richiesto
  IF b_cam1_on AND b_cam1_acq THEN
;   +-- muovi verso fuori ingombro per foto
    i_muove_verso = 20
    CALL muovi_r
    BREAK
;   +-- start acquisizione immagine
    PULSE y_cam1_acq,1
    b_cam1_acq = FALSE
    TWAIT 2
  END
;
; +-- verifica corsa estrazione fuori range
  IF SIG(x_auto) THEN
    IF DISTANCE(pos_sca_att[1],pos_sca_att[2])>35 THEN
      DREG (7) = 257
;                     +-------- set fine ciclo
;                     |    +--- attesa fine ciclo selezionato
      CALL set_fine("ON","ON")
    END
  END
;
  RETURN
.END
.PROGRAM principale()
;Ciclo di lavoro
;
  b_ciclo_run = TRUE
  WHILE b_ciclo_run DO
;   +-- reset step
    CALL reset_step
;   +-- ciclo automatico o cicli manuali speciali
    CALL chk_ciclo
;   +-- definizione base,tool,frame
    CALL origini
;   +-- crea le nuove posizioni
    CALL initpunti
;
;
;
;   +-- manuale a step
    IF SIG(x_manuale,x_step) AND b_cam1_on AND b_cam1_ready THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      b_ciclosoft = TRUE
;     +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
      i_muove_verso = 20
      CALL muovi_r
;     +-- aggiorna le pos. sulla griglia di raff.
      CALL initpunti_cam
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
    END
;
;
;   +-- se richiesta ciclo taglio manuale
    IF SIG(x_manuale,x_man_tag) THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      b_ciclosoft = FALSE
      IF NOT SIG(x_alm_plc) AND b_cam1_on AND b_cam1_ready AND SIG(y_tcp_sv_ok) THEN
;       +-- set uscite ad inizio ciclo
        CALL set_out
;       +-- reset ultima pos. di presa su griglia
        old_gruppo = 0
;       +-- set prima foto per prendere in carico lo stato delle posizioni
        b_prima_foto = TRUE
;       +-- set dati acquisiti a telecamera
        SIGNAL m_ok_read_cam1
;       +-- ciclo solo taglio
        CALL ciclosolotaglio
      ELSE
;
        IF NOT b_cam1_on OR NOT b_cam1_ready OR NOT SIG(y_tcp_sv_ok) THEN
          DREG (7) = 40
        END
      END
;                      +-------- reset taglio manuale
;                      |    +--- attesa taglio manuale resettato
      CALL res_mantag("ON","ON")
;                    +---------- reset fine ciclo
;                    |    +----- attesa fine ciclo resettato
      CALL res_fine("ON","OFF")
    END
;
;
;   +-- se richiesta scarico pressa in manuale
    IF SIG(x_manuale,x_man_scar) THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      IF NOT SIG(x_alm_plc) THEN
;       +-- reset pezzi da scartare ad ogni avvio ciclo
        nscarti_avv = 0
;       +-- set uscite ad inizio ciclo
        CALL set_out
;       +-- scarico pressa
        CALL scarico_manuale
      END
;                       +-------- reset scarico manuale
;                       |    +--- attesa scarico manuale resettato
      CALL res_manscar("ON","ON")
    END
;
;
;   +-- se richiesta cambio tipo
    IF SIG(x_manuale,x_cambio) THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      b_ciclosoft = TRUE
      IF NOT SIG(x_alm_plc) THEN
;       +-- set uscite ad inizio ciclo
        CALL set_out
        CALL cambiotipo
      END
;                      +-------- reset cambio tipo
;                      |    +--- attesa cambio tipo resettato
      CALL res_cambio("ON","ON")
    END
;
;
;   +-- bTaglio determina la scelta dei due cicli di funzionamento, puo' essere settato solo in manuale
    IF (b_btaglio1 OR b_btaglio2 OR b_btaglio3 OR b_btaglio4) AND (SIG(x_auto) OR SIG(x_manuale,x_step)) THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      IF NOT SIG(x_alm_plc) AND b_cam1_on AND b_cam1_ready AND SIG(y_tcp_sv_ok) THEN
;       +-- set uscite ad inizio ciclo
        CALL set_out
;       +-- reset ultima pos. di presa su griglia
        old_gruppo = 0
;       +-- set prima foto per prendere in carico lo stato delle posizioni
        b_prima_foto = TRUE
;       +-- reset pezzi da scartare ad ogni avvio ciclo
        nscarti_avv = 0
;       +-- set dati acquisiti a telecamera 1
        SIGNAL m_ok_read_cam1
;       +-- al primo ciclo la velocita e' 25% della max
        b_ciclosoft = TRUE
;       +-- ciclo con taglio
        CALL ciclotagliosi
      ELSE
;
        IF b_cam1_on OR b_cam1_ready OR NOT SIG(y_tcp_sv_ok) THEN
          DREG (7) = 40
        END
;                      +--------- reset fine ciclo
;                      |    +---- attesa fine ciclo resettato
        CALL res_fine("ON","OFF")
;                      +--------- reset ciclo automatico
;                      |    +---- attesa ciclo automatico resettato
        CALL res_auto("ON","ON")
;                       +-------- reset avvio ciclo
;                       |    +--- attesa start ciclo resettato
        CALL res_start("ON","ON")
;                      +--------- set ciclo manuale
;                      |    +---- attesa ciclo manuale attivo
        CALL set_manu("ON","ON")
      END
    END
;
;
;   +-- ciclo senza taglio
    IF NOT b_btaglio1 AND NOT b_btaglio2 AND NOT b_btaglio3 AND NOT b_btaglio4 AND (SIG(x_auto) OR SIG(x_manuale,x_step)) THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      IF NOT SIG(x_alm_plc) THEN
;       +-- reset ultima pos. di presa su griglia
        old_gruppo = 0
;       +-- set prima foto per prendere in carico lo stato delle posizioni
        b_prima_foto = TRUE
;       +-- reset pezzi da scartare ad ogni avvio ciclo
        nscarti_avv = 0
;       +-- set dati acquisiti a telecamera 1
        SIGNAL m_ok_read_cam1
;       +-- al primo ciclo la velocita e' 25% della max
        b_ciclosoft = TRUE
;       +-- ciclo senza taglio
        CALL ciclotagliono
      ELSE
;
;                      +--------- reset fine ciclo
;                      |    +---- attesa fine ciclo resettato
        CALL res_fine("ON","OFF")
;                      +--------- reset ciclo automatico
;                      |    +---- attesa ciclo automatico resettato
        CALL res_auto("ON","ON")
;                       +-------- reset avvio ciclo
;                       |    +--- attesa start ciclo resettato
        CALL res_start("ON","ON")
;                      +--------- set ciclo manuale
;                      |    +---- attesa ciclo manuale attivo
        CALL set_manu("ON","ON")
      END
    END
;
;
;   +-- se richiesta ciclo di test telecamere 3d e ptfe
    IF SIG(x_manuale,x_test_cam) THEN
      r_speed_auto = MINVAL(40,r_speed_02)
      IF NOT SIG(x_alm_plc) THEN
;       +-- reset pezzi da scartare ad ogni avvio ciclo
        nscarti_avv = 0
;       +-- set uscite ad inizio ciclo
        CALL set_out
;       +-- ciclo di test telecamere
        CALL cam_test
      END
;                       +-------- reset ciclo test
;                       |    +--- attesa ciclo test resettato
      CALL res_test("ON","ON")
    END
  END
;
  RETURN
.END
.PROGRAM prosegui()
;Ferma il ciclo dopo un anomalia
;
  WAIT SIG(x_alm_reset)
;
  RETURN
.END
.PROGRAM res_auto(.$comando,.$controllo)
;Richiesta reset ciclo automatico
;
; +-- reset ciclo automatico
  IF .$comando=="ON" THEN
    PULSE y_res_ciclo_aut,1
  END
;
; +-- attesa ciclo automatico resettato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_auto) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 58
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_ciclo_aut,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM res_cambio(.$comando,.$controllo)
;Reset cambio tipo
;
; +-- reset richiesta cambio tipo
  IF .$comando=="ON" THEN
    PULSE y_res_cambio,1
  END
;
; +-- attesa cambio tipo resettato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_cambio) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 260
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_cambio,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM res_fine(.$comm,.$wait)
;Reset fine ciclo
;
; +-- reset fine ciclo
  IF .$comm=="ON" THEN
    PULSE y_res_fine,1
  END
;
; +-- attesa fine ciclo resettato
  IF .$wait=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_fine) DO
      IF UTIMER(.@r_tim_wait)>60 THEN
        DREG (7) = 60
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_fine,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM res_manscar(.$comando,.$controllo)
;Reset scarico manuale
;
; +-- reset ciclo scarico manuale
  IF .$comando=="ON" THEN
    PULSE y_res_man_scar,1
  END
;
; +-- attesa scarico manuale resettato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_man_scar) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 262
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_man_scar,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM res_mantag(.$comando,.$controllo)
;Reset taglio manuale
;
; +-- reset ciclo taglio manuale
  IF .$comando=="ON" THEN
    PULSE y_res_ciclo_tag,1
  END
;
; +-- attesa taglio manuale resettato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_man_tag) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 261
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_ciclo_tag,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM res_start(.$comando,.$controllo)
;Reset avvio ciclo
;
; +-- reset avvio ciclo
  IF .$comando=="ON" THEN
    PULSE y_res_start,1
  END
;
; +-- attesa avvio ciclo resettato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_start) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 64
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_start,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM res_test(.$comando,.$controllo)
;Reset ciclo test telecamere 3d e ptfe
;
; +-- reset ciclo teset
  IF .$comando=="ON" THEN
    PULSE y_res_test_cam,1
  END
;
; +-- attesa ciclo test resettato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_test_cam) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 263
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_test_cam,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM reset_coll()
;Reset collaudo anello
;
; +-- se richiesta collaudo anello attiva
  IF SIG(x_coll_pezz) THEN
;   +-- reset collaudo anello
    PULSE y_res_ciclo_col,1
;   +-- attesa collaudo anello resettato
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_coll_pezz) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 66
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_ciclo_col,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM reset_step()
;Reset ciclo step
;
; +-- se step attivo
  IF SIG(x_step) THEN
;   +-- reset step
    PULSE y_res_step,1
;   +-- attesa step resettato
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_step) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 62
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_res_step,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM ring_receiver(.gruppo)
;Ricezione anelli a RS05
;
  BREAK
; +-- reset anello presente a RS5
  SIGNAL -y_ane_pres
; +-- set stato pinza gommato (PEZZO IN PINZA)
  rob_stato_pin[2] = 1
;
; +-- attesa richiesta chiusura pinza da RS5
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(x_rs5_ask_pre) DO
    PULSE y_r1_direct,0.5
    IF UTIMER(.@r_tim_wait)>5 THEN
      UTIMER .@r_tim_wait = 0
    END
    TWAIT 0.05
  END
;
; +-- reset RS20 pronto al trasferimento anelli
  SIGNAL -y_r1_direct
;
; +-- comando chiusura pinza gommato
  CALL pinzascachiude("BREAK","ON","ON",0)
; +-- set anello prelevato da pinza gommato
  PULSE y_ane_pre,1
; +-- attesa reset richiesta chiusura pinza da RS5
  UTIMER .@r_tim_wait = 0
  WHILE SIG(x_rs5_ask_pre) DO
    IF UTIMER(.@r_tim_wait)>5 THEN
      UTIMER .@r_tim_wait = 0
      PULSE y_ane_pre,1
    END
    TWAIT 0.05
  END
;
; +-- Memorizza stato del pezzo appena scaricato
  IF SIG(x_cam_3d_scarto) THEN
    b_scarto_3d = TRUE
  ELSE
    b_scarto_3d = FALSE
  END
;
  RETURN
.END
.PROGRAM ring_transfer(.gruppo)
;Trasferimento anelli a RS05
;
  BREAK
; +-- set anello presente a RS5
  SIGNAL y_ane_pres
; +-- set stato pinza gommato (VUOTA)
  rob_stato_pin[2] = 0
;
; +-- attesa richiesta apertura pinza da RS5
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(x_rs5_ask_ril) DO
    PULSE y_r1_direct,0.5
    IF UTIMER(.@r_tim_wait)>5 THEN
      UTIMER .@r_tim_wait = 0
    END
    TWAIT 0.05
  END
;
; +-- reset RS20 pronto al trasferimento anelli
  SIGNAL -y_r1_direct
;
; +-- comando apertura pinza gommato
  CALL pinzascaapre("BREAK","ON","ON",0)
; +-- set anello rilasciato da pinza gommato
  PULSE y_ane_ril,1
; +-- attesa reset richiesta apertura pinza da RS5
  UTIMER .@r_tim_wait = 0
  WHILE SIG(x_rs5_ask_ril) DO
    IF UTIMER(.@r_tim_wait)>5 THEN
      UTIMER .@r_tim_wait = 0
      PULSE y_ane_ril,1
    END
    TWAIT 0.05
  END
  b_scarto_pinclb = b_scarto_pincla
; +-- set stato gruppo appena depositato
  IF gruppo_scarto[.gruppo] THEN
    b_scarto_pincla = TRUE
  ELSE
;
    b_scarto_pincla = FALSE
  END
;
  RETURN
.END
.PROGRAM rs5_transfer(.gruppo)
;Trasferimento anelli a rs5
;
; +-- porta il robot in posizione (PIANO INCLINATO PER PASSAGGIO ANELLI A RS05)
  i_muove_verso = 25
  CALL muovi_r
;
; +-- in modo manuale attesa freccia sinistra per modo step
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- resetta eventuali scarti precedenti da visione
    b_scarto_3d = FALSE
    b_scarto_cam_p = FALSE
;   +-- reset step
    CALL reset_step
  ELSE
;
;   +-- trasferimento anello a RS5
    CALL ring_transfer(.gruppo)
  END
;
; +-- se anello da prelevare da RS5
  IF SIG(x_rs5_okpre) OR (SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar)) THEN
;   +-- esegui il trasferimento solo se non sono in modo step
    IF NOT (SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar)) THEN
;     +-- ricezione anello da RS5
      CALL ring_receiver(.gruppo)
    END
;   +-- gruppo buono o gruppo scarto e lavorazione scarti attiva
    IF NOT b_scarto_pinclb OR (b_lavora_sca AND b_scarto_pinclb) THEN
;     +-- se controllo telecamera incluso
      IF b_inc_cam_p AND (NOT b_scarto_3d OR NOT b_inc_cam_3d) THEN
;       +-- controllo telecamera ptfe
        CALL camera_ptfe
      END
;     +-- controllo mandrel pronto e non scarto da camera ptfe o 3d
      IF NOT SIG(x_mandrel_nok) THEN
        IF (NOT b_scarto_cam_p OR NOT b_inc_cam_p) AND (NOT b_scarto_3d OR NOT b_inc_cam_3d) THEN
          IF b_inc_mandrel THEN
;           +-- montaggio mandrel
            CALL mandrel
          END
        END
      END
      IF (b_scarto_cam_p AND b_inc_cam_p) THEN
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
      ELSE
        IF (b_scarto_3d AND b_inc_cam_3d) THEN
;         +-- deposito scarto
          CALL depo_scarto("GOMMATO")
        ELSE
;         +-- deposito su nastro
          CALL deposito
        END
      END
    ELSE
;
;     +-- deposito scarto
      CALL depo_scarto("GOMMATO")
    END
;   +-- reset eventuale pezzo scarto
    b_scarto_pinclb = FALSE
  END
;
  RETURN
.END
.PROGRAM scarico_manuale()
;Scarico pressa automatico in manuale
;
; +-- se sistema di visione attivo
  IF b_cam1_on THEN
;   +-- set acquisizione immagine al prossimo ingresso in pressa
    b_cam1_acq = TRUE
;   +-- set apertura sportello telecamera/accensione luci
    CALL tele_on
  END
; +-- set ciclo soft
  b_ciclosoft = TRUE
; +-- porta il robot in posizione (SCARICO PRESSA)
  i_muove_verso = 3
  CALL muovi_r
; +-- scarico pressa
  CALL pressascarico
  i_muove_verso = 3
  CALL muovi_r
  LMOVE #fi_pressa
; +-- reset ciclo soft
  b_ciclosoft = FALSE
; +-- se sistema di visione attivo
  IF b_cam1_on THEN
;   +-- aggiorna le pos. sulla griglia di raff.
    CALL initpunti_cam
;   +-- se ho superato il numero di max foto
    IF b_nfoto_max THEN
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- deposito scarto
      CALL depo_scarto("GOMMATO")
;     +-- deposito scarto cappuccio		
      CALL depo_scarto("CAPPUCCIO")
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
      RETURN
    END
nuovo_deposito:
;   +-- se telecamera pronta
    IF b_cam1_ready THEN
;     +-- reset gruppo
      .gruppo = 0
;     +-- cerca un gruppo libero
      FOR .i = 1 TO i_npos_vis
        IF vis_stato_pos[.i]==0 THEN
;      	  +-- set gruppo trovato
          .gruppo = .i
;         +-- set gruppo pezzi scarto
          gruppo_scarto[.gruppo] = TRUE
;         +-- deposito su raff.
          CALL depo_raff(.gruppo)
;         +-- deposito scarto cappuccio		
          CALL depo_scarto("CAPPUCCIO")
          .i = 10000
        END
      END
;     +-- se non ha trovato nessun gruppo dove depositare
      IF .gruppo==0 THEN
;       +-- deposito scarto
        CALL depo_scarto("GOMMATO")
;       +-- deposito scarto cappuccio		
        CALL depo_scarto("CAPPUCCIO")
      END
    ELSE
;
      DREG (7) = 40
;     +-- deposito scarto
      CALL depo_scarto("GOMMATO")
;     +-- deposito scarto cappuccio		
      CALL depo_scarto("CAPPUCCIO")
    END
  ELSE
;
;   +-- deposito scarto
    CALL depo_scarto("GOMMATO")
;   +-- deposito scarto cappuccio		
    CALL depo_scarto("CAPPUCCIO")
  END
; +-- set chiusura sportello telecamera/spegnimento luci
  CALL tele_off
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
; +-- comando apertura pinza lamiera
  CALL pinzacarapre("BREAK","ON","ON",0)
; +-- comando apertura pinza gommato
  CALL pinzascaapre("BREAK","ON","ON",0)
;
  RETURN
.END
.PROGRAM set_fine(.$comando,.$controllo)
;Richiesta fine ciclo
;
; +-- set fine ciclo
  IF .$comando=="ON" THEN
    PULSE y_rob_fine,1
  END
;
; +-- attesa fine ciclo selezionato
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_fine) DO
      IF UTIMER(.@r_tim_wait)>3 THEN
        DREG (4) = 61
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_rob_fine,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
