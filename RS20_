.PROGRAM set_manu(.$comando,.$controllo)
;Richiesta ciclo manuale
;
; +-- set fine ciclo
  IF .$comando=="ON" THEN
    PULSE y_rob_manuale,1
  END
;
; +-- attesa ciclo manuale attivo
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_manuale) DO
      IF UTIMER(.@r_tim_wait)>3 THEN
        DREG (7) = 57
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        PULSE y_rob_manuale,1
      END
      TWAIT 0.2
    END
  END
;
  RETURN
.END
.PROGRAM set_out()
;Set uscite ad inizio ciclo
;
  SIGNAL -y_arb_ok_ch,-y_arb_st_free,-y_arb_est_ind,-y_arb_est_ava
  SIGNAL -y_nsca_start,-y_abilita_esa,-y_rs5_okpre
  SIGNAL y_rs5_fi,-y_luce_tele_on,-y_sp_tele_ap,-y_sp_tele_ch,y_mand_fi,y_fi_cass_ptfe
  SIGNAL m_ok_read_cam1
; +-- discesa sollevatore
  CALL soll_dw("BREAK","ON","ON",0)
; +-- discesa pinza pinza codolo
  CALL pinzacodgiu("BREAK","ON","ON",0)
  PULSE y_sp_tele_ch,1
  PULSE y_res_fine,1
  SIGNAL y_pos_mand
;
  RETURN
.END
.PROGRAM set_tool(.$tool_id)
;Set tool
;
  IF .$tool_id=="tpcar" THEN
    POINT .tool_req = tpcar
  ELSE
;
    IF .$tool_id=="tpscar" THEN
      POINT .tool_req = tpscar
    ELSE
;
      IF .$tool_id=="tventose" THEN
        POINT .tool_req = tpscar
      ELSE
;
        POINT .tool_req = NULL
      END
    END
  END
;
  POINT .tool_act = TOOL
  IF DISTANCE(.tool_act,.tool_req)>1 THEN
    TOOL .tool_req
  END
;
  RETURN
.END
.PROGRAM set_var()
;Definizione variabili
;
; +-- variabili
  i_arburg_recv = 0                                        ;trasmissione con arburg disattiva
  nscarti_avv = 0                                          ;reset pezzi da scartare ad ogni avvio ciclo
  b_lam_prel = FALSE                                       ;reset lamiera prelevata
  b_speed_fp02 = FALSE                                     ;reset speed_fp per griglia piena
  b_cam1_acq = FALSE                                       ;reset acuisizione immagine nel pg successivo
  old_gruppo = 0                                           ;reset ultima pos. di presa su griglia
  b_p_teach = 0                                            ;abilita/disabilita pagina teach
  b_conf = FALSE                                           ;reset confema valori posizione
  r_speed_auto = r_speed_02                                ;set speed calcolata
  r_sp_praff = 250                                         ;MM/S presa anelli su griglia
  r_sp_pincl = 250                                         ;MM/S presa anelli su piano inclinato
  r_sp_soll = 250                                          ;MM/S presa lamiere da sollevatore
  b_ciclosoft = TRUE                                       ;monitor spped al 20% al primo ciclo
  bciclofinantic = FALSE                                   ;reset fine ciclo
  i_allarme = 0                                            ;reset var. allarme
  nstepoffset = 0.1                                        ;(mm) off.set movimenti robot in teach
  b_prima_foto = TRUE                                      ;set prima foto per prendere in carico lo stato delle posizioni
  b_scarto_pincla = FALSE
  b_scarto_pinclb = FALSE
;
; +-- iniziallizzo i timer pressa
  i_time_robot = 0
  i_time_frq_sca = 0
  UTIMER @r_time_robot = 0
  UTIMER @r_time_frq_sca = 0
;
  RETURN
;
  HALT
  HALT
;
  CALL parametri
.END
.PROGRAM soll_dw(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando sollevatore basso
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_soll_up,-y_soll_dw
    PULSE y_soll_dw,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_soll_dw,-x_soll_up) DO
      IF UTIMER(.@r_tim_wait)>10 THEN
        DREG (7) = 129
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        SIGNAL -y_soll_up,-y_soll_dw
        PULSE y_soll_dw,1
      END
      TWAIT 0.05
    END
  END
;
  IF .r_tim_attesa>0 THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM soll_up(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando sollevatore alto
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_soll_dw,-y_soll_up
    PULSE y_soll_up,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_soll_up,-x_soll_dw) DO
      IF UTIMER(.@r_tim_wait)>10 THEN
        DREG (7) = 126
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        SIGNAL -y_soll_dw,-y_soll_up
        PULSE y_soll_up,1
      END
      TWAIT 0.05
    END
  END
;
  IF .r_tim_attesa>0 THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM start_cam_ptfe()
;Start a telecamera controllo ptfe (sopra mandrel)
;
  BREAK
; +-- reset acquisizione a telecamera
  WHILE b_cam_p_buono OR b_cam_p_scarto DO
    PULSE m_cam_p_res,1
    TWAIT 0.02
  END
  SIGNAL -m_cam_p_res
;
; +-- set pezzo scarto prima di iniziare
  b_scarto_cam_p = TRUE
;
; +-- se telecamera pronta
  IF b_cam_p_ready THEN
;   +-- richiesta acquisizione immagine
    PULSE y_cam_p_acq,1
;   +-- timer attesa dati pronti da telecamera
    UTIMER .@r_tim_wait = 0
;
;   +-- attesa pezzo buono/scarto o timeout
    WAIT b_cam_p_buono OR b_cam_p_scarto OR UTIMER(.@r_tim_wait)>20
;   +-- se sono in timeout
    IF UTIMER(.@r_tim_wait)>20 THEN
;     +-- set pezzo scarto
      b_scarto_cam_p = TRUE
;     +-- reset acquisizione a telecamera
      PULSE m_cam_p_res,2
;     +-- segnalazione a pannello
      DREG (4) = 33
      RETURN
    END
;
;   +-- Verifica risultato buono o scarto
    IF b_cam_p_buono THEN
      b_scarto_cam_p = FALSE
    ELSE
      b_scarto_cam_p = TRUE
    END
    PULSE m_cam_p_res,2
  ELSE
;   +-- segnalazione a pannello
    DREG (4) = 32
  END
;	
  RETURN
.END
.PROGRAM stato_raff_rob()
;Verifica stato teorico griglia se visione attiva
;
; +-- se visione attiva
  IF b_cam1_on THEN
    ngruppi = 0
    ngruppi_vis = 0
    FOR .i = 1 TO i_npos_vis
      ngruppi_vis = ngruppi_vis+1
      IF rob_stato_pos[.i]==1 THEN
        ngruppi = ngruppi+1
      END
    END
  END
;
  RETURN
.END
.PROGRAM stato_raff_vis()
;Verifica stato reale griglia
;
; +-- se visione attiva
  IF b_cam1_on THEN
    ngruppi = 0
    ngruppi_vis = 0
    FOR .i = 1 TO i_npos_vis
      ngruppi_vis = ngruppi_vis+1
      IF vis_stato_pos[.i]==1 THEN
        ngruppi = ngruppi+1
      END
    END
  END
;
  RETURN
.END
.PROGRAM tele_off()
;Chiusura sportello telecamera
;
; +-- set apertura sportello telecamera
  SIGNAL -y_sp_tele_ap,-y_sp_tele_ch
  PULSE y_sp_tele_ch,1
;
  RETURN
.END
.PROGRAM tele_on()
;Apertura sportello telecamera e accensione luci
;
; +-- set apertura sportello telecamera
  SIGNAL -y_sp_tele_ap,-y_sp_tele_ch
  PULSE y_sp_tele_ap,1
; +-- set luce telecamera ON
  PULSE y_luce_tele_on,1
;
  RETURN
.END
.PROGRAM vuoto_off(.$break,.$comando,.$controllo,.r_tim_attesa)
;Reset comando vuoto presa ptfe
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_soffio_ptfe,-y_vuoto_ptfe
    PULSE y_soffio_ptfe,2
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_vuoto_ptfe) DO
      IF UTIMER(.@r_tim_wait)>10 THEN
        DREG (7) = 70
        CALL prosegui
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.02
    END
  END
;
  IF .r_tim_attesa THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM vuoto_on(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando vuoto presa ptfe
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_soffio_ptfe,-y_vuoto_ptfe
    PULSE y_vuoto_ptfe,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_vuoto_ptfe) DO
      IF UTIMER(.@r_tim_wait)>10 THEN
        DREG (7) = 31
        CALL prosegui
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.02
    END
  END
;
  IF .r_tim_attesa THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM z_ricerca(.#target,.input,.sp_ric)
;Movimento verso la posizione .#TARGET finche' l'ingresso e' attivo/disattivo
;
; +-- controllo sensore prima di iniziare la ricerca
  WHILE SIG(.input) DO
    DREG (7) = 272
    CALL prosegui
  END
;
  ABS.SPEED ON
  POINT .target = .#target
  SPEED .sp_ric MM/S ALWAYS
  ACCURACY 0.1 ALWAYS
;
  LMOVE .#target
  DO
    IF DISTANCE(.target,HERE)<1 THEN
      ABS.SPEED OFF
      RETURN
    END
  UNTIL NOT SIG(-.input)
  BRAKE
  ABS.SPEED OFF
;
  RETURN
.END
.PROGRAM zzz_set_up()
;Set.up posizioni macchina
;
;STEP N.21   CALCOLO TOOL
;STEP N.47   PRELIEVO PTFE
;STEP N.66   SOLLEVATORE
;STEP N.94   SCARICO PRESSA
;STEP N.107  CARICO PRESSA
;STEP N.121  GRIGLIA DI RAFF.
;STEP N.137  DEPOSITO SU GRIGLIA DI RAFF.
;STEP N.192  PRELIEVO DA GRIGLIA DI RAFF.
;STEP N.223  MONTAGGIO MANDREL
;STEP N.235  PASSAGGIO ANELLI A RS5
;STEP N.242  TELECAMERA CONTROLLO PTFE
;STEP N.
;STEP N.
;STEP N.
;STEP N.
;STEP N.
;
  PAUSE
; +-- Posizioni per AUTOTOOL
  LMOVE #auto_1;avvicinamento Posture A1
  PAUSE
  LMOVE #auto_1;allontanamento Posture A1  
;
  LMOVE #auto_2;avvicinamento   Posture A2
  PAUSE
  LMOVE #auto_2;allontanamento Posture A2 
;  
  LMOVE #auto_3;avvicinamento   Posture A3
  PAUSE
  LMOVE #auto_3;allontanamento Posture A3 
;  
  LMOVE #auto_4;avvicinamento   Posture A4
  PAUSE
  LMOVE #auto_4;allontanamento Posture A4
  PAUSE
;
; +-- calcolo tool
  POINT tpcar = RZ(45)+TRANS(,,159)+RY(90)+TRANS(,,75)
  POINT tpscar = RZ(45)+TRANS(,,399)+RY(90)+TRANS(,,100)
  POINT tventose = RZ(45)+TRANS(,,171.5)+RY(90)+TRANS(,,75)
  POINT tcoltello = tcoltello0+RZ(45)+RX(90)
  PAUSE
;
;
; +-- presa ptfe
  CALL muove_da1_a28
  TOOL tventose
  JMOVE #avv_ptfe1_01
  LMOVE #avv_ptfe1_02
  LMOVE #min_ptfe1
  LMOVE #all_ptfe1_01
  JMOVE #fi_cass_ptfe
;
  JMOVE #fi_cass_ptfe
  JMOVE #avv_ptfe2_01
  LMOVE #avv_ptfe2_02
  LMOVE #min_ptfe2
  LMOVE #all_ptfe2_01
  JMOVE #fi_cass_ptfe
  CALL muove_da28_a1
  PAUSE
;
;
; +-- frame sollevatore
  CALL muove_da1_a21
  TOOL tcoltello
  JAPPRO x1_soll,100
  LMOVE x1_soll
  LDEPART 100
  JAPPRO x2_soll,100
  LMOVE x2_soll
  LDEPART 100
  JAPPRO y1_soll,100
  LMOVE y1_soll
  LDEPART 100
  POINT f_soll = FRAME(x1_soll,x2_soll,y1_soll,x1_soll)
  PAUSE
; +-- posizione 1 presa sollevatore
  TOOL tpcar
  LAPPRO f_soll+org_soll1,50
  LMOVE f_soll+org_soll1
  LDEPART 50
  PAUSE
; +-- posizione 1 presa sollevatore
  LAPPRO f_soll+org_soll2,50
  LMOVE f_soll+org_soll2
  LDEPART 50
  CALL muove_da21_a1
  PAUSE
;
;
; +-- scarico pressa
  CALL muove_da1_a3
  TOOL tpscar
  LMOVE #avv_prpresa_01
  LMOVE #avv_prpresa_02
  LMOVE org_prespresa
  CALL initpunti
  LMOVE #all_prpresa_01
  LMOVE #all_prpresa_02
  CALL muove_da3_a1
  PAUSE
;
;
; +-- carico pressa
  CALL muove_da1_a3
  TOOL tpcar
  JMOVE #avv_prdep_01
  LMOVE #avv_prdep_02
  LMOVE org_presdep
  CALL initpunti
  LMOVE #all_prdep_01
  LMOVE #all_prdep_02
  JMOVE #fi_pressa
  CALL muove_da3_a1
  PAUSE
;
;
; +-- frame griglia di raff.
  CALL muove_da1_a16
  TOOL tcoltello
  JAPPRO x1_raff,100
  LMOVE x1_raff
  LDEPART 100
  JAPPRO x2_raff,100
  LMOVE x2_raff
  LDEPART 100
  JAPPRO y1_raff,100
  LMOVE y1_raff
  LDEPART 100
  POINT f_raff = FRAME(x1_raff,x2_raff,y1_raff,x1_raff)
  PAUSE
;
;
; +-- deposito griglia di raff.
  TOOL tpscar
  POINT .#rif = x1_raff
  POINT f_raff+.rif = .#rif
  DECOMPOSE .a[1] = .rif
  POINT .new_rif = TRANS(.a[1],.a[2],.a[3]+10,-90,180,0)
  POINT #drif[1] = f_raff+.new_rif,#fi_dzona1_raff
  POINT .new_rif = TRANS(.a[1]+400,.a[2],.a[3]+10,-90,180,0)
  POINT #drif[2] = f_raff+.new_rif,#fi_dzona1_raff
  POINT .new_rif = TRANS(.a[1],.a[2]+340,.a[3]+10,90,180,0)
  POINT #drif[3] = f_raff+.new_rif,#fi_dzona2_raff
  POINT .new_rif = TRANS(.a[1]+400,.a[2]+340,.a[3]+10,90,180,0)
  POINT #drif[4] = f_raff+.new_rif,#fi_dzona2_raff
  POINT .new_rif = TRANS(.a[1],.a[2]+680,.a[3]+10,90,180,0)
  POINT #drif[5] = f_raff+.new_rif,#fi_dzona2_raff
  POINT .new_rif = TRANS(.a[1]+400,.a[2]+680,.a[3]+10,90,180,0)
  POINT #drif[6] = f_raff+.new_rif,#fi_dzona2_raff
  POINT .new_rif = TRANS(.a[1]+200,.a[2]+163,.a[3]+10,-90,180,0)
  POINT #drif[7] = f_raff+.new_rif,#fi_dzona1_raff
  POINT .new_rif = TRANS(.a[1]+200,.a[2]+517,.a[3]+10,90,180,0)
  POINT #drif[8] = f_raff+.new_rif,#fi_dzona2_raff
;
  PAUSE
; +-- verifica posizioni calcolate deposito
  JMOVE #fi_dzona1_raff
  JAPPRO #drif[1],100
  LMOVE #drif[1]
  LDEPART 100
  JAPPRO #drif[2],100
  LMOVE #drif[2]
  LDEPART 100
  JAPPRO #drif[7],100
  LMOVE #drif[7]
  LDEPART 100
  JMOVE #fi_dzona1_raff
  JMOVE #fi_dzona2_raff
  JAPPRO #drif[3],100
  LMOVE #drif[3]
  LDEPART 100
  JAPPRO #drif[4],100
  LMOVE #drif[4]
  LDEPART 100
  JAPPRO #drif[5],100
  LMOVE #drif[5]
  LDEPART 100
  JAPPRO #drif[8],100
  LMOVE #drif[8]
  LDEPART 100
  JAPPRO #drif[6],100
  LMOVE #drif[6]
  LDEPART 100
  JMOVE #fi_dzona2_raff
  PAUSE
;
;
; +-- prelievo griglia di raff.
;
; Attenzione!!! le origini sono calcolate posizionanto da pinza postetiore con il
; centro della grioglia, in realt poi il calcolo e fatto con il tool della pinza anteriore
; questo perch tale posizione e irraggiungibile con la pinza anteriore
;
  CALL muove_da17_a18
  JAPPRO #org_prif0a,100
  LMOVE #org_prif0a
  LDEPART 100
  JAPPRO #org_prif0b,100
  LMOVE #org_prif0b
  LDEPART 100
  JAPPRO #org_prif0c,100
  LMOVE #org_prif0c
  LDEPART 100
  CALL muove_da18_a19
  JAPPRO #org_prif180a,100
  LMOVE #org_prif180a
  LDEPART 100
  JAPPRO #org_prif180b,100
  LMOVE #org_prif180b
  LDEPART 100
  JAPPRO #org_prif180c,100
  LMOVE #org_prif180c
  LDEPART 100
  JMOVE #fi_pzona2_raff
  CALL muove_da19_a1
  PAUSE
;
;
; +-- montaggio mandrel
  JMOVE #fi_mandrel
  TOOL tpscar
  CALL pinzacodsu("BREAK","ON","",0)
  LMOVE #pmandrel_01
  LMOVE #pmandrel
  LMOVE #pmandrel_01
  LMOVE #fi_mandrel
  CALL muove_da22_a1
  PAUSE
;
;
; +-- passaggio anelli a rs5
  CALL muove_da1_a25
  JMOVE #pres_rs5
  CALL muove_da25_a1
  PAUSE
;
;
; +-- telecamera controllo PTFE
  CALL muove_da1_a11
  TOOL tpscar
  JMOVE #fi_cam_ptfe
  LMOVE #cam_ptfe_01
  PAUSE
;use JAPPRO only without backlight
  JAPPRO #cam_ptfe,30
  LMOVE #cam_ptfe
  PAUSE
;use JAPPRO only without backlight
  JAPPRO #cam_ptfe,30
  LMOVE #cam_ptfe_01
  JMOVE #fi_cam_ptfe
  CALL muove_da11_a1
  PAUSE
;
.END
