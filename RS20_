      COLRON
      COLRJON
      CALL p_lmove(#fi_scarto_gom,x_home)
      CALL p_jmove(#m24_1_01,x_home)
;
     VALUE 47:
      CALL p_lmove(#scarto_lam,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_scarto_lam,x_home)
      CALL p_jmove(#m23_1_01,x_home)
;
     VALUE 48:
      CALL p_lmove(#cam_ptfe,x_home)
      COLRON
      COLRJON
      CALL p_lmove(#cam_ptfe_01,x_home)
      CALL p_lmove(#fi_cam_ptfe,x_home)
      CALL p_jmove(#m11_1_01,x_home)
      CALL p_jmove(#m11_1_02,x_home)
      CALL p_jmove(#m11_1_03,x_home)
;
     VALUE 49:
      CALL p_lmove(#cam_ptfe_01,x_home)
      COLRON
      COLRJON
      CALL p_lmove(#fi_cam_ptfe,x_home)
      CALL p_jmove(#m11_1_01,x_home)
      CALL p_jmove(#m11_1_02,x_home)
      CALL p_jmove(#m11_1_03,x_home)
;
     VALUE 50:
      CALL p_lmove(#fi_cam_ptfe,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#m11_1_01,x_home)
      CALL p_jmove(#m11_1_02,x_home)
      CALL p_jmove(#m11_1_03,x_home)
;
     VALUE 51:
      CALL p_lmove(#m11_1_02,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#m11_1_03,x_home)
;
     VALUE 52:
      CALL p_lmove(#m11_4_03,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#m11_4_04,x_home)
      CALL p_jmove(#fi_deposito,x_home)
      CALL p_jmove(#m1_4_02,x_home)
      CALL p_lmove(#m1_4_01,x_home)
;
     VALUE 53:
      CALL p_lmove(#m11_22_02,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#m11_22_03,x_home)
      CALL p_lmove(#fi_mandrel,x_home)
      CALL p_jmove(#m22_1_01,x_home)
      CALL p_jmove(#m22_1_02,x_home)
      CALL p_jmove(#m22_1_03,x_home)
;
     VALUE 54:
      COLRON
      COLRJON
      CALL p_jmove(#fi_scarto_lam,x_home)
      CALL p_jmove(#m23_1_01,x_home)
;
     VALUE 55:
      COLRON
      COLRJON
      CALL p_jmove(#m23_1_01,x_home)
;
     VALUE 56:
      CALL p_jmove(#pres_rs5,x_home)
      COLRON
      COLRJON
      CALL p_jmove(#m25_1_01,x_home)
;
     VALUE 58:
      COLRON
      COLRJON
      CALL p_jmove(#cambio01,x_home)
;
     VALUE 59:
      COLRON
      COLRJON
      CALL p_jmove(#fi_cambio,x_home)
      CALL p_jmove(#cambio01,x_home)
;
     VALUE 60:
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona1_raff,x_home)
;
     VALUE 61:
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona2_raff,x_home)
      CALL p_jmove(#m17_1_01,x_home)
      CALL p_jmove(#m17_1_02,x_home)
;
     VALUE 62:
      CALL p_jmove(#all_draff[1],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona1_raff,x_home)
;
     VALUE 63:
      CALL p_jmove(#all_draff[2],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona1_raff,x_home)
;
     VALUE 64:
      CALL p_jmove(#all_draff[3],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona2_raff,x_home)
      CALL p_jmove(#m17_1_01,x_home)
      CALL p_jmove(#m17_1_02,x_home)
;
     VALUE 65:
      CALL p_jmove(#all_draff[4],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona2_raff,x_home)
      CALL p_jmove(#m17_1_01,x_home)
      CALL p_jmove(#m17_1_02,x_home)
;
     VALUE 66:
      CALL p_jmove(#all_draff[5],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona2_raff,x_home)
      CALL p_jmove(#m17_1_01,x_home)
      CALL p_jmove(#m17_1_02,x_home)
;
     VALUE 67:
      CALL p_jmove(#all_draff[6],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona2_raff,x_home)
      CALL p_jmove(#m17_1_01,x_home)
      CALL p_jmove(#m17_1_02,x_home)
;
     VALUE 68:
      COLRON
      COLRJON
;     +-- attesa estrattore indietro
      .r_tim0 = TIMER(0)
      WHILE NOT SIG(x_arb_est_ind,-x_arb_est_ava) DO
        IF TIMER(0)>.r_tim0+10 THEN
          DREG (7) = 51
          CALL prosegui
          .r_tim0 = TIMER(0)
        END
        TWAIT 0.05
      END
      CALL p_jmove(#avv_prpresa_02,x_home)
      CALL p_lmove(#avv_prpresa_01,x_home)
      CALL p_jmove(#fi_pressa,x_home)
;
     VALUE 69:
      COLRON
      COLRJON
;     +-- attesa estrattore indietro
      .r_tim0 = TIMER(0)
      WHILE NOT SIG(x_arb_est_ind,-x_arb_est_ava) DO
        IF TIMER(0)>.r_tim0+10 THEN
          DREG (7) = 51
          CALL prosegui
          .r_tim0 = TIMER(0)
        END
        TWAIT 0.05
      END
      CALL p_lmove(#avv_prpresa_01,x_home)
      CALL p_jmove(#fi_pressa,x_home)
;
     VALUE 70:
      COLRON
      COLRJON
      CALL p_jmove(#fi_pressa,x_home)
;
     VALUE 72:
      CALL p_jmove(#all_draff[7],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona1_raff,x_home)
;
     VALUE 73:
      CALL p_jmove(#all_draff[8],x_home)
      COLRON
      COLRJON
      CALL p_jmove(#fi_dzona2_raff,x_home)
      CALL p_jmove(#m17_1_01,x_home)
      CALL p_jmove(#m17_1_02,x_home)
;
     ANY :
      DREG (7) = 16
      HALT
    END
    CALL p_jmove(#phome,x_home)
    i_muove_da = 1
  ELSE
;
    i_muove_da = 1
  END
; +-- piccolo movimento per capire che sono in Home
  SPEED 10
  DRAW 5,5,5
  JMOVE #phome
  BREAK
; +-- set uscite ad inizio ciclo
  CALL set_out
; +-- se non ho la pinza di scarico apereta
  IF SIG(x_psca_ch) OR NOT SIG(x_psca_ap) THEN
;
    CALL depo_scarto("GOMMATO")
  END
;
; +-- se non ho la pinza di carico aperta
  IF SIG(x_pcar_ch) OR NOT SIG(x_pcar_ap) THEN
;
    CALL depo_scarto("LAMIERA")
    b_lam_prel = FALSE
  END
;
; +-- se ho il ptfe in pinza
  IF SIG(x_vuoto_ptfe) THEN
;
    CALL depo_scarto("PTFE")
    b_ptfe_prel = FALSE
  END
;
; +-- se non ho la pinza cappuccio aperta
  IF SIG(x_pcod_ch) OR NOT SIG(x_pcod_ap) THEN
;   +-- deposito scarto cappuccio
    CALL depo_scarto("CAPPUCCIO")
  END
;
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
; +-- comando apertura pinze
  CALL pinzascaapre("BREAK","ON","ON",0)
  CALL pinzacarapre("BREAK","ON","ON",0)
  CALL vuoto_off("BREAK","ON","ON",0)
;	
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
;
  RETURN
.END
.PROGRAM nastropresa()
;Presa lamiera da nastro
;
; +-- set tool
  CALL set_tool("tpcar")
; +-- reset lamiera prelevata
  b_lam_prel = FALSE
; +-- se non siamo in manuale
  IF NOT SIG(x_manuale) THEN
;   +-- attesa anelli su sollevatore
    IF NOT SIG(x_soll_pz_pre) THEN
;     +-- richiesta anello a convogliatore
      PULSE y_ask_pz_soll,1
;     +-- attesa lamiere in posizione
      UTIMER .@r_tim_wait = 0
      WHILE NOT SIG(x_soll_pz_pre) DO
        IF UTIMER(.@r_tim_wait)>20 THEN
          DREG (7) = 73
;                        +-------- set fine ciclo
;                        |    +--- attesa fine ciclo selezionato
          CALL set_fine("ON","ON")
        END
;       +-- fine ciclo
        IF SIG(x_fine) THEN
          RETURN
        END
        TWAIT 0.05
      END
    END
;   +-- comando e controllo sollevatore alto
    IF NOT SIG(x_soll_up,-x_soll_dw) THEN
      CALL soll_up("NOBREAK","ON","OFF",0)
    END
  ELSE
;
    CALL soll_up("NOBREAK","ON","OFF",0)
  END
; +-- porta il robot in posizione (PRESA LAMIERE DA SOLLEVATORE)
  i_muove_verso = 21
  CALL muovi_r
; +-- set lamiera prelevata
  b_lam_prel = TRUE
; +-- reset F.I. a sollevatore
  SIGNAL -y_soll_fi
; +-- presenza lamiera 1
  IF SIG(x_soll_lam1) THEN
;   +-- presa lamiera 1
    CALL pre_soll_lam1
  ELSE
;
;   +-- presenza lamiera 2
    IF SIG(x_soll_lam2) THEN
;     +-- presa lamiera 2
      CALL pre_soll_lam2
    ELSE
;
;     +-- set ciclo soft
      b_ciclosoft = TRUE
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- comando apertura pinza gommato
      CALL pinzascaapre("BREAK","ON","ON",0)
      DREG (7) = 252
      HALT
    END
  END
;
; +-- chiusura sollevatore
  CALL soll_dw("BREAK","ON","OFF",0)
  JMOVE #fi_soll_lam
; +-- set F.I. a sollevatore
  SIGNAL y_soll_fi
  b_lamsoll = FALSE
  IF NOT SIG(x_manuale) AND NOT SIG(x_soll_lam1) AND NOT SIG(x_soll_lam2) THEN
;   +-- set anelli prelevati da sollevatore
    PULSE y_pz_pre_soll,1
;   +-- richiesta anelli a sollevatore per ciclo successivo
    PULSE y_ask_pz_soll,1
  END
;
  RETURN
.END
.PROGRAM origini()
;Definizione tool,base,frame
;
; +-- base
  BASE RX(90)+RZ(90)+RX(180)
;
; +-- tool
  POINT tpcar = RZ(45)+TRANS(,,159)+RY(90)+TRANS(,,75)
  POINT tpscar = RZ(45)+TRANS(,,399)+RY(90)+TRANS(,,100)
  POINT tventose = RZ(45)+TRANS(,,171.5)+RY(90)+TRANS(,,75)
  POINT tcoltello = tcoltello0+RZ(45)+RX(90)
;
; +-- frame sollevatore
  POINT f_soll = FRAME(x1_soll,x2_soll,y1_soll,x1_soll)
; +-- frame griglia raff.
  POINT f_raff = FRAME(x1_raff,x2_raff,y1_raff,x1_raff)
; +-- weight
  WEIGHT 20,0,0,150
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
  COLRON
  COLRJON
;
  RETURN
.END
.PROGRAM p_jmove(.#target,.input)
;Movimento verso la posizione .#TARGET finche' l'ingresso e' attivo
;
  BREAK
  POINT .target = .#target
  IF SWITCH(REPEAT ) THEN
;   +-- spadino presente in pinza
    IF utensile_at[2]==3 THEN
      SPEED 6 ALWAYS
      WAIT SIG(.input)
move_a: JMOVE .#target
      DO
        IF DISTANCE(.target,HERE)<1 THEN
          RETURN
        END
      UNTIL SIG(-.input)
;			
      BRAKE
;
      WAIT SIG(.input)
      GOTO move_a
    ELSE
;     +-- spadino non presente in pinza		
      SPEED 8 ALWAYS
move_b: JMOVE .#target
      IF DISTANCE(.target,HERE)<1 THEN
        RETURN
      END
      GOTO move_b
    END
  ELSE
    JMOVE .#target
  END
.END
.PROGRAM p_lmove(.#target,.input)
;Movimento verso la posizione .#TARGET finche' l'ingresso e' attivo
;
  BREAK
  POINT .target = .#target
  IF SWITCH(REPEAT ) THEN
;   +-- spadino presente in pinza
    IF utensile_at[2]==3 THEN
      SPEED 4 ALWAYS
      WAIT SIG(.input)
move_a: LMOVE .#target
      DO
        IF DISTANCE(.target,HERE)<1 THEN
          RETURN
        END
      UNTIL SIG(-.input)
;			
      BRAKE
;
      WAIT SIG(.input)
      GOTO move_a
    ELSE
;     +-- spadino non presente in pinza		
      SPEED 8 ALWAYS
move_b: LMOVE .#target
      IF DISTANCE(.target,HERE)<1 THEN
        RETURN
      END
      GOTO move_b
    END
  ELSE
    LMOVE .#target
  END
.END
.PROGRAM parametri()
;Parametri pezzo campione utilizzato per memo posizioni macchina
;
  ndiametron = 106.8       ;(mm) diametro pezzo campione
  naltezzan = 14.7         ;(mm) altezza lamiera campione
  naltezzang = 14.7        ;(mm) altezza anello gommato campione
  naltezzastampon = 252    ;(mm) altezza stampo inferiore
  naltpinzascan = 50       ;(mm) altezza pinza scarico
;   nDiametro Prelievo lamiera da nastro (120 gradi) (POS2)
;   nAltezza Prelievo e deposito lamiera in matrice (POS2, POS5, POS6, POS7)
;   nAltezzaStampo Prelievo e deposito lamiera e pezzo in matrice (POS3, POS4, POS5, POS6, POS7)
;   nAltPinzaSca Posa nastro su nastro (POS8)
;
  RETURN
.END
.PROGRAM pc1_chk_battery()
;Controllo errore batterie scariche
;
; +-- controllo batterie Encoder
  IF ERROR==-21013 THEN
    DREG (7) = 23
  END
; +-- controllo batterie RAM
  IF ERROR==-21010 THEN
    DREG (7) = 59
  END
;
  RETURN
.END
.PROGRAM pc1_chk_pc2_run()
;Controllo parallelo 2 in run
;
  IF NOT SIG(m_pc2_run) AND TASK(1002)<>1 AND b_pc_run THEN
    PULSE m_pc2_run,1
    PCABORT 2: 
    PCEXECUTE 2: autostart2.pc
  END
;
  RETURN
.END
.PROGRAM pc1_ciclo_soft()
;Abilitazione ciclo soft in manuale
;
  IF SIG(x_manuale) AND NOT b_ciclosoft THEN
    b_ciclosoft = TRUE
  END
;
  RETURN
.END
.PROGRAM pc1_control_var()
;Verifica dei parametri geometrici inseriti da pannello
;
; +-- max correzzione parametri in automatico
  i_offset_param = 1
; +-- altezza estrattore
  i_alt_estrattor = 35
; +-- altezza pinza
  i_alt_pinza = 150
; +-- distanza piani pressa
  i_dis_piani_pre = 700
; +-- max angolo taglio cappuccio coltello 1
  i_max_angolo[1] = 90
; +-- min angolo taglio cappuccio coltello 1
  i_min_angolo[1] = 0
; +-- max angolo taglio cappuccio coltello 2
  i_max_angolo[2] = 90
; +-- min angolo taglio cappuccio coltello 2
  i_min_angolo[2] = 0
;
; +-- condizioni per controllo tagli
  IF DREG(4)<140 AND (SIG(x_auto) OR SIG(x_manuale,x_man_tag) OR SIG(x_manuale,x_man_scar) OR SIG(x_manuale,x_tag_sing)) THEN
;
;	
;   +-- controllo ingombri robot/pressa
    IF (i_alt_sup+i_alt_inf+i_alt_estrattor+i_alt_pinza+i_alt_grif)>i_dis_piani_pre-10 THEN
      DREG (4) = 139
    END
;
;
;   +-- colcolo l'angolo tra punta coltello e portacoltello del coltello = ang_portacol
;   +-- diametro portacoltello/2 lunghezza inserto dovo infilo i coltello+lunghezza coltello
    IF cut_param[38]==1 OR cut_param[38]==2 THEN
      beta = (25.5+cut_param[18])
      ang_portacol = ATAN2(22.5,beta)
    END
;
;
;   +-- controlli taglio 1
    IF b_btaglio1 THEN
;     +-- angolo taglio 1
      IF (cut_param[29]>i_max_angolo[cut_param[38]] OR cut_param[29]<i_min_angolo[cut_param[38]]) THEN
        DREG (4) = 140
      END
;     +--controllo collisione coltello/griffa taglio 1
;     +--profondita del coltello oltre lo 0 alle varie inclinazioni = prof_1
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
        .delta1 = cut_param[18]*SIN(90-cut_param[29])
        .delta2 = (cut_param[18]+cut_param[32])*SIN(90-cut_param[29])
        prof_1 = .delta2-.delta1
      END
;     +--controllo collisione    
      IF cut_param[33]<5 AND cut_param[33]<0+prof_1 THEN
        DREG (4) = 144
      END
;     +-- controllo diametro taglio 1 maggiore a diametro anello
      IF cut_param[31]>i_diam_gom*1.1 THEN
        DREG (4) = 146
      END
;     +-- calcolo l'angolo utile tra il punto di taglio e l'altezza anello per sapere se vado in collisione con l'anello == ang_utile1
;     +-- spe_lam == spessore della lamiera
      spe_lam = 1
      alfa1 = ((i_diam_gom-cut_param[31])/2)-spe_lam    ; diametro taglio
      gamma1 = (i_alt_gommato-cut_param[33])          ; altezza taglio
      ang_utile1 = ATAN2(alfa1,gamma1)
;     +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;     +-- e che (l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto alla lama del coltello)
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
;       +--se il diametro di taglio e + grande del diametro gommato non controllo l'angolo utile	
        IF cut_param[31]<=i_diam_gom THEN
;         +--se il punto di taglio e + basso di coltello-profondita	   
          IF (i_alt_gommato-cut_param[33])<(cut_param[18]-cut_param[32]) THEN
;           +--controllo che il coltello non collida con anello
            IF (ang_utile1<cut_param[29]) THEN
;             DREG (4) = 148
            END
          ELSE
;           +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;           +-- e che l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto all'angolo lama/portacoltello
            IF (i_alt_gommato-cut_param[33])>(cut_param[18]-cut_param[32]) THEN
              IF (cut_param[29]+ang_portacol)>ang_utile1 THEN
                DREG (4) = 148
              END
            END
          END
        END
      END
;     +-- angolo di spoglia taglio 1
      IF cut_param[30]>7 OR cut_param[30]<-7 THEN
        DREG (4) = 184
      END
;     +-- altezza anello per taglio 1
      IF cut_param[33]>i_alt_gommato*1.5 THEN
        DREG (4) = 186
      END
;     +-- velocita' mandrino taglio 1
      IF cut_param[25]>1800 THEN
        DREG (4) = 158
      END
    END
;
;
;   +-- controlli taglio 2
    IF b_btaglio2 THEN
;     +-- angolo taglio 2
      IF (cut_param[46]>i_max_angolo[cut_param[38]] OR cut_param[46]<i_min_angolo[cut_param[38]]) THEN
        DREG (4) = 141
      END
;     +--controllo collisione coltello/griffa taglio 2
;     +--profondita del coltello oltre lo 0 alle varie inclinazioni = prof_2
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
        .delta3 = cut_param[18]*SIN(90-cut_param[46])
        .delta4 = (cut_param[18]+cut_param[49])*SIN(90-cut_param[46])
        prof_2 = .delta4-.delta3
      END
;     +--controllo collisione       
      IF cut_param[50]<5 AND cut_param[50]<0+prof_2 THEN
        DREG (4) = 145
      END
;     +-- controllo diametro taglio 2 maggiore a diametro anello
      IF cut_param[48]>i_diam_gom*1.1 THEN
        DREG (4) = 147
      END
;     +-- calcolo l'angolo utile tra il punto di taglio e l'altezza anello per sapere se vado in collisione con l'anello == ang_utile2
;     +-- spe_lam == spessore della lamiera
      spe_lam = 1
      alfa2 = ((i_diam_gom-cut_param[48])/2)-spe_lam    ; diametro taglio
      gamma2 = (i_alt_gommato-cut_param[50])          ; altezza taglio
      ang_utile2 = ATAN2(alfa2,gamma2)
;     +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;     +-- e che (l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto alla lama del coltello)
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
;       +--se il diametro di taglio e + grande del diametro gommato non controllo l'angolo utile	
        IF cut_param[48]<=i_diam_gom THEN
;         +--se il punto di taglio e + basso di coltello-profondita	   
          IF (i_alt_gommato-cut_param[50])<(cut_param[18]-cut_param[49]) THEN
;           +--controllo che il coltello non collida con anello
            IF (ang_utile2<cut_param[46]) THEN
;             DREG (4) = 149
            END
          ELSE
;           +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;           +-- e che l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto all'angolo lama/portacoltello
            IF (i_alt_gommato-cut_param[50])>(cut_param[18]-cut_param[49]) THEN
              IF (cut_param[46]+ang_portacol)>ang_utile2 THEN
                DREG (4) = 149
              END
            END
          END
        END
      END
;     +-- velocita' mandrino taglio 2
      IF cut_param[42]>1800 THEN
        DREG (4) = 159
      END
;     +-- angolo di spoglia taglio 2
      IF cut_param[47]>7 OR cut_param[47]<-7 THEN
        DREG (4) = 185
      END
;     +-- altezza anello per taglio 2
      IF cut_param[50]>i_alt_gommato*1.5 THEN
        DREG (4) = 187
      END
    END
;
;
;   +-- controlli taglio 3
    IF b_btaglio3 THEN
;     +-- angolo taglio 3
      IF (cut_param[63]>i_max_angolo[cut_param[38]] OR cut_param[63]<i_min_angolo[cut_param[38]]) THEN
        DREG (4) = 150
      END
;     +--controllo collisione coltello/griffa taglio 3
;     +--profondita del coltello oltre lo 0 alle varie inclinazioni = prof_3
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
        .delta5 = cut_param[18]*SIN(90-cut_param[63])
        .delta6 = (cut_param[18]+cut_param[66])*SIN(90-cut_param[63])
        prof_3 = .delta6-.delta5
      END
;     +--controllo collisione 
      IF cut_param[67]<5 AND cut_param[67]<0+prof_3 THEN
        DREG (4) = 169
      END
;     +-- controllo diametro taglio 3 maggiore a diametro anello
      IF cut_param[65]>i_diam_gom*1.1 THEN
        DREG (4) = 152
      END
;     +-- calcolo l'angolo utile tra il punto di taglio e l'altezza anello per sapere se vado in collisione con l'anello == ang_utile3
;     +-- spe_lam == spessore della lamiera
      spe_lam = 1
      alfa3 = ((i_diam_gom-cut_param[65])/2)-spe_lam    ; diametro taglio
      gamma3 = (i_alt_gommato-cut_param[67])          ; altezza taglio
      ang_utile3 = ATAN2(alfa3,gamma3)
;     +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;     +--  e che (l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto alla lama del coltello)
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
        IF (i_alt_gommato-cut_param[67])<(cut_param[18]-cut_param[66]) THEN
          IF (ang_utile3<cut_param[63]) THEN
            DREG (4) = 171
          END
        ELSE
;         +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;         +--e che l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto all'angolo lama/portacoltello
          IF (i_alt_gommato-cut_param[67])>(cut_param[18]-cut_param[66]) THEN
            IF (cut_param[63]+ang_portacol)>ang_utile3 THEN
              DREG (4) = 171
            END
          END
        END
      END
;     +-- angolo di spoglia taglio 3
      IF cut_param[64]>7 OR cut_param[64]<-7 THEN
        DREG (4) = 154
      END
;     +-- altezza anello per taglio 3
      IF cut_param[67]>i_alt_gommato*1.5 THEN
        DREG (4) = 156
      END
;     +-- velocita' mandrino taglio 3
      IF cut_param[59]>1800 THEN
        DREG (4) = 160
      END
    END
;
;
;   +-- controlli taglio 4
    IF b_btaglio4 THEN
;     +-- angolo taglio 4
      IF (cut_param[80]>i_max_angolo[cut_param[38]] OR cut_param[80]<i_min_angolo[cut_param[38]]) THEN
        DREG (4) = 151
      END
;     +--controllo collisione coltello/griffa taglio 4
;     +--profondita del coltello oltre lo 0 alle varie inclinazioni = prof_4
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
        .delta7 = cut_param[18]*SIN(90-cut_param[80])
        .delta8 = (cut_param[18]+cut_param[83])*SIN(90-cut_param[80])
        prof_4 = .delta8-.delta7
      END
;     +--controllo collisione 
      IF cut_param[84]<5 AND cut_param[84]<0+prof_4 THEN
        DREG (4) = 170
      END
;     +-- controllo diametro taglio 4 maggiore a diametro anello
      IF cut_param[82]>i_diam_gom*1.1 THEN
        DREG (4) = 153
      END
;     +-- calcolo l'angolo utile tra il punto di taglio e l'altezza anello per sapere se vado in collisione con l'anello == ang_utile4
;     +-- spe_lam == spessore della lamiera
      spe_lam = 1
      alfa4 = ((i_diam_gom-cut_param[82])/2)-spe_lam    ; diametro taglio
      gamma4 = (i_alt_gommato-cut_param[84])          ; altezza taglio
      ang_utile4 = ATAN2(alfa4,gamma4)
;     +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;     +-- e che (l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto alla lama del coltello)
      IF cut_param[38]==1 OR cut_param[38]==2 THEN
        IF (i_alt_gommato-cut_param[84])<(cut_param[18]-cut_param[83]) THEN
          IF (ang_utile4<cut_param[80]) THEN
            DREG (4) = 172
          END
        ELSE
;         +-- verifico che (l'altezza di taglio, rispetto all'altezza anello) non sia piu profonda della (lama coltello - la profondita)
;         +-- e che l'angolo di taglio non sia piu grande dell'angolo di ingombro, rispetto all'angolo lama/portacoltello
          IF (i_alt_gommato-cut_param[84])>(cut_param[18]-cut_param[83]) THEN
            IF (cut_param[80]+ang_portacol)>ang_utile4 THEN
              DREG (4) = 172
            END
          END
        END
      END
;     +-- angolo di spoglia taglio 4
      IF cut_param[81]>7 OR cut_param[81]<-7 THEN
        DREG (4) = 155
      END
;     +-- altezza anello per taglio 4
      IF cut_param[84]>i_alt_gommato*1.5 THEN
        DREG (4) = 157
      END
;     +-- velocita' mandrino taglio 4
      IF cut_param[76]>1800 THEN
        DREG (4) = 161
      END
    END
;
;
;     +-- limite modifica parametri
;     +-- taglio 1
    IF b_btaglio1 THEN
      IF ABS(cut_param[31]-diam_t1_old)>i_offset_param THEN
        DREG (4) = 163
      END
      IF ABS(cut_param[33]-alt_ane1_old)>i_offset_param THEN
        DREG (4) = 165
      END
      IF ABS(cut_param[29]-angolo1_old)>i_offset_param THEN
        DREG (4) = 167
      END
      IF ABS(cut_param[32]-profon1_old)>i_offset_param THEN
        DREG (4) = 173
      END
      IF ABS(cut_param[30]-ang_sfoo1)>i_offset_param THEN
        DREG (4) = 197
      END
    END
;     +-- taglio 2
    IF b_btaglio2 THEN
      IF ABS(cut_param[48]-diam_t2_old)>i_offset_param THEN
        DREG (4) = 164
      END
      IF ABS(cut_param[50]-alt_ane2_old)>i_offset_param THEN
        DREG (4) = 166
      END
      IF ABS(cut_param[46]-angolo2_old)>i_offset_param THEN
        DREG (4) = 168
      END
      IF ABS(cut_param[49]-profon2_old)>i_offset_param THEN
        DREG (4) = 174
      END
      IF ABS(cut_param[47]-ang_sfoo2)>i_offset_param THEN
        DREG (4) = 198
      END
    END
;     +-- taglio 3
    IF b_btaglio3 THEN
      IF ABS(cut_param[65]-diam_t3_old)>i_offset_param THEN
        DREG (4) = 178
      END
      IF ABS(cut_param[67]-alt_ane3_old)>i_offset_param THEN
        DREG (4) = 182
      END
      IF ABS(cut_param[63]-angolo3_old)>i_offset_param THEN
        DREG (4) = 176
      END
      IF ABS(cut_param[66]-profon3_old)>i_offset_param THEN
        DREG (4) = 188
      END
      IF ABS(cut_param[64]-ang_sfoo3)>i_offset_param THEN
        DREG (4) = 180
      END
    END
;     +-- taglio 4
    IF b_btaglio4 THEN
      IF ABS(cut_param[82]-diam_t4_old)>i_offset_param THEN
        DREG (4) = 179
      END
      IF ABS(cut_param[84]-alt_ane4_old)>i_offset_param THEN
        DREG (4) = 183
      END
      IF ABS(cut_param[80]-angolo4_old)>i_offset_param THEN
        DREG (4) = 177
      END
      IF ABS(cut_param[83]-profon4_old)>i_offset_param THEN
        DREG (4) = 189
      END
      IF ABS(cut_param[81]-ang_sfoo4)>i_offset_param THEN
        DREG (4) = 181
      END
    END
;
;
;   +-- se dati non corretti ricarica il valore vecchio
    IF DREG(4)>139 THEN
;     +-- set dati errati a pannello
      b_data_wrong = TRUE
      cut_param[29] = angolo1_old
      cut_param[46] = angolo2_old
      cut_param[63] = angolo3_old
      cut_param[80] = angolo4_old
      cut_param[33] = alt_ane1_old
      cut_param[50] = alt_ane2_old
      cut_param[67] = alt_ane3_old
      cut_param[84] = alt_ane4_old
      cut_param[32] = profon1_old
      cut_param[49] = profon2_old
      cut_param[66] = profon3_old
      cut_param[83] = profon4_old
      cut_param[31] = diam_t1_old
      cut_param[48] = diam_t2_old
      cut_param[65] = diam_t3_old
      cut_param[82] = diam_t4_old
      cut_param[25] = speed_old1
      cut_param[42] = speed_old2
      cut_param[59] = speed_old3
      cut_param[76] = speed_old4
      cut_param[30] = ang_sfoo1
      cut_param[47] = ang_sfoo2
      cut_param[64] = ang_sfoo3
      cut_param[81] = ang_sfoo4
      i_diam_gom = diam_g_old
      i_alt_lam = i_alt_lam_old
      i_alt_gommato = alt_gom_old
      i_alt_sup = i_alt_sup_old
      i_alt_grif = alt_gri_old
      i_alt_inf = i_alt_inf_old
      cut_param[29] = angolo1_old
      cut_param[46] = angolo2_old
      cut_param[63] = angolo3_old
      cut_param[80] = angolo4_old
      cut_param[33] = alt_ane1_old
      cut_param[50] = alt_ane2_old
      cut_param[67] = alt_ane3_old
      cut_param[84] = alt_ane4_old
      cut_param[32] = profon1_old
      cut_param[49] = profon2_old
      cut_param[66] = profon3_old
      cut_param[83] = profon4_old
      cut_param[31] = diam_t1_old
      cut_param[48] = diam_t2_old
      cut_param[65] = diam_t3_old
      cut_param[82] = diam_t4_old
      cut_param[30] = ang_sfoo1
      cut_param[47] = ang_sfoo2
      cut_param[64] = ang_sfoo3
      cut_param[81] = ang_sfoo4
    ELSE
;
      b_data_wrong = FALSE
      angolo1_old = cut_param[29]
      angolo2_old = cut_param[46]
      angolo3_old = cut_param[63]
      angolo4_old = cut_param[80]
      alt_ane1_old = cut_param[33]
      alt_ane2_old = cut_param[50]
      alt_ane3_old = cut_param[67]
      alt_ane4_old = cut_param[84]
      profon1_old = cut_param[32]
      profon2_old = cut_param[49]
      profon3_old = cut_param[66]
      profon4_old = cut_param[83]
      diam_t1_old = cut_param[31]
      diam_t2_old = cut_param[48]
      diam_t3_old = cut_param[65]
      diam_t4_old = cut_param[82]
      speed_old1 = cut_param[25]
      speed_old2 = cut_param[42]
      speed_old3 = cut_param[59]
      speed_old4 = cut_param[76]
      ang_sfoo1 = cut_param[30]
      ang_sfoo2 = cut_param[47]
      ang_sfoo3 = cut_param[64]
      ang_sfoo4 = cut_param[81]
      diam_g_old = i_diam_gom
      i_alt_lam_old = i_alt_lam
      alt_gom_old = i_alt_gommato
      i_alt_sup_old = i_alt_sup
      alt_gri_old = i_alt_grif
      i_alt_inf_old = i_alt_inf
    END
  END
;
  RETURN
.END
.PROGRAM pc1_muovih_run()
;Muovihome in esecuzione
;
  SIGNAL (-1+(2*ABS(WHICHTASK("muovihome")==1 AND SWITCH(POWER ) AND SWITCH(CS ))))*y_phome_run
;
  RETURN
.END
.PROGRAM pc1_pos_cambio()
;Controllo robot in posizione di cambio tipo
;
  .b_cond_a = ABS(DEXT(#fi_cambio,1)-DEXT(#HERE,1))<0.5
  .b_cond_b = ABS(DEXT(#fi_cambio,2)-DEXT(#HERE,2))<0.5
  .b_cond_c = ABS(DEXT(#fi_cambio,3)-DEXT(#HERE,3))<0.5
  .b_cond_d = ABS(DEXT(#fi_cambio,4)-DEXT(#HERE,4))<0.5
  .b_cond_e = ABS(DEXT(#fi_cambio,5)-DEXT(#HERE,5))<0.5
  .b_cond_f = ABS(DEXT(#fi_cambio,6)-DEXT(#HERE,6))<0.5
  IF .b_cond_a AND .b_cond_b AND .b_cond_c AND .b_cond_d AND .b_cond_e AND .b_cond_f THEN
    PULSE y_pos_cambio,1
  ELSE
;
    SIGNAL -y_pos_cambio
  END
;
  RETURN
.END
.PROGRAM pc1_read_var()
;Lettura variabili da pannello
;
; +-- stato connessione TCP Robot/SV
  IF tcp_enabled AND b_cam1_on THEN
    IF st_conn_sv==2 OR st_conn_sv==3 THEN
      b_tcp_conn = TRUE
      b_tcp_alm1 = FALSE
      b_tcp_alm2 = FALSE
      UTIMER @r_tcp_tout1 = 0
      UTIMER @r_tcp_tout2 = 0
      PULSE y_tcp_sv_ok,0.5
    ELSE
      b_tcp_conn = FALSE
    END
    IF NOT b_tcp_conn THEN
      IF UTIMER(@r_tcp_tout1)>3 AND NOT b_tcp_alm1 THEN
        DREG (4) = 113
        b_tcp_alm1 = TRUE
      END
      IF UTIMER(@r_tcp_tout2)>60 AND NOT b_tcp_alm2 THEN
        PULSE y_rob_fine,1
        b_tcp_alm2 = TRUE
      END
    END
  ELSE
;
    b_tcp_alm1 = FALSE
    b_tcp_alm2 = FALSE
    UTIMER @r_tcp_tout1 = 0
    UTIMER @r_tcp_tout2 = 0
  END
; +-- inclusione mandrel
  SIGNAL (-1+(2*ABS(b_inc_mandrel)))*y_mandrel_inc
;
; +-- visione artificiale attiva
  SIGNAL (-1+(2*ABS(b_cam1_on)))*y_cam1_on
; +-- telecamera 1 pronta
  SIGNAL (-1+(2*ABS(b_cam1_ready)))*y_cam1_ready
; +-- telecamera 1 occupata
  SIGNAL (-1+(2*ABS(b_cam1_busy)))*y_cam1_busy
; +-- dati pronti da telecamera 1
  SIGNAL (-1+(2*ABS(b_cam1_dati)))*m_cam1_dati
;
; +-- Scrittura variabili telecamera ptfe verso kladder
; +-- telecamera ptfe pronta
  SIGNAL (-1+(2*ABS(b_cam_p_ready)))*y_cam_p_ready
; +-- telecamera ptfe occupata
  SIGNAL (-1+(2*ABS(b_cam_p_busy)))*y_cam_p_busy
; +-- telecamera ptfe inclusa / esclusa
  SIGNAL (-1+(2*ABS(b_inc_cam_p)))*y_cam_p_inc
;
; +-- Scrittura variabili telecamera 3d verso kladder
; +-- telecamera 3D pronta
  SIGNAL (-1+(2*ABS(b_cam_3d_ready)))*y_cam_3d_ready
; +-- telecamera 3D occupata
  SIGNAL (-1+(2*ABS(b_cam_3d_busy)))*y_cam_3d_busy
; +-- telecamera 3D fine acquisizione 
  SIGNAL (-1+(2*ABS(b_cam_3d_endacq)))*y_cam_3d_endacq
; +-- telecamera 3D pezzo buono
  SIGNAL (-1+(2*ABS(b_cam_3d_buono)))*y_cam_3d_buono
; +-- telecamera 3D pezzo scarto
  SIGNAL (-1+(2*ABS(b_cam_3d_scarto)))*y_cam_3d_scarto
; +-- telecamera 3D inclusa / esclusa
  SIGNAL (-1+(2*ABS(b_inc_cam_3d)))*y_cam_3d_inc
;
; +-- fine elaborazione dati
  SIGNAL (-1+(2*ABS(b_cam1_endacq)))*y_cam1_endacq
; +-- taglio 1 attivo
  SIGNAL (-1+(2*ABS(b_btaglio1)))*y_tag_attivo[1]
; +-- taglio 2 attivo
  SIGNAL (-1+(2*ABS(b_btaglio2)))*y_tag_attivo[2]
; +-- taglio 3 attivo
  SIGNAL (-1+(2*ABS(b_btaglio3)))*y_tag_attivo[3]
; +-- taglio 4 attivo
  SIGNAL (-1+(2*ABS(b_btaglio4)))*y_tag_attivo[4]
; +-- monitor speed
; +-- ciclo automatico e non ciclo soft // oppure ciclo manuale di taglio
  IF ((NOT b_ciclosoft) AND SIG(x_auto)) OR (SIG(x_manuale) AND SIG(x_man_tag)) OR (SIG(x_manuale) AND SIG(x_man_scar) AND NOT b_ciclosoft) THEN
    IF r_speed_03<>MSPEED THEN
      MC SPEED MAXVAL(MINVAL(100,r_speed_03),1)
    END
  ELSE
;
;   +-- in tutte le altre condizioni 
    IF MSPEED<>MINVAL(40,r_speed_03) AND SIG(x_manuale) THEN
      MC speed MINVAL(40,r_speed_03)
    END
  END
; +-- gestione velocita' r_speed_02 fuori pressa ciclo automatico
  IF SIG(x_fine) OR b_speed_fp02 THEN
    speed_fp = MAXVAL(MINVAL(100,r_speed_02),20)
  ELSE
;
    speed_fp = MINVAL(r_speed_02,r_speed_auto)
  END
; +-- reset valori di teach
  IF NOT b_res_teach THEN
    .b_res_teach = FALSE
  END
  IF b_res_teach AND NOT .b_res_teach THEN
    .b_res_teach = TRUE
    r_temp_nx = 0
    r_temp_ny = 0
    r_temp_nz = 0
    FOR .i = 1 TO 32
      nx[.i] = 0
      ny[.i] = 0
      nz[.i] = 0
    END
  END
; +-- reset tagli coltello 1
  IF NOT b_res_tag1 THEN
    .b_res_tag1 = FALSE
  END
  IF b_res_tag1 AND NOT .b_res_tag1 THEN
    .b_res_tag1 = TRUE
    i_coltello1 = 0
    PULSE y_res_cutlife1,1
  END
; +-- reset tagli coltello 2
  IF NOT b_res_tag2 THEN
    .b_res_tag2 = FALSE
  END
  IF b_res_tag2 AND NOT .b_res_tag2 THEN
    .b_res_tag2 = TRUE
    i_coltello2 = 0
    PULSE y_res_cutlife2,1
  END
; +-- reset contatore parziale pezzi prelevati da pressa
  IF NOT b_res_parz THEN
    .b_res_parz = FALSE
  END
  IF b_res_parz AND NOT .b_res_parz THEN
    .b_res_parz = TRUE
    i_pezzi_parz = 0
  END
; +-- reset dati acquisiti da visione
  IF (SIG(y_cam1_acq)) AND SIG(m_ok_read_cam1) THEN
    SIGNAL -m_ok_read_cam1
  END
; +-- percentuale utilizzo coltello 1,2
  i_utcolt1 = INT((i_colt1_max*cut_param[9])/(MAXVAL(i_colt1_max,1)*(MAXVAL(i_colt1_max,1)/100)))
  i_utcolt2 = INT((i_colt2_max*cut_param[10])/(MAXVAL(i_colt2_max,1)*(MAXVAL(i_colt2_max,1)/100)))
;
  RETURN
.END
.PROGRAM pc1_res_abs.sp()
;Reset absolute speed
;
  IF SWITCH(ABS.SPEED) AND NOT SWITCH(CS ) THEN
    ABS.SPEED OFF
  END
;
  RETURN
.END
.PROGRAM pc1_res_out()
;Reset uscite a pressa se manca automatico
;
  IF NOT SIG(x_automatico) THEN
    IF SIG(y_arb_ok_ch) THEN
      SIGNAL -y_arb_ok_ch
    END
    IF SIG(y_arb_st_free) THEN
      SIGNAL -y_arb_st_free
    END
  END
;
  RETURN
.END
.PROGRAM pc1_start_robot()
;Seleziona ed avvia programma robot
;
; +-- antiripetizione pulsante
  IF NOT SIG(x_selez_home) THEN
    .b_start = FALSE
  END
;	+-- se siamo in repeat
  IF SWITCH(REPEAT ) THEN
;   +-- richiesta seleziona pg o muovihome
    IF SIG(x_manuale,x_selez_home) AND NOT .b_start AND NOT SWITCH(TEACH_LOCK ) AND SWITCH(RUN ) THEN
      .b_start = TRUE
      i_fase_start = 1
      UTIMER .@r_tim_start = 0
      MC HOLD
    END
;   +-- robot non in ciclo, error reset e prime robot
    IF i_fase_start==1 AND NOT SWITCH(CS ) THEN
      IF UTIMER(.@r_tim_start)>1 THEN
        i_fase_start = 2
        UTIMER .@r_tim_start = 0
        MC ERESET
        MC PRIME master
      END
    END
;   +-- verifica programma master selezionato
    IF i_fase_start==2 AND WHICHTASK("master")==1 THEN
      IF UTIMER(.@r_tim_start)>1 THEN
        i_fase_start = 3
        UTIMER .@r_tim_start = 0
        MC ZPOW ON
      END
    END
;   +-- avvio ciclo
    IF i_fase_start==3 AND SWITCH(POWER ) THEN
      IF UTIMER(.@r_tim_start)>1 THEN
        IF NOT SWITCH(CS ) THEN
          i_fase_start = 0
          MC continue
        ELSE
          i_fase_start = 0
        END
      END
    END
  ELSE
;
    i_fase_start = 0
  END
;
  RETURN
.END
.PROGRAM pc1_stato_macc()
;Invio stato macchina a pannello
;
; +-- macchina in manuale
  IF NOT b_anomalia THEN
    IF SIG(x_manuale,x_step) THEN
      $stato_macc = "1";"MODALITA\' STEP"
    ELSE
;
      IF SIG(x_manuale,x_man_tag) THEN
        $stato_macc = "2";"TAGLIO MANUALE"
      ELSE
;
        IF SIG(x_manuale,x_cambio) THEN
          $stato_macc = "3";"CAMBIO TIPO"
        ELSE
;
          IF SIG(x_manuale,x_man_scar) THEN
            $stato_macc = "4";"SCARICO PRESSA MANUALE"
          ELSE
;
;            IF SIG(x_manuale,x_posa_colt) THEN
;              $stato_macc = "5";"POSA COLTELLO"
;            ELSE
;
            IF SIG(x_manuale) THEN
              $stato_macc = "6";"MANUALE"
            ELSE
;
              IF SIG(x_auto,x_fine) THEN
                $stato_macc = "7";"FINE CICLO"
              ELSE
;
                IF SIG(x_auto) THEN
                  $stato_macc = "8";"AUTOMATICO"
                ELSE
;
                  IF NOT SIG(x_auto) AND NOT SIG(x_manuale) THEN
                    $stato_macc = "9";"SELEZIONA MODO"
                  ELSE
;
                    IF SIG(x_manuale,x_test_cam) THEN
                      $stato_macc = "10";"TEST MANUALE TELECAMERE"
                    ELSE
;
                      $stato_macc = "11";"ANOMALIA"
                    END
                  END
                END
              END
            END
          END
        END
      END
    END
  ELSE
;
    $stato_macc = "10";"ANOMALIA"
  END
;
  RETURN
.END
.PROGRAM pc1_tim_auto()
;Contaore robot in ciclo automatico
;
  IF NOT b_res_counter THEN
    IF NOT SIG(x_auto) THEN
      UTIMER @r_tim_pre1 = 0.1
      IF b_tim_auto THEN
        r_tim_provv1 = r_tim_auto
        b_tim_auto = FALSE
      END
    END
;
    IF SIG(x_auto) THEN
      b_tim_auto = TRUE
      r_tim_auto = r_tim_provv1+UTIMER(@r_tim_pre1)
    END
;
;   +-- ore robot in automatico
    IF INT(MAXVAL(r_tim_auto/3600,0))>i_time_auto THEN
      i_time_auto = i_time_auto+1
    END
  ELSE
;
    UTIMER @r_tim_pre1 = 0.1
    r_tim_provv1 = 0.1
    r_tim_auto = 0.1
    i_time_auto = 0.1
  END
;
  RETURN
.END
.PROGRAM pc1_tim_effic()
;Lettura tempo efficienza macchina
;
  i_efficienza = INT((i_time_auto/i_time_on)*1000)/10
;
  RETURN
.END
.PROGRAM pc1_tim_on()
;Lettura tempo macchina accesa
;
  IF NOT b_res_counter THEN
    IF NOT SIG(x_ausiliari) THEN
      UTIMER @r_tim_pre2 = 0.1
      IF b_tim_on THEN
        r_tim_provv2 = r_tim_on
        b_tim_on = FALSE
      END
    END
;
    IF SIG(x_ausiliari) THEN
      b_tim_on = TRUE
      r_tim_on = r_tim_provv2+UTIMER(@r_tim_pre2)
    END
;
;   +-- def. ore,minuti e secondi di robot in cycle start
    IF INT(MAXVAL(r_tim_on/3600,0))>i_time_on THEN
      i_time_on = i_time_on+1
    END
  ELSE
;
    UTIMER @r_tim_pre2 = 0.1
    r_tim_provv2 = 0.1
    r_tim_on = 0.1
    i_time_on = 0.1
  END
;
  RETURN
.END
.PROGRAM pc1_tim_pressa()
;Lettura tempo pressa aperta in automatico
;
  IF NOT b_res_counter THEN
    IF SIG(x_arb_auto) THEN
      IF NOT SIG(x_arb_ch) AND b_time_pressa THEN
        UTIMER @r_tim_pre3 = 0.1
        b_time_pressa = FALSE
      END
      IF SIG(x_arb_ch) AND NOT b_time_pressa THEN
        i_time_pressa = UTIMER(@r_tim_pre3)
        b_time_pressa = TRUE
      END
    END
;
;   +-- lettura tempo ciclo
    IF SIG(x_arb_auto) THEN
      IF NOT SIG(x_arb_ch) AND NOT b_time_robot AND b_one_time THEN
        i_time_robot = UTIMER(@r_time_robot)
        UTIMER @r_time_robot = 0
        b_time_robot = TRUE
        b_one_time = FALSE
      END
      IF b_time_robot AND NOT SIG(x_arb_ch) AND b_one_time THEN
        i_time_robot = UTIMER(@r_time_robot)
        UTIMER @r_time_robot = 0
        b_time_robot = FALSE
        b_one_time = FALSE
      END
      IF SIG(x_arb_vite) THEN
        b_one_time = TRUE
      END
    END
  ELSE
;
    UTIMER @r_tim_pre3 = 0.1
    UTIMER @r_time_robot = 0.1
    i_time_pressa = 0.1
    i_time_robot = 0.1
    i_pezzi_tot = 0.1
  END
;	
  RETURN
.END
.PROGRAM pc1_whichtask()
;Verifica programmi da non eseguire in automatico
;
  .b_reset = FALSE
  IF SWITCH(REPEAT ) THEN
    IF WHICHTASK("zzz_set_up")==1 THEN
      .b_reset = TRUE
    END
    IF WHICHTASK("aaa_align")==1 THEN
      .b_reset = TRUE
    END
    IF WHICHTASK("aaa_muovihome")==1 THEN
      .b_reset = TRUE
    END
;
    IF .b_reset THEN
      MC HOLD
      MC KILL
    END
  END
;
  RETURN
.END
.PROGRAM pc1_write_kl()
;Scrittura variabili verso kladder
;
; +-- Abilitazione caricamento mandrel
  IF (NOT b_btaglio1 AND NOT b_btaglio2 AND NOT b_btaglio3 AND NOT b_btaglio4) THEN
    SIGNAL -y_ab_mand
  ELSE
    SIGNAL y_ab_mand
  END
;
  RETURN
.END
.PROGRAM pc2_change_par(.par,.chk_par)
;Controllo cambio parametri
;
  IF cut_param[.chk_par]<>cut_param_old[.chk_par] AND .chk_par<>9 AND .chk_par<>10 AND .chk_par<>23 THEN
    .par = .chk_par
  END
;
  RETURN
.END
.PROGRAM pc2_chk_pc1_run()
;Controllo parallelo 1 in run
;
  IF NOT SIG(m_pc1_run) AND TASK(1001)<>1 AND b_pc_run THEN
    PULSE m_pc1_run,1
    PCABORT
    PCEXECUTE autostart.pc
  END
;
  RETURN
.END
.PROGRAM pc2_cut_client()
;Gestione comunicazione con RS5
;
; +-- controllo cambio parametri
  .par = -1
;                      +------------- parametro da scrivere
;                      |   +--------- parametro da controllare
  CALL pc2_change_par(.par,chk_par)
  chk_par = chk_par+1
  IF chk_par=>93 THEN
    chk_par = 0
  END
;
; +-- invio parametro
  IF .par>0 THEN
    CALL pc2_cut_write(.par,cut_param[.par],.esito)
    IF .esito<>0 THEN
      TYPE "esito",.esito
      DREG (7) = 98
      b_res_client = TRUE
      RETURN
    ELSE
;
      cut_param_old[.par] = cut_param[.par]
    END
  ELSE
;
;   +-- lettura tagli coltello 1 da RS5
    IF UTIMER(@pc2_t_read)>1 AND read_par==1 AND (NOT b_res_tag1 AND NOT b_res_tag2) THEN
      read_par = 2
      CALL pc2_cut_read(9,cut_param[9],.esito)
      IF .esito<>0 THEN
        DREG (7) = 98
        b_res_client = TRUE
        RETURN
      ELSE
;
        cut_param_old[9] = cut_param[9]
      END
    END
;
;   +-- lettura tagli coltello 2 da RS5
    IF UTIMER(@pc2_t_read)>2 AND read_par==2 AND (NOT b_res_tag1 AND NOT b_res_tag2) THEN
      read_par = 3
      CALL pc2_cut_read(10,cut_param[10],.esito)
      IF .esito<>0 THEN
        DREG (7) = 98
        b_res_client = TRUE
        RETURN
      ELSE
;
        cut_param_old[10] = cut_param[10]
      END
      UTIMER @pc2_t_read = 0
    END
;
;   +-- lettura tempo ciclo RS5
    IF UTIMER(@pc2_t_read)>2 AND read_par==3 THEN
      read_par = 1
      CALL pc2_cut_read(23,cut_param[23],.esito)
      IF .esito<>0 THEN
        DREG (7) = 98
        b_res_client = TRUE
        RETURN
      ELSE
;
        cut_param_old[23] = cut_param[23]
      END
      UTIMER @pc2_t_read = 0
    END
  END
;
  RETURN
.END
.PROGRAM pc2_cut_read(.n_par,.ret_value,.esito)
;Read one cutting parameter from robot 2
;
; +-- reset esito lettura
  .esito = -1
  UTIMER .@r_tim_cut = 0
  WHILE NOT SIG(x_cut_ready,-x_cut_busy,-x_cut_data_rdy,-x_cut_integer,-x_cut_negative,-x_cut_param_err,-y_res_comm) DO
    IF UTIMER(.@r_tim_cut)>5 THEN
      RETURN
    END
    TWAIT 0.05
  END
;
; +-- set ADDR and 'Read Request' bit
  BITS y_cut_addr,16 = .n_par
;
; +-- waiting for answer
  UTIMER .@r_tim_cut = 0
  WHILE NOT SIG(x_cut_data_rdy) AND NOT SIG(x_cut_param_err) DO
    PULSE y_cut_read_req
    IF UTIMER(.@r_tim_cut)>5 THEN
      .esito = -1
      RETURN
    END
    TWAIT 0.05
  END
  SIGNAL -y_cut_read_req
  IF SIG(x_cut_data_rdy) THEN
    cut_r_data = BITS(x_cut_r_data,16)
    IF NOT SIG(x_cut_integer) THEN
      cut_r_data = cut_r_data/100
    END
    IF SIG(x_cut_negative) THEN
      cut_r_data = -cut_r_data
    END
    .ret_value = cut_r_data
;   +-- esito OK
    .esito = 0
  ELSE
;
    IF SIG(x_cut_param_err) THEN
      .ret_value = -99999
      .esito = -2
    END
  END
;
; +-- reset cycle on 'Data Ready' bit reset
  UTIMER .@r_tim_cut = 0
  WHILE NOT SIG(-x_cut_data_rdy,-x_cut_param_err) DO
;   +-- sending ACK to server
    PULSE y_cut_ack
    IF UTIMER(.@r_tim_cut)>5 THEN
      .esito = -1  ;read not completed
      RETURN
    END
    TWAIT 0.05
  END
;
  SIGNAL -y_cut_ack
;
  RETURN
.END
.PROGRAM pc2_cut_write(.n_par,.inp_value,.esito)
;Write one cutting parameter on robot 2
;
  .esito = -1
  UTIMER .@r_tim_cut = 0
  WHILE NOT SIG(x_cut_ready,-x_cut_busy,-x_cut_data_rdy,-x_cut_integer,-x_cut_negative,-x_cut_param_err,-y_res_comm) DO
    IF UTIMER(.@r_tim_cut)>5 THEN
      RETURN
    END
    TWAIT 0.05
  END
;
; +-- set ADDR , W_DATA , INT , NEG and 'Write Request' bits
  BITS y_cut_addr,16 = .n_par
  cut_w_data = .inp_value
  IF .inp_value<0 THEN
    SIGNAL y_cut_negative
    cut_w_data = -cut_w_data
  END
  IF cut_w_data>655.35 THEN
    SIGNAL y_cut_integer
    cut_w_data = INT(cut_w_data)
    cut_w_data = MINVAL(cut_w_data,65535)
  ELSE
;
    cut_w_data = INT(cut_w_data*100)
  END
  BITS y_cut_w_data,16 = cut_w_data
;
; +-- waiting for verifing data
  UTIMER .@r_tim_cut = 0
  WHILE NOT SIG(x_cut_data_rdy) AND NOT SIG(x_cut_param_err) DO
    PULSE y_cut_write_req
    IF UTIMER(.@r_tim_cut)>5 THEN
      .esito = -1
      RETURN
    END
    TWAIT 0.05
  END
  SIGNAL -y_cut_write_req,-y_cut_negative,-y_cut_integer
  IF SIG(x_cut_param_err) THEN
    .esito = -2
  ELSE
    cut_v_data = BITS(x_cut_r_data,16)
    IF cut_v_data<>cut_w_data THEN
      .esito = -3
    ELSE
;     +-- esito OK
      .esito = 0
    END
  END
;
; +-- reset cycle on 'Data Ready' bit reset
  UTIMER .@r_tim_cut = 0
  WHILE NOT SIG(-x_cut_data_rdy,-x_cut_param_err) DO
;   +-- sending ACK to server
    PULSE y_cut_ack
    IF UTIMER(.@r_tim_cut)>5 THEN
      .esito = -1
      RETURN
    END
    TWAIT 0.05
  END
;
  SIGNAL -y_cut_ack
;
  RETURN
.END
.PROGRAM pc3_init_var()
;Inizializza le stringhe libere a s.v.
;
  FOR .i = 67 TO 99
    $var[.i] = $ENCODE(/F0.0,0)
  END
  FOR .i = 230 TO 249
    $var[.i] = $ENCODE(/F0.0,0)
  END
  FOR .i = 250 TO 1149
    $var[.i] = $ENCODE(/F0.0,0)
  END
; +-- forzatura ricetta visione per init
;$var[1150] = "not assigned" ; tolto da Peirone 2015/07/29. Creava problemi in apertura ricette visione ad avvio macchina.
  FOR .i = 0 TO 50
    IF NOT EXISTCHAR("$vid_stringa1["+$ENCODE(/F0.0,.i)+"]") THEN
      $vid_stringa1[.i] = " "
    END
    IF NOT EXISTCHAR("$vid_stringa2["+$ENCODE(/F0.0,.i)+"]") THEN
      $vid_stringa2[.i] = " "
    END
  END
; +-- forzo la scrittura delle variabili nell'array di appoggio per scambio dati
  tcp_gruppo_da_t = 2
  CALL pc3_write_var
;
  RETURN
.END
.PROGRAM pc3_invio_all()
;Invio allarmi a pannello ESA
;
  IF ((SIG(x_alm_plc) AND DREG(^Ha)<>0) OR DREG(^Hb)<>0 OR DREG(^Hc)<>0) AND NOT b_set_all AND NOT SIG(x_alm_reset) THEN
    IF SIG(x_alm_plc) AND DREG(^Ha)<>0 THEN
      i_allarme = DREG(^Ha)
    ELSE
;
      IF DREG(^Hb)<>0 THEN
        i_allarme = 199+DREG(^Hb)
      ELSE
;
        IF DREG(^Hc)<>0 THEN
          i_allarme = 240+DREG(^Hc)
        END
      END
    END
    DREG (7) = 0
    DREG (4) = 0
    b_set_all = TRUE
    b_anomalia = TRUE
;
    $vid_stringa1[50] = $vid_stringa1[49]
    $vid_stringa2[50] = $vid_stringa2[49]
    $vid_stringa1[49] = $vid_stringa1[48]
    $vid_stringa2[49] = $vid_stringa2[48]
    $vid_stringa1[48] = $vid_stringa1[47]
    $vid_stringa2[48] = $vid_stringa2[47]
    $vid_stringa1[47] = $vid_stringa1[46]
    $vid_stringa2[47] = $vid_stringa2[46]
    $vid_stringa1[46] = $vid_stringa1[45]
    $vid_stringa2[46] = $vid_stringa2[45]
    $vid_stringa1[45] = $vid_stringa1[44]
    $vid_stringa2[45] = $vid_stringa2[44]
    $vid_stringa1[44] = $vid_stringa1[43]
    $vid_stringa2[44] = $vid_stringa2[43]
    $vid_stringa1[43] = $vid_stringa1[42]
    $vid_stringa2[43] = $vid_stringa2[42]
    $vid_stringa1[42] = $vid_stringa1[41]
    $vid_stringa2[42] = $vid_stringa2[41]
    $vid_stringa1[41] = $vid_stringa1[40]
    $vid_stringa2[41] = $vid_stringa2[40]
    $vid_stringa1[40] = $vid_stringa1[39]
    $vid_stringa2[40] = $vid_stringa2[39]
    $vid_stringa1[39] = $vid_stringa1[38]
    $vid_stringa2[39] = $vid_stringa2[38]
    $vid_stringa1[38] = $vid_stringa1[37]
    $vid_stringa2[38] = $vid_stringa2[37]
    $vid_stringa1[37] = $vid_stringa1[36]
    $vid_stringa2[37] = $vid_stringa2[36]
    $vid_stringa1[36] = $vid_stringa1[35]
    $vid_stringa2[36] = $vid_stringa2[35]
    $vid_stringa1[35] = $vid_stringa1[34]
    $vid_stringa2[35] = $vid_stringa2[34]
    $vid_stringa1[34] = $vid_stringa1[33]
    $vid_stringa2[34] = $vid_stringa2[33]
    $vid_stringa1[33] = $vid_stringa1[32]
    $vid_stringa2[33] = $vid_stringa2[32]
    $vid_stringa1[32] = $vid_stringa1[31]
    $vid_stringa2[32] = $vid_stringa2[31]
    $vid_stringa1[31] = $vid_stringa1[30]
    $vid_stringa2[31] = $vid_stringa2[30]
    $vid_stringa1[30] = $vid_stringa1[29]
    $vid_stringa2[30] = $vid_stringa2[29]
    $vid_stringa1[29] = $vid_stringa1[28]
    $vid_stringa2[29] = $vid_stringa2[28]
    $vid_stringa1[28] = $vid_stringa1[27]
    $vid_stringa2[28] = $vid_stringa2[27]
    $vid_stringa1[27] = $vid_stringa1[26]
    $vid_stringa2[27] = $vid_stringa2[26]
    $vid_stringa1[26] = $vid_stringa1[25]
    $vid_stringa2[26] = $vid_stringa2[25]
    $vid_stringa1[25] = $vid_stringa1[24]
    $vid_stringa2[25] = $vid_stringa2[24]
    $vid_stringa1[24] = $vid_stringa1[23]
    $vid_stringa2[24] = $vid_stringa2[23]
    $vid_stringa1[23] = $vid_stringa1[22]
    $vid_stringa2[23] = $vid_stringa2[22]
    $vid_stringa1[22] = $vid_stringa1[21]
    $vid_stringa2[22] = $vid_stringa2[21]
    $vid_stringa1[21] = $vid_stringa1[20]
    $vid_stringa2[21] = $vid_stringa2[20]
    $vid_stringa1[20] = $vid_stringa1[19]
    $vid_stringa2[20] = $vid_stringa2[19]
    $vid_stringa1[19] = $vid_stringa1[18]
    $vid_stringa2[19] = $vid_stringa2[18]
    $vid_stringa1[18] = $vid_stringa1[17]
    $vid_stringa2[18] = $vid_stringa2[17]
    $vid_stringa1[17] = $vid_stringa1[16]
    $vid_stringa2[17] = $vid_stringa2[16]
    $vid_stringa1[16] = $vid_stringa1[15]
    $vid_stringa2[16] = $vid_stringa2[15]
    $vid_stringa1[15] = $vid_stringa1[14]
    $vid_stringa2[15] = $vid_stringa2[14]
    $vid_stringa1[14] = $vid_stringa1[13]
    $vid_stringa2[14] = $vid_stringa2[13]
    $vid_stringa1[13] = $vid_stringa1[12]
    $vid_stringa2[13] = $vid_stringa2[12]
    $vid_stringa1[12] = $vid_stringa1[11]
    $vid_stringa2[12] = $vid_stringa2[11]
    $vid_stringa1[11] = $vid_stringa1[10]
    $vid_stringa2[11] = $vid_stringa2[10]
    $vid_stringa1[10] = $vid_stringa1[9]
    $vid_stringa2[10] = $vid_stringa2[9]
    $vid_stringa1[9] = $vid_stringa1[8]
    $vid_stringa2[9] = $vid_stringa2[8]
    $vid_stringa1[8] = $vid_stringa1[7]
    $vid_stringa2[8] = $vid_stringa2[7]
    $vid_stringa1[7] = $vid_stringa1[6]
    $vid_stringa2[7] = $vid_stringa2[6]
    $vid_stringa1[6] = $vid_stringa1[5]
    $vid_stringa2[6] = $vid_stringa2[5]
    $vid_stringa1[5] = $vid_stringa1[4]
    $vid_stringa2[5] = $vid_stringa2[4]
    $vid_stringa1[4] = $vid_stringa1[3]
    $vid_stringa2[4] = $vid_stringa2[3]
    $vid_stringa1[3] = $vid_stringa1[2]
    $vid_stringa2[3] = $vid_stringa2[2]
    $vid_stringa1[2] = $vid_stringa1[1]
    $vid_stringa2[2] = $vid_stringa2[1]
    $vid_stringa1[1] = $ENCODE(/F0.0,i_allarme)
    $vid_stringa2[1] = $TIME+" "+$DATE(2)
  END
;
; +-- controllo richiesta reset allarme
  IF NOT SIG(x_alm_reset) THEN
    .b_alm_reset = FALSE
  END
  IF SIG(x_alm_reset) AND NOT .b_alm_reset THEN
    .b_alm_reset = TRUE
    i_allarme = 0
    b_set_all = FALSE
    b_anomalia = FALSE
  END
;
  RETURN
.END
.PROGRAM pc3_read_var()
;Lettura variabili da pannello
; 
; da 1 a 29 stato macchina         (gruppo 1)
; da 30 a 99 booleani              (gruppo 1)
; da 100 a 249 dati numerici       (gruppo 2)
; da 250 a 349 dati X Y visione    (gruppo 3)
; da 350 a 589 INPUT               (gruppo 4)
; da 590 a 799 OUT                 (gruppo 5)
; da 800 a 1099 anomalie           (gruppo 6)
; da 1100 a 1149 storico           (gruppo 7)
; da 1150 a 1249 parametri visione (gruppo 8)
;
  CASE tcp_gruppo_da_t OF
   VALUE 1:
;   +-- gruppo 1: stato macchina, booleani
    b_cam1_ok_acq = -1*VAL($var[30])                       ;elaborazione immagine terminata da telecamera 1
    b_cam1_dati = -1*VAL($var[31])                         ;dati pronti da telecamera 1
    b_cam1_ready = -1*VAL($var[32])                        ;telecamera 1 pronta
    b_cam1_busy = -1*VAL($var[33])                         ;telecamera 1 in fase di elaborazione
    b_cam1_endacq = -1*VAL($var[34])                       ;acquisizione immagine terminata da telecamera 1
    i_indice_trasm = VAL($var[35])                         ;indice trasmissione (non utilizzato dal robot)
    b_teach = -1*VAL($var[36])                             ;abilita movimenti da pannello
    b_conf = -1*VAL($var[37])                              ;conferma valori posizione
    b_accetta = -1*VAL($var[38])                           ;conferma parametri anello
;                                                          
    b_res_teach = -1*VAL($var[44])                         ;reset valori di teach
    b_res_tag1 = -1*VAL($var[45])                          ;reset tagli coltello 1
    b_res_tag2 = -1*VAL($var[46])                          ;reset tagli coltello 2
;                                                          
    b_res_counter = -1*VAL($var[48])                       ;reset contatori macchina
;
    b_res_parz = -1*VAL($var[52])                          ;reset contatori parziali
;
    b_cam_p_ready = -1*VAL($var[53])                         ;telecamera analisi ptfe pronta (vicino montaggio mandrel)
    b_cam_p_busy = -1*VAL($var[54])                          ;telecamera analisi ptfe in fase di elaborazione (vicino montaggio mandrel)
    b_cam_p_endacq = -1*VAL($var[55])                        ;acquisizione immagine analisi ptfe terminata (vicino montaggio mandrel)
    b_cam_p_buono = -1*VAL($var[56])                         ;pezzo buono da telecamera analisi ptfe (vicino montaggio mandrel)
    b_cam_p_scarto = -1*VAL($var[57])                        ;pezzo scarto da telecamera analisi ptfe (vicino montaggio mandrel)
;
    b_cam_3d_ready = -1*VAL($var[60])                         ;telecamera 3D pronta (sopra rs5)
    b_cam_3d_busy = -1*VAL($var[61])                          ;telecamera 3D in fase di elaborazione (sopra rs5)
    b_cam_3d_endacq = -1*VAL($var[62])                        ;acquisizione immagine 3D terminata (sopra rs5)
    b_cam_3d_buono = -1*VAL($var[63])                         ;pezzo buono da telecamera 3D (sopra rs5)
    b_cam_3d_scarto = -1*VAL($var[64])                        ;pezzo scarto da telecamera 3D (sopra rs5)
;
   VALUE 2:
;	  +-- gruppo 2: dati numerici
    i_diam_est = VAL($var[100])                            ;diametro esterno lamiera
    i_alt_lam = VAL($var[101])                             ;altezza lamiera
    r_tim_raff = VAL($var[104])                            ;tempo raffreddamento anello sulla griglia
    cut_param[38] = VAL($var[105])                         ;identificativo coltello da utilizzare (1/2)
    cut_param[55] = VAL($var[105])
    cut_param[72] = VAL($var[105])
    cut_param[89] = VAL($var[105])
    b_btaglio1 = -1*VAL($var[106])                         ;abilita taglio 1 (0=disabilitato 1=abilitato)
    cut_param[26] = ABS(b_btaglio1)                        ;taglio 1 abilitato [0=disabilitato  1=abilitato]
    b_btaglio2 = -1*VAL($var[107])                         ;abilita taglio 2 (0=disabilitato 1=abilitato)
    cut_param[43] = ABS(b_btaglio2)                        ;cut enable [0=disabled  1=enabled]
    b_btaglio3 = -1*VAL($var[113])                         ;abilita taglio 3 (0=disabilitato 1=abilitato)
    cut_param[60] = ABS(b_btaglio3)                        ;cut enable [0=disabled  1=enabled]
    b_btaglio4 = -1*VAL($var[114])                         ;abilita taglio 4 (0=disabilitato 1=abilitato)
    cut_param[77] = ABS(b_btaglio4)                        ;cut enable [0=disabled  1=enabled]
    i_alt_gommato = VAL($var[108])                         ;altezza anello gommato
    i_diam_gom = VAL($var[109])                            ;diametro anello gommato
    i_alt_inf = VAL($var[110])                             ;altezza stampo inferiore
    i_alt_grif = VAL($var[111])                            ;altezza griffa scarico
    i_alt_sup = VAL($var[112])                             ;altezza stampo superiore
;                                                          
    i_nscarti_avv = VAL($var[116])                         ;numero pezzi da scartare ad ogni avvio ciclo
    raggio_portacol = VAL($var[117])                       ;raggio portacoltello medio
    cut_param[18] = VAL($var[118])                         ;mm di fuoriuscita coltello da portacoltello
    cut_param[39] = VAL($var[125])                         ;attesa prima della presa da mandrino
