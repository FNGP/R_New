    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM pre_soll_lam1()
;Prelievo lamiera 1 da sollevatore
;
; +-- set tool
  CALL set_tool("tpcar")
; +-- sicura apertura pinza di carico
  CALL pinzacarapre("NOBREAK","ON","ON",0)
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #fi_soll_lam
  JMOVE #avv1_soll_lam1
; +-- verifica di avere il sollevatore alto
  CALL soll_up("BREAK","","ON",0)
  JMOVE #avv2_soll_lam1
; +-- set valori collision detection
  COLR 460,400,400,500,300,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(5,1,100,100)
  LMOVE #pre_soll_lam1
  BREAK
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
; +-- disabilitazione modo step
  CALL reset_step
; +-- se siamo in manuale
  IF SIG(x_manuale) AND NOT SIG(y_phome_run) THEN
;   +-- modifica POS6
    CALL modificapunto(&org_soll1,&tpcar,3)
;   +-- crea le nuove posizioni		
    CALL initpunti
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  ELSE
;   +-- comando chiusura pinza lamiera
    CALL pinzacarchiude("BREAK","ON","OFF",0.2)
  END
;
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all1_soll_lam1
  JMOVE #all2_soll_lam1
  JMOVE #fi_soll_lam
;
  RETURN
.END
.PROGRAM pre_soll_lam2()
;Prelievo lamiera 2 da sollevatore
;
; +-- set tool
  CALL set_tool("tpcar")
; +-- sicura apertura pinza di carico
  CALL pinzacarapre("NOBREAK","ON","ON",0)
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #fi_soll_lam
  JMOVE #avv1_soll_lam2
; +-- verifica di avere il sollevatore alto
  CALL soll_up("BREAK","","ON",0)
  JMOVE #avv2_soll_lam2
; +-- set valori collision detection
  COLR 460,400,400,500,300,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(5,1,100,100)
  LMOVE #pre_soll_lam2
  BREAK
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
; +-- disabilitazione modo step
  CALL reset_step
; +-- se siamo in manuale
  IF SIG(x_manuale) AND NOT SIG(y_phome_run) THEN
;   +-- modifica POS6
    CALL modificapunto(&org_soll2,&tpcar,4)
;   +-- crea le nuove posizioni		
    CALL initpunti
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  ELSE
;   +-- comando chiusura pinza lamiera
    CALL pinzacarchiude("BREAK","ON","OFF",0.2)
  END
;
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all1_soll_lam2
  JMOVE #all2_soll_lam2
  JMOVE #fi_soll_lam
;
  RETURN
.END
.PROGRAM prel_raff(.gruppo)
;Prelievo da raffreddatore
;
; +-- comando chiusura pinza gommato
  CALL pinzascaapre("BREAK","ON","ON",0)
  CASE .gruppo OF
   VALUE 1:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 1)
    i_muove_verso = 18
    CALL muovi_r
;   +-- presa anello da posizione 1
    CALL prel_raff1
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona1_raff
    old_gruppo = 1
;
   VALUE 2:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 1)
    i_muove_verso = 18
    CALL muovi_r
;   +-- presa anello da posizione 2
    CALL prel_raff2
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona1_raff
    old_gruppo = 2
;
   VALUE 3:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 19
    CALL muovi_r
;   +-- presa anello da posizione 3
    CALL prel_raff3
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona2_raff
    old_gruppo = 3
;
   VALUE 4:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 19
    CALL muovi_r
;   +-- presa anello da posizione 4  
    CALL prel_raff4
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona2_raff
    old_gruppo = 4
;
   VALUE 5:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 19
    CALL muovi_r
;   +-- presa anello da posizione 5
    CALL prel_raff5
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona2_raff
    old_gruppo = 5
;
   VALUE 6:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 19
    CALL muovi_r
;   +-- presa anello da posizione 6
    CALL prel_raff6
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona2_raff
;   +-- set old gruppo
    old_gruppo = 0
;   +-- se lavoro con piu di 6 posizioni
    IF ngruppi_vis>6 THEN
;     +-- set old gruppo
      old_gruppo = 6
    END
;
   VALUE 7:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 1)
    i_muove_verso = 18
    CALL muovi_r
;   +-- presa anello da posizione 7
    CALL prel_raff7
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona1_raff
    old_gruppo = 7
;
   VALUE 8:
;   +-- porta il robot in posizione (PRELIEVO RAFFREDDAMENTO ZONA 2)
    i_muove_verso = 19
    CALL muovi_r
;   +-- presa anello da posizione 8
    CALL prel_raff8
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #fi_pzona2_raff
    old_gruppo = 0
   ANY :
;
    DREG (7) = 195
    HALT
  END
; +-- set stato posizione ".gruppo", visione
  vis_stato_pos[.gruppo] = 0
; +-- set stato posizione ".gruppo", robot
  rob_stato_pos[.gruppo] = 0
;
  RETURN
.END
.PROGRAM prel_raff1()
;Presa anello 1 da griglia di raffreddamento
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #avv_praff[1]
  BREAK
; +-- set valori collision detection
  COLR 460,400,400,500,100,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(10,1,100,100)
  LMOVE #ppraff[1]
  BREAK
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
; +-- reset_step
  CALL reset_step
  IF NOT SIG(x_manuale) OR SIG(x_man_tag) OR SIG(x_man_scar) THEN
;   +-- comando chiusura pinza gommato
    CALL pinzascachiude("BREAK","ON","ON",0)
  END
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
  CALL reset_step
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all_praff[1]
;
  RETURN
.END
.PROGRAM prel_raff2()
;Presa anello 2 da griglia di raffreddamento
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #avv_praff[2]
  BREAK
; +-- set valori collision detection
  COLR 460,400,400,500,100,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(10,1,100,100)
  LMOVE #ppraff[2]
  BREAK
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
; +-- reset_step		
  CALL reset_step
  IF NOT SIG(x_manuale) OR SIG(x_man_tag) OR SIG(x_man_scar) THEN
;   +-- comando chiusura pinza gommato
    CALL pinzascachiude("BREAK","ON","ON",0)
  END
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
  CALL reset_step
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all_praff[2]
;
  RETURN
.END
.PROGRAM prel_raff3()
;Presa anello 3 da griglia di raffreddamento
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #avv_praff[3]
; +-- set valori collision detection
  COLR 460,400,400,500,100,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(10,1,100,100)
  LMOVE #ppraff[3]
  BREAK
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
; +-- reset_step		
  CALL reset_step
  IF NOT SIG(x_manuale) OR SIG(x_man_tag) OR SIG(x_man_scar) THEN
;   +-- comando chiusura pinza gommato
    CALL pinzascachiude("BREAK","ON","ON",0)
  END
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
  CALL reset_step
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all_praff[3]
;
  RETURN
.END
.PROGRAM prel_raff4()
;Presa anello 4 da griglia di raffreddamento
;
  CALL mov_par(speed_fp,300,100,100)
  JMOVE #avv_praff[4]
; +-- set valori collision detection
  COLR 460,400,400,500,100,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(10,1,100,100)
  LMOVE #ppraff[4]
  BREAK
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
; +-- reset_step		
  CALL reset_step
  IF NOT SIG(x_manuale) OR SIG(x_man_tag) OR SIG(x_man_scar) THEN
;   +-- comando chiusura pinza gommato
    CALL pinzascachiude("BREAK","ON","ON",0)
  END
  IF SIG(x_manuale) AND NOT SIG(x_man_tag) AND NOT SIG(x_man_scar) THEN
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
  CALL reset_step
  CALL mov_par(speed_fp,300,100,100)
  LMOVE #all_praff[4]
;
  RETURN
.END
