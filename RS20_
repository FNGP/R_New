.PROGRAM lsqpg()
  LSQ FST
  LSQ OUT,&M0700
  LSQ STR,&M0700
  LSQ WMOVR,0,&D0000
  LSQ WMOVR,0,&D000A
  LSQ WMOVR,0,&D0004
  LSQ WMOVR,0,&D0005
  LSQ WMOVR,0,&D0010
  LSQ WMOVR,0,&D0011
  LSQ WMOVR,0,&D0012
  LSQ WMOVR,0,&D0013
  LSQ WMOVR,0,&D0014
  LSQ WMOVR,0,&D0015
  LSQ WMOVR,0,&D0016
  LSQ WMOVR,0,&D0017
  LSQ WMOVR,0,&D0018
  LSQ WMOVR,0,&D0019
  LSQ WMOVR,0,&D001A
