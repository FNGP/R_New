.PROGRAM depo_scarto(.$id_scarto)
;Deposito pezzo scarto
;
  SCASE .$id_scarto OF
   SVALUE "GOMMATO":
;   +-- porta il robot in posizione (DEPOSITO PEZZI SCARTO (pinza gommato))
    i_muove_verso = 24
    CALL muovi_r
;   +-- set tool
    CALL set_tool("tpscar")
;   +-- set valori collision detection
    COLR 460,400,400,500,150,500
    COLRJ 10,10,20,30,10,30
    CALL mov_par(speed_fp,300,100,100)
    LMOVE #scarto_gom
    MVWAIT 15 MM
;   +-- comando apertura pinza gommato
    CALL pinzascaapre("BREAK","ON","ON",0)
    IF NOT SIG(x_pcod_giu) THEN
;     +-- discesa pinza codolo
      CALL pinzacodgiu("BREAK","ON","ON",0)
    END
;
    LMOVE #fi_scarto_gom
;   +-- set valori collision detection
    COLR 500,500,500,500,500,500
    COLRJ 50,50,50,50,50,50
;
   SVALUE "LAMIERA":
;   +-- porta il robot in posizione (DEPOSITO PEZZI SCARTO (pinza lamiera))
    i_muove_verso = 23
    CALL muovi_r
;   +-- set tool
    CALL set_tool("tpcar")
;   +-- set valori collision detection
    COLR 460,400,400,500,150,500
    COLRJ 10,10,20,30,10,30
    CALL mov_par(speed_fp,300,100,100)
    LMOVE #scarto_lam
    MVWAIT 15 MM
;   +-- comando apertura pinza lamiera
    CALL pinzacarapre("BREAK","ON","ON",0)
    IF NOT SIG(x_pcod_giu) THEN
;     +-- discesa pinza codolo
      CALL pinzacodgiu("BREAK","ON","ON",0)
    END
;
    LMOVE #fi_scarto_lam
;   +-- set valori collision detection
    COLR 500,500,500,500,500,500
    COLRJ 50,50,50,50,50,50
;
   SVALUE "PTFE":
;   +-- porta il robot in posizione (DEPOSITO PEZZI SCARTO (pinza ptfe))
    i_muove_verso = 26
    CALL muovi_r
;   +-- set tool
    CALL set_tool("tpcar")
;   +-- set valori collision detection
    COLR 460,400,400,500,150,500
    COLRJ 10,10,20,30,10,30
    CALL mov_par(speed_fp,300,100,100)
    LMOVE #scarto_ptfe
    MVWAIT 15 MM
;   +-- comando vuoto off
    CALL vuoto_off("BREAK","ON","ON",0)
;
    LMOVE #fi_scarto_ptfe
;   +-- set valori collision detection
    COLR 500,500,500,500,500,500
    COLRJ 50,50,50,50,50,50
; 
   SVALUE "CAPPUCCIO":
;   +-- verifica di avere il sollevatore basso
    IF NOT SIG(x_soll_dw) THEN
      CALL soll_dw("BREAK","ON","ON",0)
    END
;   +-- Aspetta il segnale di fuori ingombro dall'RS5
    IF NOT SIG(x_fi_capp,x_rs5_ws) THEN
      UTIMER .@r_tim_wait = 0
      WHILE NOT SIG(x_fi_capp,x_rs5_ws) DO
        IF UTIMER(.@r_tim_wait)>20 THEN
          DREG (7) = 276
          CALL prosegui
          UTIMER .@r_tim_wait = 0
        END
        TWAIT 0.05
      END
    END
;   +-- porta il robot in posizione (DEPOSITO PEZZI SCARTO (pinza cappuccio))
    i_muove_verso = 27
    CALL muovi_r
;   +-- Reset fuori ingombro per RS5
;    MVWAIT 5 MM
    SIGNAL -y_mand_fi
;
;   +-- set valori collision detection
    COLR 460,400,400,500,150,500
    COLRJ 10,10,20,30,10,30
    CALL mov_par(speed_fp,300,100,100)
    JMOVE #scarto_cap
;
    IF SIG(x_manuale) AND NOT (SIG(y_phome_run) OR SIG(x_man_scar)) THEN
;       +-- reset step
      CALL reset_step
;       +-- attesa consenso a step successivo
      WAIT SIG(x_step)
;       +-- reset step
      CALL reset_step
    ELSE
;
;     +-- comando apertura/salita pinza cappuccio
      CALL pinzacodgiu("BREAK","ON","ON",0)
      CALL pinzacodapre("BREAK","ON","ON",0)
      CALL pinzacodsu("BREAK","ON","ON",0)
      CALL pinzacodgiu("BREAK","ON","ON",0)
    END
;
    JMOVE #fi_scarto_cap
;   +-- Set fuori ingombro per RS5	
    MVWAIT 5 MM
    SIGNAL y_mand_fi
;   +-- set valori collision detection
    COLR 500,500,500,500,500,500
    COLRJ 50,50,50,50,50,50
   ANY :
;
    DREG (7) = 338
    CALL prosegui
  END
;  
  RETURN
.END
.PROGRAM deposito()
;Deposito pezzo
;
; +-- porta il robot in posizione (DEPOSITO SU NASTRO)
  i_muove_verso = 4
  CALL muovi_r
; +-- controllo nastro di deposito libero
  CALL chk_depo_free
; +-- set valori collision detection
  COLR 460,400,400,500,300,500
  COLRJ 10,10,20,30,20,30
  CALL mov_par(speed_fp,300,100,100)
; +-- deposito
  DECEL 50
  LMOVE #pnastrodep
; +-- se non sono in manuale
  IF NOT SIG(x_manuale) OR SIG(x_man_scar) OR SIG(y_phome_run) OR SIG(x_man_tag) OR SIG(x_test_cam) THEN
;   +-- comando apertura pinza di scarico
    CALL pinzascaapre("BREAK","ON","ON",0)
  ELSE
;
;   +-- reset step
    CALL reset_step
;   +-- attesa consenso a step successivo
    WAIT SIG(x_step)
;   +-- reset step
    CALL reset_step
  END
;
  IF NOT SIG(x_pcod_giu) THEN
; +-- discesa pinza codolo
    CALL pinzacodgiu("BREAK","ON","ON",0)
  END
; +-- allontanamento
  ACCEL 50
; +-- set valori collision detection
  COLR 500,500,500,500,500,500
  COLRJ 50,50,50,50,50,50
  LMOVE #fi_deposito
; +-- start nastro di scarico  
  PULSE y_nsca_start,1
;
  RETURN
.END
.PROGRAM initpunti()
;Crea le posizioni di lavoro
;
  h_ingresso_sca = 45
  h_ingresso_car = 70
  POINT .tool_att = TOOL
;
; +-- POSIZIONE FUORI PRESSA
  CALL set_tool("tpscar")
  POINT pprespresa = SHIFT(org_prespresa BY ny[1]+(i_alt_inf-naltezzastampon)+(i_alt_grif-naltpinzascan),-nz[1],-nx[1])
  POINT #fi_pressa = SHIFT(pprespresa BY h_ingresso_sca,0,720),#fi_pressa
;
;
; +-- POS1: SCARICO PRESSA
  POINT #avv_prpresa_01 = SHIFT(pprespresa BY h_ingresso_sca,0,470),#fi_pressa
  POINT #avv_prpresa_02 = SHIFT(pprespresa BY h_ingresso_sca,0,0),#fi_pressa
  POINT #pprespresa = pprespresa,#fi_pressa
  POINT #all_prpresa_01 = SHIFT(pprespresa BY h_ingresso_sca,0,0),#fi_pressa
  POINT #all_prpresa_02 = SHIFT(pprespresa BY h_ingresso_sca,0,720),#fi_pressa ;solo in scarico completo
;
;
; +-- POS2: CARICO PRESSA
  CALL set_tool("tpcar")
  POINT ppresdep = SHIFT(org_presdep BY ny[2]+(i_alt_lam-naltezzan)+(i_alt_inf-naltezzastampon),-nz[2],-nx[2])
  POINT #avv_prdep_01 = SHIFT(ppresdep BY h_ingresso_car,0,470),#fi_pressa     ;solo in carico singolo
;
  POINT #avv_prdep_02 = SHIFT(ppresdep BY h_ingresso_car,0,0),#fi_pressa
  POINT #ppresdep = ppresdep,#fi_pressa
  POINT #all_prdep_01 = SHIFT(ppresdep BY h_ingresso_car,0,0),#fi_pressa
  POINT #all_prdep_02 = SHIFT(ppresdep BY h_ingresso_car,0,600),#fi_pressa
;
;
  CALL set_tool("tpcar")
; +-- POS3: presa lamiera 1 da sollevatore
  POINT #avv1_soll_lam1 = f_soll+SHIFT(org_soll1 BY -nz[3],-nx[3]+((i_diam_est-ndiametron)/(2*SIN(60))),ny[3]+(i_alt_lam-naltezzan)+100),#fi_soll_lam
  POINT #avv2_soll_lam1 = f_soll+SHIFT(org_soll1 BY -nz[3],-nx[3]+((i_diam_est-ndiametron)/(2*SIN(60))),ny[3]+(i_alt_lam-naltezzan)+20),#fi_soll_lam
  POINT #all1_soll_lam1 = f_soll+SHIFT(org_soll1 BY -nz[3],-nx[3]+((i_diam_est-ndiametron)/(2*SIN(60))+2),ny[3]+(i_alt_lam-naltezzan)+20),#fi_soll_lam
  POINT #all2_soll_lam1 = f_soll+SHIFT(org_soll1 BY -nz[3],-nx[3]+((i_diam_est-ndiametron)/(2*SIN(60))+2),ny[3]+(i_alt_lam-naltezzan)+100),#fi_soll_lam
  POINT #pre_soll_lam1 = f_soll+SHIFT(org_soll1 BY -nz[3],-nx[3]+((i_diam_est-ndiametron)/(2*SIN(60))),ny[3]+(i_alt_lam-naltezzan)),#fi_soll_lam
; +-- POS4: presa lamiera 2 da sollevatore
  POINT #avv1_soll_lam2 = f_soll+SHIFT(org_soll2 BY -nz[4],-nx[4]+((i_diam_est-ndiametron)/(2*SIN(60))),ny[4]+(i_alt_lam-naltezzan)+100),#fi_soll_lam
  POINT #avv2_soll_lam2 = f_soll+SHIFT(org_soll2 BY -nz[4],-nx[4]+((i_diam_est-ndiametron)/(2*SIN(60))),ny[4]+(i_alt_lam-naltezzan)+20),#fi_soll_lam
  POINT #all1_soll_lam2 = f_soll+SHIFT(org_soll2 BY -nz[4],-nx[4]+((i_diam_est-ndiametron)/(2*SIN(60))+2),ny[4]+(i_alt_lam-naltezzan)+20),#fi_soll_lam
  POINT #all2_soll_lam2 = f_soll+SHIFT(org_soll2 BY -nz[4],-nx[4]+((i_diam_est-ndiametron)/(2*SIN(60))+2),ny[4]+(i_alt_lam-naltezzan)+100),#fi_soll_lam
  POINT #pre_soll_lam2 = f_soll+SHIFT(org_soll2 BY -nz[4],-nx[4]+((i_diam_est-ndiametron)/(2*SIN(60))),ny[4]+(i_alt_lam-naltezzan)),#fi_soll_lam
;
;
  CALL set_tool("tpscar")
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 1
  POINT f_raff+.r = #drif[1]
  POINT #avv_draff[1] = f_raff+SHIFT(.r BY 0,12.5,70+(i_alt_grif-naltpinzascan)),#drif[1]
  POINT #ddraff[1] = f_raff+SHIFT(.r BY 0,12.5,(i_alt_grif-naltpinzascan)-3),#drif[1]
  POINT #all_draff[1] = f_raff+SHIFT(.r BY 0,12.5,70+(i_alt_grif-naltpinzascan)),#drif[1]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 2
  POINT f_raff+.r = #drif[2]
  POINT #avv_draff[2] = f_raff+SHIFT(.r BY 0,12.5,70+(i_alt_grif-naltpinzascan)),#drif[2]
  POINT #ddraff[2] = f_raff+SHIFT(.r BY 0,12.5,(i_alt_grif-naltpinzascan)-3),#drif[2]
  POINT #all_draff[2] = f_raff+SHIFT(.r BY 12.5,0,70+(i_alt_grif-naltpinzascan)),#drif[2]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 3
  POINT f_raff+.r = #drif[3]
  POINT #avv_draff[3] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[3]
  POINT #ddraff[3] = f_raff+SHIFT(.r BY 0,-12.5,(i_alt_grif-naltpinzascan)-3),#drif[3]
  POINT #all_draff[3] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[3]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 4
  POINT f_raff+.r = #drif[4]
  POINT #avv_draff[4] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[4]
  POINT #ddraff[4] = f_raff+SHIFT(.r BY 0,-12.5,(i_alt_grif-naltpinzascan)-3),#drif[4]
  POINT #all_draff[4] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[4]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 5
  POINT f_raff+.r = #drif[5]
  POINT #avv_draff[5] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[5]
  POINT #ddraff[5] = f_raff+SHIFT(.r BY 0,-12.5,(i_alt_grif-naltpinzascan)-3),#drif[5]
  POINT #all_draff[5] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[5]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 6
  POINT f_raff+.r = #drif[6]
  POINT #avv_draff[6] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[6]
  POINT #ddraff[6] = f_raff+SHIFT(.r BY 0,-12.5,(i_alt_grif-naltpinzascan)-3),#drif[6]
  POINT #all_draff[6] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[6]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 7
  POINT f_raff+.r = #drif[7]
  POINT #avv_draff[7] = f_raff+SHIFT(.r BY 0,12.5,70+(i_alt_grif-naltpinzascan)),#drif[7]
  POINT #ddraff[7] = f_raff+SHIFT(.r BY 0,12.5,(i_alt_grif-naltpinzascan)-3),#drif[7]
  POINT #all_draff[7] = f_raff+SHIFT(.r BY 0,12.5,70+(i_alt_grif-naltpinzascan)),#drif[7]
; +-- DEPOSITO SU GRIGLIA DI RAFF. POSIZIONE 8
  POINT f_raff+.r = #drif[8]
  POINT #avv_draff[8] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[8]
  POINT #ddraff[8] = f_raff+SHIFT(.r BY 0,-12.5,(i_alt_grif-naltpinzascan)-3),#drif[8]
  POINT #all_draff[8] = f_raff+SHIFT(.r BY 0,-12.5,70+(i_alt_grif-naltpinzascan)),#drif[8]
  TOOL .tool_att
;
  RETURN
.END
.PROGRAM initpunti_cam()
;Modifica punti prelievo griglia raff. dopo acq da telecamera
;
; +-- se telecamera pronta
  IF b_cam1_ready THEN
;   +-- reset max foto per incongruenza stato posizioni
    .b_max_foto = FALSE
;   +-- se sistema di visione attivo
    IF b_cam1_on THEN
;     +-- attesa dati pronti da telecamera
      UTIMER .@r_tim_wait = 0
      .i_nfoto = 1
      b_nfoto_max = FALSE
;     +-- attesa dati pronti ed acquisiti
      WHILE NOT SIG(m_cam1_dati,m_ok_gr3) DO
        IF UTIMER(.@r_tim_wait)>20 THEN
;
nuova_foto:
;         +-- set speed_fp per griglia piena
          b_speed_fp02 = TRUE
;         +-- set apertura sportello telecamera/accensione luci
          CALL tele_on
;         +-- porta il robot in posizione (FUORI-INGOMBRO PER FOTO A GRIGLIA DI RAFF.)
          i_muove_verso = 20
          CALL muovi_r
          BREAK
;         +-- start acquisizione immagine se richiesto
          PULSE y_cam1_acq,1
          b_cam1_acq = FALSE
;         +-- attesa fine acquisizione immagine
          UTIMER .@r_tim_wait = 0
          WAIT b_cam1_busy OR UTIMER(.@r_tim_wait)>5
          IF b_cam1_busy THEN
            UTIMER .@r_tim_wait = 0
            WAIT b_cam1_endacq AND NOT b_cam1_busy OR UTIMER(.@r_tim_wait)>5
          END
          UTIMER .@r_tim_wait = 0
;         +-- set chiusura sportello telecamera/spegnimento luci
          CALL tele_off
          .i_nfoto = .i_nfoto+1
        END
;       +-- max tentativi acquisizione immagine
        IF .i_nfoto=>3 THEN
          DREG (7) = 264
;         +-- set max foto eseguite
          b_nfoto_max = TRUE
;         +-- porta il robot in posizione (HOME)
          i_muove_verso = 1
          CALL muovi_r
;         +-- se siamo in manuale a ciclo step
          IF SIG(x_manuale,x_step) THEN
;           +-- reset step
            CALL reset_step
          ELSE
;
;                          +-------- set fine ciclo
;                          |    +--- attesa fine ciclo selezionato
            CALL set_fine("ON","ON")
          END
          RETURN
        END
        TWAIT 0.05
      END
    END
;   +-- set dati acquisiti a telecamera
    SIGNAL m_ok_read_cam1,-m_ok_gr3
;
    CALL set_tool("tpscar")
;                      +------------------------------- indice posizione
;                      | +----------------------------- posizione di riferimento
;                      | |          +----------------- offset X
;                      | |          | +----------- offset Y
;                      | |          | | +--------- ID pinza
;                      | |          | | | +------- ID orientamento pinza (0-180 gradi)
;                      | |          | | | |
    CALL calc_pre_raff(1,&org_prif0a,0,0,1,0);        gruppo 1
    CALL calc_pre_raff(2,&org_prif0a,0,0,1,0);        gruppo 2
    CALL calc_pre_raff(3,&org_prif180a,0,0,1,180);    gruppo 3
    CALL calc_pre_raff(4,&org_prif180a,0,0,1,180);    gruppo 4
    CALL calc_pre_raff(5,&org_prif180a,0,0,1,180);    gruppo 5
    CALL calc_pre_raff(6,&org_prif180a,0,0,1,180);    gruppo 6
    CALL calc_pre_raff(7,&org_prif0a,0,0,1,0);        gruppo 7
    CALL calc_pre_raff(8,&org_prif180a,0,0,1,180);    gruppo 8
;
;   +-- se prima foto da visione, prende in carico lo stato delle posizioni griglia
    IF b_prima_foto THEN
;     +-- verifica coerenza presenza anelli/scarti
      FOR .i = 1 TO i_npos_vis
        rob_stato_pos[.i] = vis_stato_pos[.i]
        IF vis_stato_pos[.i]<>1 AND gruppo_scarto[.i] THEN
          gruppo_scarto[.i] = FALSE
        END
      END
;                    +-- reset prima foto da visione
      b_prima_foto = FALSE
    END
;
;   +-- reset max posizioni non agibili
    .pos_ko = 0
;   +-- reset incongruenza stato posizioni robot/visione
    .stato_pos_ko = FALSE
    FOR .i = 1 TO i_npos_vis
;                           +-- posizione non agibile da visione
      IF vis_stato_pos[.i]==2 THEN
;       +-- incremento le posizioni non agibili
        .pos_ko = .pos_ko+1
      END
;        +--------------------- stato posizione da visione
;        |                  +-- stato posizione da robot
      IF vis_stato_pos[.i]<>rob_stato_pos[.i] THEN
;                       +-- set incongruenza posizione
        .stato_pos_ko = TRUE
      END
    END
;   +-- max posizioni non agibili superate
    IF .pos_ko=>2 THEN
      DREG (7) = 38
    END
;   +-- incongruenza stato posizioni griglia
    IF .stato_pos_ko THEN
;     +-- se max foto non raggiunto, esegue una nuova foto
      IF NOT .b_max_foto THEN
;                     +-- set max foto raggiunto
        .b_max_foto = TRUE
;                      +-- set prima foto per prendere in carico lo stato delle posizioni
        b_prima_foto = TRUE
;       +-- riesegue una nuova foto
        GOTO nuova_foto
      END
      DREG (7) = 39
    END
  END
; +-- set chiusura sportello telecamera/spegnimento luci
  CALL tele_off
;	
  RETURN
.END
.PROGRAM inizializza()
;Controlli iniziali
;
; +-- inizializzazione in/out
  CALL set_io
;
; +-- inizializzazione variabili
  CALL set_var
;
; +-- richiamo muovi home
  CALL muovihome
  i_muove_da = 1
;
; +-- controllo pc1 in run
  CALL chk_pc1_run
;
; +-- definizione base,tool,frame
  CALL origini
;
; +-- crea le nuove posizioni
  CALL initpunti
;
  RETURN
;
;
  HALT
  CALL aaa_align
  CALL aaa_muovihome
  CALL aaa_sw_version
  CALL arresta
  CALL autostart3.pc
  CALL autostart5.pc
  CALL lsqpg
  CALL muove_da14_a1
