.PROGRAM pc3_set_gruppi()
;Setta gli estremi dei gruppi di parametri da trasmettere
;
  tcp_gruppo[1] = 1
  tcp_gruppo[2] = 100
  tcp_gruppo[3] = 250
  tcp_gruppo[4] = 350
  tcp_gruppo[5] = 590
  tcp_gruppo[6] = 800
  tcp_gruppo[7] = 1100
  tcp_gruppo[8] = 1150
  tcp_gruppo[9] = 1250
;
  RETURN
.END
.PROGRAM pc3_tcp_client()
;Gestione client TCP
;
; +-- eventuale tempo di attesa al riavvio della comunicazione
  IF st_conn_sv==-2 THEN
    UTIMER @t_tcp_sv = 0
    st_conn_sv = -1
  END
  IF st_conn_sv==-1 AND UTIMER(@t_tcp_sv)>5 THEN
    st_conn_sv = 0
  END
;
; +-- apertura socket
  IF st_conn_sv==0 AND sk_id_sv<=0 AND tcp_enabled==ON THEN
    TCP_CONNECT res_sv,port_sv,addr_sv[1],0.5
    IF res_sv>0 THEN
      sk_id_sv = res_sv
      st_conn_sv = 2
      UTIMER @t_tcp_sv = 0
    ELSE
;
      st_conn_sv = -2
    END
  END
;
; +-- ricezione (RECEIVE)  
  IF st_conn_sv==2 THEN
;   +-- ricezione del pacchetto (anche in parti separate)
    .r = -1
    .exit_do = 0
    DO
      TCP_RECV res_sv,sk_id_sv,$rcv_raw[0],n_raw,20,40
      IF res_sv<0 THEN
        .exit_do = 1
      ELSE
;
;       +-- scansione delle linee ricevute
        FOR .i = 0 TO n_raw-1
          IF INSTR(1 ,$rcv_raw[.i] , "DISCONNECT")>0 THEN
            .exit_do = 2
            .i = n_raw+10 ;exit_for
          ELSE
;
            IF INSTR(1 ,$rcv_raw[.i] , "<EOP>")>0 THEN
              .exit_do = 3
              .i = n_raw+10
;
            ELSE
              .r = .r+1
              $rcvbf_sv[.r] = $rcv_raw[.i]
;
            END
          END
        END
      END
    UNTIL .exit_do>0
    exit_do = .exit_do
;
    CASE .exit_do OF
     VALUE 3:
;     +-- ricezione completata correttamente (<EOP> = End_Of_Packet)
      rcvbf_sv_len = .r+1
      CALL pc3_tcp_recv;  decodifica $rcvbf_sv[]
      st_conn_sv = 3
      UTIMER @t_tcp_sv = 0
;
     VALUE 1:
;     +-- errore in RX
      IF res_sv==-34024 THEN
;
      ELSE
;
        st_conn_sv = -102
      END
;
     ANY :
;
      CALL pc3_tcp_disconn
    END
  END
;
; +-- trasmissione (SEND)
  IF st_conn_sv==3 AND tcp_enabled==ON AND UTIMER(@t_tcp_sv)>0.02 THEN
    CALL pc3_tcp_send;  costruisce $sndbf_sv[0] e sndbf_sv_len
    TCP_SEND res_sv,sk_id_sv,$sndbf_sv[0],sndbf_sv_len,5
    IF res_sv=>0 THEN
      st_conn_sv = 2
    ELSE
;
      IF res_sv==-34024 THEN
;       +-- time-out tasmissione
      ELSE
;
        st_conn_sv = -103
      END
    END
  END
;
; +-- controllo dei time-out
  IF st_conn_sv==2 AND UTIMER(@t_tcp_sv)>6 THEN
    CALL pc3_tcp_disconn
  END
;
  IF st_conn_sv==-102 OR st_conn_sv==-103 THEN
    CALL pc3_tcp_disconn
  END
;
; +-- disattivazione manuale TCP
  IF tcp_enabled==OFF AND st_conn_sv>0 THEN
    $sndbf_sv[0] = "DISCONNECT"+$CHR(13)+$CHR(10)
    TCP_SEND res_sv,sk_id_sv,$sndbf_sv[0],1,1
    CALL pc3_tcp_disconn
  END
;
  RETURN
;
; st_conn_sv: 0 = nessuna connessione attiva, tentativo di CONNECT
;             1 = non usato in client-mode
;             2 = connesso... ricezione
;             3 = connesso... trasmissione
;          -102 = errore generico in ricezione
;          -103 = errore generico in trasmissione
.END
.PROGRAM pc3_tcp_decode(.n_elem)
;Decodifica stringa parametri
;
; +-- scansione delle linee ricevute
  FOR .i = 0 TO .n_elem-1
    DO
      .$rw[.i] = $DECODE($rcvbf_sv[.i],"|",0)                 ; R o W
      .$separatore = $DECODE($rcvbf_sv[.i],"|",1)             ; |
      IF .$rw[.i]=="R" THEN
;       +-- memorizza quale gruppo di parametri verra' trasmesso
        tcp_gruppo_da_t = VAL($DECODE($rcvbf_sv[.i],"|",0))   ; 1
        .$separatore = $DECODE($rcvbf_sv[.i],"|",1)           ; |
        .$chiusura = $LEFT($rcvbf_sv[.i],1)                   ; # o W (non rimosso)
      ELSE
;
;       +-- recupera il parametro da sovrascrivere
        .indice = VAL($DECODE($rcvbf_sv[.i],"_",0))           ; 68
        .$underline = $DECODE($rcvbf_sv[.i],"_",1)            ; |
        .$valore = $DECODE($rcvbf_sv[.i],"|",0)               ; 118.35
        .$separatore = $DECODE($rcvbf_sv[.i],"|",1)           ; |
        .$chiusura = $LEFT($rcvbf_sv[.i],1)                   ; # o W (non rimosso)
;       +-- sovrascrive il parametro richiesto
        $var[.indice] = .$valore
      END
      .$ch = $LEFT(.$chiusura,1)
      IF debug_t THEN
        TWAIT 0.002
      END
    UNTIL .$ch=="#"
  END
;
  RETURN
.END
.PROGRAM pc3_tcp_disconn()
;Disconnessione del socket TCP
;
; +-- verifica lo stato del TCP
  TCP_STATUS .i_res,.i_port[1],.i_sok_id[1],.i_sok_err[1],.i_sok_sub[1],$ip_addr[1]
;
; +-- cerca la porta di comunicazione con il PC
  FOR .p = 1 TO .i_res
;   +-- porta 50001 identificata
    IF .i_port[.p]==port_sv THEN
;     +-- se ho un socket aperto legato alla 50001 lo chiudo
      IF .i_sok_id[.p]<>0 THEN
        .try = 0
        DO
          .try = .try+1
          TCP_CLOSE res_sv,.i_sok_id[.p]
          TWAIT 0.5
        UNTIL res_sv==0 OR .try>4
        TWAIT 1
;
        IF res_sv<0 THEN
          st_conn_sv = -103
        ELSE
;
          sk_id_sv = 0
          st_conn_sv = -2
        END
      ELSE
;
        sk_id_sv = 0
        st_conn_sv = -2
      END
    END
  END
;
  RETURN
.END
.PROGRAM pc3_tcp_encode(.n_elem)
;Costruisce le stringhe da inviare (in risposta al server)
;
  .max_str = 1
  .max_char = 220
;
; +-- elemento iniziale da trasmettere
  .el_i = tcp_gruppo[tcp_gruppo_da_t]
; +-- elemento finale da trasmettere
  .el_f = tcp_gruppo[tcp_gruppo_da_t+1]-1
;
  .str = 0
  $sndbf_sv[.str] = ""
  FOR .el = .el_i TO .el_f
    $sndbf_sv[.str] = $sndbf_sv[.str]+$ENCODE(/F0.0,.el)+"_"+$var[.el]+"|"
    IF LEN($sndbf_sv[.str])>.max_char AND .el<.el_f THEN
      $sndbf_sv[.str] = $sndbf_sv[.str]+$CHR(13)+$CHR(10)
      .str = .str+1
      $sndbf_sv[.str] = ""
      .max_str = .str
    END
    IF debug_t THEN
      TWAIT 0.002
    END
  END
  $sndbf_sv[.max_str] = $sndbf_sv[.max_str]+$CHR(13)+$CHR(10)
  $sndbf_sv[.max_str+1] = "<EOP>"+$CHR(13)+$CHR(10)
  .n_elem = .max_str+2
;
  RETURN
.END
.PROGRAM pc3_tcp_recv()
;Decodifica le stringhe ricevite (dal server)
;
  CALL pc3_tcp_decode(rcvbf_sv_len)
  IF i_arburg_recv==0 THEN
    CALL pc3_read_var
  END
;
  RETURN
.END
.PROGRAM pc3_tcp_send()
;Costruisce le stringhe da inviare (in richiesta al server)
;
  CALL pc3_write_var
  CALL pc3_tcp_encode(sndbf_sv_len)
;
  RETURN
.END
.PROGRAM pc3_write_var()
;Scrittura variabili a pannello
;
; da 1 a 29 stato macchina         (gruppo 1)
; da 30 a 99 booleani              (gruppo 1)
; da 100 a 249 dati numerici       (gruppo 2)
; da 250 a 349 dati X Y visione    (gruppo 3)
; da 350 a 589 INPUT               (gruppo 4)
; da 590 a 799 OUT                 (gruppo 5)
; da 800 a 1099 anomalie           (gruppo 6)
; da 1100 a 1149 storico           (gruppo 7)
; da 1150 a 1249 parametri visione (gruppo 8)
;
  CASE tcp_gruppo_da_t OF
   VALUE 1:
;	  +-- gruppo 1: stato macchina, booleane
    $var[1] = $stato_macc                                  ;stato macchina
    $var[2] = $DATE(2)                                     ;data
    $var[3] = $TIME                                        ;ora
    $var[4] = $ENCODE(/F0.0,i_time_frq_sca)                ;tempo freq. scarico pressa
    $var[5] = $ENCODE(/F0.0,i_pezzi_parz)                  ;totale pezzi prelevati da pressa (azzerabile)
    $var[6] = $ENCODE(/F0.0,i_time_auto)                   ;tempo totale macchina in automatico
    $var[7] = $ENCODE(/F0.0,i_time_on)                     ;tempo totale macchina accesa
    $var[8] = $ENCODE(/F0.2,i_time_pressa)                 ;tempo ciclo macchina da stampo chiuso a stampo chiuso
    $var[9] = $ENCODE(/F0.0,i_pezzi_tot)                   ;totale pezzi prelevati da pressa
    $var[10] = $ENCODE(/F0.1,r_temp_nx)                    ;offset temporaneo X
    $var[11] = $ENCODE(/F0.1,r_temp_ny)                    ;offset temporaneo Y
    $var[12] = $ENCODE(/F0.1,r_temp_nz)                    ;offset temporaneo Z
    $var[13] = $ENCODE(/F0.1,cut_param[9])                 ;numero tagli coltello 1
    $var[14] = $ENCODE(/F0.1,cut_param[10])                ;numero tagli coltello 2
    $var[15] = $ENCODE(/F0.1,0)                            ;disp.
    $var[16] = $ENCODE(/F0.1,i_utcolt1)                    ;% utilizzo coltello 1
    $var[17] = $ENCODE(/F0.1,i_utcolt2)                    ;% utilizzo coltello 2
    $var[18] = $ENCODE(/F0.1,0)                            ;disp.
    $var[19] = $ENCODE(/F0.1,i_allarme)                    ;anomalia da robot
    $var[20] = $ENCODE(/F0.1,cut_param[23])                ;tempo ciclo RS5
    $var[21] = $ENCODE(/F0.1,0)                            ;disp.
    $var[22] = $ENCODE(/F0.0,rob_stato_pin[1])             ;stato pinza lamiera
    $var[23] = $ENCODE(/F0.0,rob_stato_pin[2])             ;stato pinza gommato
    $var[24] = $ENCODE(/F0.0,0)                            ;disp.
    $var[25] = $ENCODE(/F0.0,0)                            ;disp.
    $var[26] = $ENCODE(/F0.0,0)                            ;disp.
    $var[27] = $ENCODE(/F0.0,0)                            ;disp.
    $var[28] = $ENCODE(/F0.0,0)                            ;disp.
    $var[29] = $ENCODE(/F0.0,0)                            ;disp.
    $var[30] = $ENCODE(/F0.0,ABS(b_cam1_ok_acq))           ;elaborazione immagine terminata da telecamera 1
    $var[31] = $ENCODE(/F0.0,ABS(b_cam1_dati))             ;dati pronti da telecamera
    $var[32] = $ENCODE(/F0.0,ABS(b_cam1_ready))            ;telecamera 1 pronta
    $var[33] = $ENCODE(/F0.0,ABS(b_cam1_busy))             ;telecamera 1 in fase di elaborazione
    $var[34] = $ENCODE(/F0.0,ABS(b_cam1_endacq))           ;acquisizione immagine terminata da telecamera 1
    $var[35] = $ENCODE(/F0.0,i_indice_trasm)               ;indice trasmissione (non utilizzato dal robot)
    $var[36] = $ENCODE(/F0.0,ABS(b_teach))                 ;abilita movimenti da pannello
    $var[37] = $ENCODE(/F0.0,ABS(b_conf))                  ;conferma valori posizione
    $var[38] = $ENCODE(/F0.0,ABS(b_accetta))               ;accetta modifica parametri
    $var[39] = $ENCODE(/F0.0,ABS(b_p_teach))               ;abilita pagina TEACH
    $var[40] = $ENCODE(/F0.0,0)                            ;disp.
    $var[41] = $ENCODE(/F0.0,ABS(SIG(m_ok_read_cam1)))     ;dati acquisiti da telecamera 1
    $var[42] = $ENCODE(/F0.0,ABS(SIG(x_acq_img_cam1)))     ;richiesta acquisizione immagine a telecamera 1
    $var[43] = $ENCODE(/F0.0,ABS(b_data_wrong))            ;parametro inserito non corretto
    $var[44] = $ENCODE(/F0.0,ABS(b_res_teach))             ;reset valori di teach
    $var[45] = $ENCODE(/F0.0,ABS(b_res_tag1))              ;reset tagli coltello 1
    $var[46] = $ENCODE(/F0.0,ABS(b_res_tag2))              ;reset tagli coltello 2
    $var[47] = $ENCODE(/F0.0,0)                            ;disp.
    $var[48] = $ENCODE(/F0.0,ABS(b_res_counter))           ;reset contatori macchina
    $var[49] = $ENCODE(/F0.0,ABS(NOT SIG(x_pausa)))        ;external_it
    $var[50] = $ENCODE(/F0.0,i_arburg_recv)                ;ricezione parametri da arburg
    $var[51] = $ENCODE(/F0.0,0)                            ;disp.
    $var[52] = $ENCODE(/F0.0,ABS(b_res_parz))              ;reset contatori parziali
    $var[53] = $ENCODE(/F0.0,ABS(b_cam_p_ready))           ;telecamera analisi ptfe pronta (vicino montaggio mandrel)
    $var[54] = $ENCODE(/F0.0,ABS(b_cam_p_busy))            ;telecamera analisi ptfe in fase di elaborazione (vicino montaggio mandrel)
    $var[55] = $ENCODE(/F0.0,ABS(b_cam_p_endacq))          ;acquisizione immagine analisi ptfe terminata (vicino montaggio mandrel)
    $var[56] = $ENCODE(/F0.0,ABS(b_cam_p_buono))           ;pezzo buono da telecamera analisi ptfe (vicino montaggio mandrel)
    $var[57] = $ENCODE(/F0.0,ABS(b_cam_p_scarto))          ;pezzo scarto da telecamera analisi ptfe (vicino montaggio mandrel)
    $var[58] = $ENCODE(/F0.0,ABS(SIG(x_cam_p_acq)))             ;richiesta acquisizione immagine a telecamera analisi ptfe (vicino montaggio mandrel)
    $var[59] = $ENCODE(/F0.0,ABS(SIG(m_cam_p_res)))             ;reset acquisizione immagine a telecamera analisi ptfe (vicino montaggio mandrel)
    $var[60] = $ENCODE(/F0.0,ABS(b_cam_3d_ready))          ;telecamera 3d pronta (sopra rs5)
    $var[61] = $ENCODE(/F0.0,ABS(b_cam_3d_busy))           ;telecamera 3d in fase di elaborazione (sopra rs5)
    $var[62] = $ENCODE(/F0.0,ABS(b_cam_3d_endacq))         ;acquisizione immagine 3d terminata (sopra rs5)
    $var[63] = $ENCODE(/F0.0,ABS(b_cam_3d_buono))          ;pezzo buono da telecamera 3d (sopra rs5)
    $var[64] = $ENCODE(/F0.0,ABS(b_cam_3d_scarto))         ;pezzo scarto da telecamera 3d (vsopra rs5)
    $var[65] = $ENCODE(/F0.0,ABS(SIG(x_cam_3d_acq)))            ;richiesta acquisizione immagine a telecamera 3d (sopra rs5)
    $var[66] = $ENCODE(/F0.0,ABS(SIG(x_cam_3d_res)))            ;reset acquisizione immagine a telecamera 3d (sopra rs5)
; da 67 a 99 inizializzate in pc3_init_var
;
   VALUE 2:
;	  +-- gruppo 2: dati numerici	 
    $var[100] = $ENCODE(/F0.1,i_diam_est)                  ;diametro esterno lamiera
    $var[101] = $ENCODE(/F0.1,i_alt_lam)                   ;altezza lamiera
    $var[102] = $ENCODE(/F0.1,0)                           ;disp.
    $var[103] = $ENCODE(/F0.1,0)                           ;disp.
    $var[104] = $ENCODE(/F0.1,r_tim_raff)                  ;tempo raffreddamento anello sulla griglia
    $var[105] = $ENCODE(/F0.1,cut_param[38])               ;identificativo coltello da utilizzare (1=diritto 2=inclinato)
    $var[106] = $ENCODE(/F0.1,ABS(b_btaglio1))             ;abilita taglio 1 (0=disabilitato 1=abilitato)
    $var[107] = $ENCODE(/F0.1,ABS(b_btaglio2))             ;abilita taglio 2 (0=disabilitato 1=abilitato)
    $var[108] = $ENCODE(/F0.1,i_alt_gommato)               ;altezza anello gommato
    $var[109] = $ENCODE(/F0.1,i_diam_gom)                  ;diametro anello gommato
    $var[110] = $ENCODE(/F0.1,i_alt_inf)                   ;altezza stampo inferiore
    $var[111] = $ENCODE(/F0.1,i_alt_grif)                  ;altezza griffa scarico
    $var[112] = $ENCODE(/F0.1,i_alt_sup)                   ;altezza stampo superiore
    $var[113] = $ENCODE(/F0.1,ABS(b_btaglio3))             ;abilita taglio 3 (0=disabilitato 1=abilitato)
    $var[114] = $ENCODE(/F0.1,ABS(b_btaglio4))             ;abilita taglio 4 (0=disabilitato 1=abilitato)
    $var[115] = $ENCODE(/F0.1,0)                           ;disp.
    $var[116] = $ENCODE(/F0.1,i_nscarti_avv)               ;numero pezzi da scartare ad ogni avvio ciclo
    $var[117] = $ENCODE(/F0.1,raggio_portacol)             ;raggio portacoltello medio
    $var[118] = $ENCODE(/F0.1,cut_param[18])               ;mm di fuoriuscita coltello da portacoltello
    $var[119] = $ENCODE(/F0.1,cut_param[25])               ;speed mandrino taglio 1
    $var[120] = $ENCODE(/F0.1,cut_param[28])               ;speed avanzamento taglio 1
