;
   VALUE 3:
;   +-- gruppo 3: coordinate visione
    IF NOT SIG(m_ok_read_cam1) THEN
      r_x_vis[1] = VAL($var[250])                          ;X posizione 1
      r_y_vis[1] = VAL($var[251])                          ;Y posizione 1
