    $var[124] = $ENCODE(/F0.1,cut_param[31])               ;diametro taglio 1
    $var[125] = $ENCODE(/F0.1,cut_param[39])               ;attesa prima della presa da mandrino
    $var[126] = $ENCODE(/F0.0,cut_param[30])               ;angolo di spoglia taglio 1
    $var[127] = $ENCODE(/F0.1,cut_param[42])               ;speed mandrino taglio 2
    $var[128] = $ENCODE(/F0.1,cut_param[45])               ;speed taglio 2
    $var[129] = $ENCODE(/F0.1,cut_param[50])               ;altezza anello per taglio 2
    $var[130] = $ENCODE(/F0.1,cut_param[46])               ;angolo taglio 2
    $var[131] = $ENCODE(/F0.1,cut_param[49])               ;profondita' taglio 2
    $var[132] = $ENCODE(/F0.1,cut_param[48])               ;diametro taglio	2
    $var[133] = $ENCODE(/F0.1,0)                           ;disp.
