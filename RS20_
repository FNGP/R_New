    $var[1247] = $par_vis[98]
    $var[1248] = $par_vis[99]
    $var[1249] = $par_vis[100]
  END
  IF i_arburg_recv==2 THEN
    i_arburg_recv = 0
  END
;
  RETURN
.END
.PROGRAM pc5_chk_pc3_run()
;Disconnessione del socket tcp se chiusura forzata del pc3
;
  IF TASK(1003)<>1 AND (st_conn_sv<>0 OR NOT SIG(y_tcp_sv_ok)) THEN
    IF sk_id_sv>0 THEN
      .try = 0
      DO
        .try = .try+1
        TCP_CLOSE result,sk_id_sv
      UNTIL result==0 OR .try>4
      IF result<0 THEN
        st_conn_sv = -1 ;Errore
      ELSE
        TWAIT 5
        sk_id_sv = 0
        st_conn_sv = 0
      END
    END
  END
;
  RETURN
.END
.PROGRAM pg1()
  JOINT SPEED9 ACCU1 TIMER0 TOOL1 WORK0 CLAMP1 (OFF,0,0,O) OX= WX= #[-0.0027713,-2.964,-163.02,0,-20,0.001394] ;
.END
.PROGRAM pinzacarapre(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando apertura pinza lamiera
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_pcar_ch,-y_pcar_ap
    PULSE y_pcar_ap,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_pcar_ap,-x_pcar_ch) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 106
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        SIGNAL -y_pcar_ch,-y_pcar_ap
        PULSE y_pcar_ap,1
      END
      TWAIT 0.05
    END
  END
;
  IF .r_tim_attesa>0 THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
.PROGRAM pinzacarchiude(.$break,.$comando,.$controllo,.r_tim_attesa)
;Comando chiusura pinza lamiera
;
  IF .$break=="BREAK" THEN
    BREAK
  END
;
  IF .$comando=="ON" THEN
    SIGNAL -y_pcar_ap,-y_pcar_ch
    PULSE y_pcar_ch,1
  END
;
  IF .$controllo=="ON" THEN
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_pcar_ch,-x_pcar_ap) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        DREG (7) = 107
        CALL prosegui
        UTIMER .@r_tim_wait = 0
        SIGNAL -y_pcar_ap,-y_pcar_ch
        PULSE y_pcar_ch,1
      END
      TWAIT 0.05
    END
  END
;
  IF .r_tim_attesa>0 THEN
    TWAIT .r_tim_attesa
  END
;
  RETURN
.END
