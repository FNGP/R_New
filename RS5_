    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 28:
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 29,33:
    LMOVE #table3[5]
    JMOVE #fi_table
    JMOVE #m1_22_02
    JMOVE #m1_22_01
    JMOVE #phome
;
   VALUE 30:
    JMOVE #m5_11_02
    JMOVE #m5_11_03
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 31:
    JMOVE #m5_11_04
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 32:
    JMOVE #m5_11_06
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 34:
    JMOVE #cam_3d_01
    LMOVE #cam_3d_start
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 35:
    JMOVE #cam_3d_02
    LMOVE #cam_3d_01
    LMOVE #cam_3d_start
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 36:
    JMOVE #cam_3d_03
    LMOVE #cam_3d_02
    LMOVE #cam_3d_01
    LMOVE #cam_3d_start
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 37:
    JMOVE #cam_3d_04
    LMOVE #cam_3d_03
    LMOVE #cam_3d_02
    LMOVE #cam_3d_01
    LMOVE #cam_3d_start
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 38:
    JMOVE #cam_3d_05
    LMOVE #cam_3d_04
    LMOVE #cam_3d_03
    LMOVE #cam_3d_02
    LMOVE #cam_3d_01
    LMOVE #cam_3d_start
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
  END
  BREAK
  i_muove_da = 1
; +-- reject part in clamp
  IF clamp_status>0 OR NOT SIG(x_r_clamp_open) THEN
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- deposito anello negli sfridi
    CALL m_ring_reject
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  END
;
; +-- verify spindle empty at start cycle
  CALL spindle_verify(.spindle_empty)
  IF .spindle_empty==FALSE THEN
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- presa anello da mandrino
    CALL m_pick_spindle
;   +-- deposito anello negli sfridi
    CALL m_ring_reject
;   +-- porta il robot in posizione (HOME)
    i_muove_verso = 1
    CALL muovi_r
  END
  BREAK
  SIGNAL y_r1_safe_table
;
  IF rcp_fase_mand==1 THEN
    CALL restore_spindle
  END
; +--reset fuori ingombro a rs20
  SIGNAL y_ficapp_rs20
;
; +-- reset acqusizione telecamera
  SIGNAL -m_cam3d_prep
  SIGNAL -m_cam3d_start
  SIGNAL m_cam3d_stop
;  
  RETURN
.END
.PROGRAM m_pick_cutter1() #441
;Pick cutter from holder n.1
;
  CALL mov_par(critical_speed,100,100,100)
  JMOVE #fi_cutters
  CALL cutter_clamp("UNLOCK","")
  JMOVE #c_holder1001
  BREAK
  CALL mov_par(10,1,100,2)
  LMOVE #c_holder1002
  CALL cutter_holder(1,"UNLOCK","BREAK")
  CALL cutter_clamp("LOCK","BREAK")
  cutter_status = 1
  CALL mov_par(10,1,2,100)
  LMOVE #c_holder1001
  CALL mov_par(critical_speed,100,100,100)
  JMOVE #fi_cutters
;
  RETURN
.END
