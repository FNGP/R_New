.PROGRAM m_pick_spindle() #703082
;Pick cutted ring from spindle
;
  CALL mov_par(critical_speed,100,100,100)
  JMOVE #fi_spindle_p
  JMOVE #spin_appro
;
;                   +--------------- stop mandrino cappuccio
;                   |      +-------- comando stop mandrino cappuccio
;                   |      |    +--- controllo mandrino fermo
  CALL spindle_rot("STOP","ON","ON")
;
  CALL spindle_clamp("UNLOCK","","ON","ON")
;
  CALL chk_spindle("PICK")
;
; +-- attesa prima della presa da mandrino
  STABLE rcp_wait_pre
;
  CALL ring_clamp("UNLOCK","")
  LMOVE #spin_pick
  CALL ring_clamp("LOCK","BREAK")
  TWAIT 0.4
  spin_status = 0
  clamp_status = 2
  LMOVE #spin_appro
  JMOVE #fi_spindle_p
;
; +-- set soffio mandrino
; PULSE m_mand_soffio,1
;
  RETURN
.END
