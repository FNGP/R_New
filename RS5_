.PROGRAM m_place_spindle() #705656
;Place ring on spindle
;
  CALL mov_par(critical_speed,300,100,100)
  JMOVE #fi_spindle_p
  CALL chk_spindle("PLACE")
retry:
  ACCURACY 10
  JMOVE #spin_appro
  .collision = FALSE
  CALL x_move("#spin_place",-x_r_clamp_coll,250,.collision)
  IF .collision==TRUE THEN
    SPEED 5
    LMOVE #spin_appro
    CALL alarm(15)
    GOTO retry
  ELSE
    CALL ring_clamp("UNLOCK","BREAK")
    CALL spindle_clamp("LOCK","","ON","")
    spin_status = 1
    clamp_status = 0
    acn = 1
    LMOVE #spin_appro
  END
  ACCURACY 10
  JMOVE #fi_spindle_p
  IF NOT .collision THEN
    CALL spindle_clamp("LOCK","","","ON")
  END
;
  RETURN
.END
.PROGRAM m_place_table(.table_status) #699129
;Place the completed ring on the table
;
  CALL mov_par(critical_speed,300,100,100)
  CALL wait_r1_safe
;
; +-- control position free
  CALL chk_pos_free(5)
;
  JMOVE #table4[5]
  LMOVE #table3[5]
  LMOVE #table2place[5]
  IF SIG(x_cam_3d_inc) THEN
;   +-- control cam 3d result
    CALL cam_3d_risult
  END
  CALL ring_clamp("UNLOCK","BREAK")
  clamp_status = 0
  STABLE 0.2
  tab_part_status[5] = .table_status
  LMOVE #table1[5]
  tab_part_status[5] = .table_status
;
; +-- verify ring deposit on table
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(x_table_pres[5]) DO
    IF UTIMER(.@r_tim_wait)>3 THEN
      i_muove_verso = 1
      CALL muovi_r
      CALL alarm(33)
      UTIMER .@r_tim_wait = 0
    END
    TWAIT 0.02
  END
  UTIMER .@r_tim_wait = 0
;
  RETURN
.END
.PROGRAM m_ring_reject() #6340
;Deposito anello negli sfridi
;
  CALL mov_par(critical_speed,100,100,100)
  JMOVE #reject1
  LMOVE #reject2
  CALL ring_clamp("UNLOCK","BREAK")
  clamp_status = 0
  LMOVE #reject1
;
  RETURN
.END
.PROGRAM m_tab_spin() #5508
;Movements from table area to spindle/cutters area
;
;NOTE: Table_area...JT1=135   Spindle_area...JT1=-45
  jt1 = DEXT(#HERE,1)
  IF 30<jt1 AND jt1<160 THEN
    CALL mov_par(chng_area_speed,300,100,100)
    JMOVE #interm2
    JMOVE #interm3
    JMOVE #interm4
  ELSE
    PAUSE
    HALT
  END
;
  RETURN
.END
.PROGRAM main() #11
;Main looping sequence called from 'Master() after 'Initialize()'
;
  CALL set_tool("CUTTER")
;
  DO
;   +-- wait conditions to perform one mission
    CALL wait_mission
;
;   +-- init punti
    CALL initpoints
;
;   +-- check the cutting movement
    CALL chk_calc_cut
;
;   +-- execute the selected mission
    CASE miss_exe OF
     VALUE 1:
;     +-- automatic direct transfer fro Robot1 to table
      CALL ring_transfer
;     +-- controllo richiesta fine ciclo
      IF SIG(x_end_cycle_req) THEN
;       +-- reset fine ciclo
        PULSE y_res_end_cycle,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_end_cycle_req) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_res_end_cycle,1
          END
          TWAIT 0.02
        END
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- reset ciclo automatico
        PULSE y_auto_cycle_ok,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_auto_enable) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_auto_cycle_ok,1
          END
          TWAIT 0.02
        END
      END
;
     VALUE 2,11,12:
;     +-- cutting sequence for all rings
      CALL cut_sequence
;     +-- controllo richiesta fine ciclo
      IF SIG(x_end_cycle_req) THEN
;       +-- reset fine ciclo
        PULSE y_res_end_cycle,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_end_cycle_req) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_res_end_cycle,1
          END
          TWAIT 0.02
        END
;       +-- porta il robot in posizione (HOME)
        i_muove_verso = 1
        CALL muovi_r
;       +-- reset ciclo automatico
        PULSE y_auto_cycle_ok,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_auto_enable) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_auto_cycle_ok,1
          END
          TWAIT 0.02
        END
      END
;
     VALUE 5,16,17,24:
;     +-- set ID taglio per anticipo presa coltello
      CALL set_id_taglio(.id_tag)
;     +-- presa coltello
      CALL cutter_change(rcp_cutter_n[.id_tag])
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;
     VALUE 4:
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- controllo richiesta fine ciclo
      IF SIG(x_end_cycle_req) THEN
;       +-- reset fine ciclo
        PULSE y_res_end_cycle,1
        UTIMER .@r_tim_wait = 0
        WHILE SIG(x_end_cycle_req) DO
          IF UTIMER(.@r_tim_wait)>5 THEN
            UTIMER .@r_tim_wait = 0
            PULSE y_res_end_cycle,1
          END
          TWAIT 0.02
        END
      END
;
     VALUE 13:
;     +-- deposito coltello
      CALL cutter_change(0)
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;     +-- reset richiesta posa coltello
      CALL res_cutt_rel
;
     VALUE 14:
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- deposito coltello
      CALL cutter_change(0)
;     +-- posizione di cambiotipo
      CALL m_clamp_equip
;     +-- reset richiesta cambio tipo
      CALL res_clamp_eq
      HALT
;
     VALUE 15:
;     +-- move to home position
      CALL m_movehome
;
     VALUE 21:
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- deposito anello negli sfridi
      CALL m_ring_reject
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;
     VALUE 22:
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- presa anello da mandrino
      CALL m_pick_spindle
;     +-- deposito anello negli sfridi
      CALL m_ring_reject
;     +-- porta il robot in posizione (HOME)
      i_muove_verso = 1
      CALL muovi_r
;
     VALUE 23:
;     +-- taglio singolo (manuale)
      CALL cut_single
;     +-- reset richiesta taglio singolo
      PULSE y_res_tag_sing,1
      UTIMER .@r_tim_wait = 0
      WHILE SIG(x_tag_sing) DO
        IF UTIMER(.@r_tim_wait)>5 THEN
          CALL alarm(41)
          PULSE y_res_tag_sing,1
          UTIMER .@r_tim_wait = 0
        END
        TWAIT 0.02
      END
;
     VALUE 25:
;     +-- porta il robot in posizione (MANDRINO)
      i_muove_verso = 5
      CALL muovi_r
;     +-- controllo visivo coltello
      CALL m_cutter_check
;     +-- reset richiesta controllo visivo coltello
      CALL res_cutt_chk
      HALT
;
     VALUE 26:
;     +-- controllo RS20 in fuoriingombro
      CALL wait_r1_safe
;     +-- porta il robot in posizione (PIANO INCLINATO)
      i_muove_verso = 22
      CALL muovi_r
;     +-- preleva pezzo da tavola
      CALL m_pick_table
;     +-- controllo telecamera 3d
      CALL camera_3d
;     +-- controllo RS20 in fuoriingombro
      CALL wait_r1_safe
;     +-- porta il robot in posizione (PIANO INCLINATO)
      i_muove_verso = 22
      CALL muovi_r
      IGNORE x_r1_safe_spind
;     +-- deposito su piano inclinato
;                      +-- 2 = anello finito
      CALL m_place_table(2)
;     +-- muove in fuori ingombro
      JMOVE #fi_table
;     +-- reset ciclo rs5
      BREAK
      PULSE y_res_test,1
;
    END
;
  UNTIL FALSE
;
  RETURN
.END
.PROGRAM mov_par(.r_speed,.r_accur,.r_accel,.r_decel) #8848575
;Parameters for movement
;
  SPEED MINVAL(.r_speed,100) ALWAYS
  ACCURACY .r_accur ALWAYS
  ACCEL MINVAL(.r_accel,100) ALWAYS
  DECEL MINVAL(.r_decel,100) ALWAYS
;
  RETURN
.END
