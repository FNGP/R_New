.PROGRAM muovi_r() #2395549
;Movimenti da ogni stazione
;
; +-- crea il programma da eseguire
  $program = "muove_da"+$ENCODE(/F0.0,i_muove_da)+"_a"+$ENCODE(/F0.0,i_muove_verso)
; +-- verifica l'esistenza del programma
  IF EXISTPGM($program) THEN
    SCALL $program
  ELSE
;
    CALL alarm(16)
    HALT
  END
  i_muove_da = i_muove_verso
;
  RETURN
.END
.PROGRAM optimize(.#in,.#out,.#ref) #13942305
;Optimize JT4 and JT6 cut position 
;
  DECOMPOSE in[1] = .#in
  DECOMPOSE out[1] = .#in
  DECOMPOSE ref[1] = .#ref
  DO
    .ope = 0
    IF out[4]<-90 THEN
      .ope = 1 ;increase JT4
    ELSE
      IF out[4]>90 THEN
        .ope = 2 ;decrease JT4
      END
    END
;
    CASE .ope OF
     VALUE 1:
      out[4] = out[4]+180
      out[5] = -out[5]
      out[6] = out[6]-180
     VALUE 2:
      out[4] = out[4]-180
      out[5] = -out[5]
      out[6] = out[6]+180
    END
  UNTIL .ope==0
  DO
    IF ref[6]-out[6]>180 THEN
      out[6] = out[6]+360
    ELSE
      IF ref[6]-out[6]<180 THEN
        out[6] = out[6]-360
      END
    END
  UNTIL ABS(ref[6]-out[6])<180
;
  POINT .#out = #PPOINT(out[1],out[2],out[3],out[4],out[5],out[6])
;
  RETURN
.END
.PROGRAM origins() #24165
;Definitions of BASE and TOOLS
;
; +-- base
  POINT new_base = RZ(135)+RY(-90)
  BASE new_base
;
  POINT t_clamp0 = atcr2+RX(-35)+RZ(180)      ;from AUX405: Auto Tool Coordinate Register
  POINT t_clamp = t_clamp0+TRANS(0,0,-39.4+17.5)
;
;POINT t_cutter0 = TRANS(-22.5,-114.33,95.85)+RX(70) ;from drawings
  POINT t_cutter0 = atcr1+RX(70)+TRANS(,,-15) ;from AUX405: Auto Tool Coordinate Register
  POINT t_cutter = t_cutter0+TRANS(,,rcp_blade_len)
;
; +-- frame piano inclinato
  POINT f_pincl = FRAME(x1_pincl,x2_pincl,y1_pincl,x1_pincl)
;
; +-- massa di carico
  WEIGHT 2,0,0,100
;
;Colcal data (Monitor speed 100) (2013/02/07):
; COLR 134,210,175,112,167,140
; COLRJ 8,10,13,7,8,6
  COLR 200,250,200,150,200,190
  COLRJ 10,13,16,10,10,8
  COLRON
  COLRJON
;
  RETURN
;
;Lines for testing
  TOOL t_cutter0
  PAUSE
  TOOL t_cutter
  PAUSE
  TOOL t_clamp0
  PAUSE
  TOOL t_clamp
  PAUSE
;
.END
.PROGRAM pc1_cam_3d(.prep,.start,.stop) #410182717
;Gestione telecamera controllo 3d (sopra robot rs5)
;
;
; +-- preparazione telecamera per acquisizione
  IF .prep AND NOT .start AND NOT .stop THEN
    UTIMER @r_tim_to = 0
;   +-- reset acquisizione a telecamera
    WHILE SIG(x_cam_3d_buono) OR SIG(x_cam_3d_scarto) DO
      PULSE y_cam_3d_res,1
      TWAIT 0.02
    END
    SIGNAL -y_cam_3d_res
    b_scarto_cam_3d = TRUE
;
;   +-- se telecamera pronta
    IF SIG(x_cam_3d_ready) THEN
;     +-- richiesta preparazione telecamera
      SIGNAL y_cam_3d_acq
    ELSE
      SIGNAL -m_cam3d_prep
      .prep = FALSE
;     +-- segnalazione a op
      CALL alarm(46)
    END
    SIGNAL -m_cam3d_prep
    .prep = FALSE
    TWAIT 0.02
  END
;
;
;  +-- start acquisizione telecamera
  IF .start THEN
    PULSE y_cam_3d_trig,2
;   +-- reset timer
    UTIMER @r_tim_wait = 0
    UTIMER @r_tim_to = 0
;   +-- reset richiesta start
    IF SIG(y_cam_3d_trig) THEN
      SIGNAL -m_cam3d_prep
      SIGNAL -m_cam3d_start
      .start = FALSE
    END
    TWAIT 0.02
  END
;
;  +-- stop acquisizione telecamera
  IF .stop THEN
    SIGNAL -y_cam_3d_trig
    SIGNAL -y_cam_3d_acq
;   +-- reset timer
    UTIMER @r_tim_wait = 0
    UTIMER @r_tim_to = 0
;   +-- reset richiesta stop
    IF NOT SIG(y_cam_3d_trig) AND NOT SIG(y_cam_3d_acq) THEN
      SIGNAL -m_cam3d_stop
      .stop = FALSE
    END
    TWAIT 0.02
  END
;	
;
; +-- time out richieste
  IF SIG(y_cam_3d_trig) OR SIG(y_cam_3d_res) OR SIG(y_cam_3d_acq) THEN
    IF UTIMER(@r_tim_to)>60 THEN
;     +-- reset totale timer e richieste
      SIGNAL -y_cam_3d_trig
      SIGNAL -y_cam_3d_res
      SIGNAL -y_cam_3d_acq
      .stop = FALSE
      .prep = FALSE
      .start = FALSE
      SIGNAL -m_cam3d_prep
      SIGNAL -m_cam3d_start
      SIGNAL -m_cam3d_stop
      UTIMER @r_tim_wait = 0
      UTIMER @r_tim_to = 0
      TWAIT 0.02
    END
  END
  RETURN
.END
.PROGRAM pc1_chk_battery() #1711715389
;Controllo errore batterie scariche
;
; +-- controllo batterie Encoder
  IF ERROR==-21013 THEN
    i_warning = 3
  END
; +-- controllo batterie RAM
  IF ERROR==-21010 THEN
    i_warning = 4
  END
;
  RETURN
.END
.PROGRAM pc1_chk_pc2_run() #2147483647
;Controllo parallelo 2 in run
;
  IF NOT SIG(m_pc2_run) AND TASK(1002)<>1 AND b_pc_run THEN
    PULSE m_pc2_run,1
    PCABORT 2: 
    PCEXECUTE 2: autostart2.pc
  END
;
  RETURN
.END
.PROGRAM pc1_get_rcp_par() #1611672437
;Read recipe data from buffer array
;
; = rcp_buffer[ 0]                   ;disp.
  rcp_source = rcp_buffer[1]         ;raw rings source [0=Ring_table  1=Robot1_clamps]
  hmi_jig_push = rcp_buffer[2]       ;pushing movement of ring clamp on spindle
  hmi_corr_spin_x = rcp_buffer[3]    ;spindle center correction X
  hmi_corr_spin_y = rcp_buffer[4]    ;spindle center correction Y
  hmi_corr_spin_z = rcp_buffer[5]    ;spindle center correction Z
  hmi_mon_speed = rcp_buffer[6]      ;monitor Speed
  hmi_prog_speed = prog_speed        ;program speed
  max_cutt_life[1] = rcp_buffer[8]   ;max number of cuts for cutter 1
  rcp_buffer[9] = cutter_life[1]     ;actual number of cuts of cutter 1 (read-only)
  rcp_buffer[10] = cutter_life[2]    ;actual number of cuts of cutter 2 (read-only)
  max_cutt_life[2] = rcp_buffer[11]  ;max number of cuts for cutter 2
  rcp_mand_soff[1] = rcp_buffer[12]  ;soffio dopo taglio 1
  rcp_mand_soff[2] = rcp_buffer[13]  ;soffio dopo taglio 2
  rcp_mand_soff[3] = rcp_buffer[14]  ;soffio dopo taglio 3
  rcp_mand_soff[4] = rcp_buffer[15]  ;soffio dopo taglio 4
  rcp_jig_height = rcp_buffer[16]    ;mounting jig height [mm]
  rcp_blade_ang = rcp_buffer[17]     ;blade angle (total - assuming symmetrical) [deg]
  rcp_blade_len = rcp_buffer[18]     ;blade lenght [mm]
  rcp_clamp_mode = rcp_buffer[19]    ;clamp ring locking mode [0=close4lock  1=open4lock]
  rcp_spin_mode = rcp_buffer[20]     ;spindle ring locking mode [0=close4lock  1=open4lock]
  rcp_ext_diam = rcp_buffer[21]      ;ring external diameter [mm]
  rcp_ext_height = rcp_buffer[22]    ;ring external height [mm]
  rcp_buffer[23] = cycle_time        ;tempo ciclo RS5
; = rcp_buffer[24]                   ;disp.
  rcp_speed_mand[1] = rcp_buffer[25] ;speed mandrino taglio 1
  rcp_cut_ena[1] = rcp_buffer[26]    ;cut enable [0=disabled  1=enabled]
  rcp_region[1] = rcp_buffer[27]     ;ring cutting region [0=UP   1=DOWN]
  rcp_cut_sp[1] = rcp_buffer[28]     ;cutting speed (robot movement) [mm/s]
  rcp_cut_ang1[1] = rcp_buffer[29]   ;cutting angle 1 (0=(alfa) parallelo al mandrino) [deg]
  rcp_cut_ang2[1] = rcp_buffer[30]   ;cutting angle 2 (beta - around knife axis) [deg]
  rcp_cut_diam[1] = rcp_buffer[31]   ;cutting diameter "D" (2x radius from spindle center) [mm]
  rcp_cut_thick[1] = rcp_buffer[32]  ;cutting thickness "P" (along cutter axis) [mm]
  rcp_cut_height[1] = rcp_buffer[33] ;cutting height "H1" (from spindle plane) [mm]
  rcp_cut_lift[1] = rcp_buffer[34]   ;cutting lift distance "S" (+S=esce dal mandrino in centro, -S=entra nel mandrino in fuori) [mm]
  rcp_cut_appro[1] = rcp_buffer[35]  ;cutting start point approach distance "G" [mm]
  rcp_spin_rz[1] = rcp_buffer[36]    ;spindle (frame) rotation around Z axis (gamma) [deg]
  rcp_blad_corr[1] = rcp_buffer[37]  ;knife angle correction (alfa1+ senso antiorario)[+/-deg]
  rcp_cutter_n[1] = rcp_buffer[38]   ;cutter number for each cut (1=cutter_n.1  2=cutter_n.2)
  rcp_wait_pre = rcp_buffer[39]      ;attesa prima della presa da mandrino
; = rcp_buffer[40]                   ;disp.
  rcp_mand_stop[1] = rcp_buffer[41]  ;abilita stop mandrino dopo soffio taglio 1
  rcp_speed_mand[2] = rcp_buffer[42] ;speed mandrino taglio 2
  rcp_cut_ena[2] = rcp_buffer[43]    ;cut enable [0=disabled  1=enabled]
  rcp_region[2] = rcp_buffer[44]     ;ring cutting region [0=foreward   1=backward]
  rcp_cut_sp[2] = rcp_buffer[45]     ;cutting speed (robot movement) [mm/s]
  rcp_cut_ang1[2] = rcp_buffer[46]   ;cutting angle 1 (around cutting direction) [deg]
  rcp_cut_ang2[2] = rcp_buffer[47]   ;cutting angle 2 (around cutter axis) [deg]
  rcp_cut_diam[2] = rcp_buffer[48]   ;cutting diameter (2x radius from spindle center) [mm]
  rcp_cut_thick[2] = rcp_buffer[49]  ;cutting thickness (along cutter axis) [mm]
  rcp_cut_height[2] = rcp_buffer[50] ;cutting height (from spindle plane) [mm]
  rcp_cut_lift[2] = rcp_buffer[51]   ;cutting lift distance [mm]
  rcp_cut_appro[2] = rcp_buffer[52]  ;cutting start point approach distance [mm]
  rcp_spin_rz[2] = rcp_buffer[53]    ;spindle (frame) rotation around Z axis [deg]
  rcp_blad_corr[2] = rcp_param[54]   ;blade angle correction [+/-deg]
  rcp_cutter_n[2] = rcp_buffer[55]   ;cutter number for each cut (1=cutter_n.1  2=cutter_n.2)
; = rcp_buffer[56]                   ;disp.
; = rcp_buffer[57]                   ;disp.
  rcp_mand_stop[2] = rcp_buffer[58]  ;abilita stop mandrino dopo soffio taglio 2
  rcp_speed_mand[3] = rcp_buffer[59] ;speed mandrino taglio 3
  rcp_cut_ena[3] = rcp_buffer[60]    ;cut enable [0=disabled  1=enabled]
  rcp_region[3] = rcp_buffer[61]     ;ring cutting region [0=foreward   1=backward]
  rcp_cut_sp[3] = rcp_buffer[62]     ;cutting speed (robot movement) [mm/s]
  rcp_cut_ang1[3] = rcp_buffer[63]   ;cutting angle 1 (around cutting direction) [deg]
  rcp_cut_ang2[3] = rcp_buffer[64]   ;cutting angle 2 (around cutter axis) [deg]
  rcp_cut_diam[3] = rcp_buffer[65]   ;cutting diameter (2x radius from spindle center) [mm]
  rcp_cut_thick[3] = rcp_buffer[66]  ;cutting thickness (along cutter axis) [mm]
  rcp_cut_height[3] = rcp_buffer[67] ;cutting height (from spindle plane) [mm]
  rcp_cut_lift[3] = rcp_buffer[68]   ;cutting lift distance [mm]
  rcp_cut_appro[3] = rcp_buffer[69]  ;cutting start point approach distance [mm]
  rcp_spin_rz[3] = rcp_buffer[70]    ;spindle (frame) rotation around Z axis [deg]
  rcp_blad_corr[3] = rcp_param[71]   ;blade angle correction [+/-deg]
  rcp_cutter_n[3] = rcp_buffer[72]   ;cutter number for each cut (0=cutter n.1or2  1=cutter_n.1  2=cutter_n.2)
  rcp_speed_circ = rcp_buffer[73]/100    ;circular speed for camera 3d movement
; = rcp_buffer[74]                   ;disp.
  rcp_mand_stop[3] = rcp_buffer[75]  ;abilita stop mandrino dopo soffio taglio 3
  rcp_speed_mand[4] = rcp_buffer[76] ;speed mandrino taglio 4
  rcp_cut_ena[4] = rcp_buffer[77]    ;cut enable [0=disabled  1=enabled]
  rcp_region[4] = rcp_buffer[78]     ;ring cutting region [0=foreward   1=backward]
  rcp_cut_sp[4] = rcp_buffer[79]     ;cutting speed (robot movement) [mm/s]
  rcp_cut_ang1[4] = rcp_buffer[80]   ;cutting angle 1 (around cutting direction) [deg]
  rcp_cut_ang2[4] = rcp_buffer[81]   ;cutting angle 2 (around cutter axis) [deg]
  rcp_cut_diam[4] = rcp_buffer[82]   ;cutting diameter (2x radius from spindle center) [mm]
  rcp_cut_thick[4] = rcp_buffer[83]  ;cutting thickness (along cutter axis) [mm]
  rcp_cut_height[4] = rcp_buffer[84] ;cutting height (from spindle plane) [mm]
  rcp_cut_lift[4] = rcp_buffer[85]   ;cutting lift distance [mm]
  rcp_cut_appro[4] = rcp_buffer[86]  ;cutting start point approach distance [mm]
  rcp_spin_rz[4] = rcp_buffer[87]    ;spindle (frame) rotation around Z axis [deg]
  rcp_blad_corr[4] = rcp_param[88]   ;blade angle correction [+/-deg]
  rcp_cutter_n[4] = rcp_buffer[89]   ;cutter number for veach cut (1=cutter_n.1  2=cutter_n.2)
  rcp_speed_lin = rcp_buffer[90]/100    ;linear speed for camera 3d movement
  rcp_fase_mand = rcp_buffer[91]     ;ciclo orientamento mandrino
  rcp_mand_stop[4] = rcp_buffer[92]  ;abilita stop mandrino dopo soffio taglio 4
;  0 = rcp_buffer[93]                ;disp
;
  RETURN
.END
.PROGRAM pc1_ifp_disp() #1811420355
;Display usefull informations on IFP
;
  .itp = tab_part_status[1]*1000
  .itp = .itp+tab_part_status[2]*100
  .itp = .itp+tab_part_status[3]*10
  .itp = .itp+tab_part_status[4]
  ifp_tab_part = .itp
;
  ifp_clamp_stat = clamp_status
  ifp_spin_stat = spin_status
  ifp_cutt_stat = cutter_status
  ifp_cutt_life1 = cutter_life[1]
  ifp_cutt_life2 = cutter_life[2]
;
  .ipn = apn*100
  .ipn = .ipn+fpn*10
  .ipn = .ipn+lpn
  ifp_apn = .ipn
;
  ifp_acn = acn
;
  ifp_monspeed = hmi_mon_speed     ; Monitor Speed                        
  ifp_progspeed = hmi_prog_speed   ; Program speed                        
  ifp_ext_diam = rcp_ext_diam      ; Ring external diameter [mm]                           
  ifp_ext_height = rcp_ext_height  ; Ring external height [mm]                             
  ifp_max_cuts = max_cutt_life[1]  ; Max number of cuts for cutter 1 and 2
  ifp_clamp_mode = rcp_clamp_mode  ; Clamp ring locking mode [0=close4lock  1=open4lock]   
  ifp_spin_mode = rcp_spin_mode    ; Spindle ring locking mode [0=close4lock  1=open4lock] 
;   
  ifp_jig_push = hmi_jig_push      ; Pushing movement of ring clamp on spindle
  ifp_jig_height = rcp_jig_height  ; Mounting jig height [mm]                              
  ifp_blade_ang = rcp_blade_ang    ; Blade angle (total - assuming symmetrical) [deg]      
  ifp_blade_len = rcp_blade_len    ; Blade lenght [mm]                                     
;
  RETURN
.END
.PROGRAM pc1_mand_soffio() #1645346682
;Gestione soffio mandrino
;
  IF i_fase_soffio==0 AND SIG(m_mand_soffio) THEN
    PULSE y_mand_soffio,1
    i_fase_soffio = 1
  END
  IF i_fase_soffio==1 AND NOT SIG(y_mand_soffio) THEN
    UTIMER .@r_tim_wait = 0
    i_fase_soffio = 2
  END
  IF i_fase_soffio==2 THEN
    IF UTIMER(.@r_tim_wait)>1 THEN
      PULSE y_mand_soffio,1
      i_fase_soffio = 3
    END
  END
  IF i_fase_soffio==3 AND NOT SIG(y_mand_soffio) THEN
    i_fase_soffio = 0
  END
;
  RETURN
.END
.PROGRAM pc1_muovih_run() #1692331549
;Muovihome in esecuzione
;
  SIGNAL (-1+(2*ABS(WHICHTASK("muovihome")==1 AND SWITCH(POWER ) AND SWITCH(CS ))))*y_phome_run
;
  RETURN
.END
.PROGRAM pc1_plc_if() #1437876792
;Asynchronus operations
;
; +-- invio fine vita coltelli 1 RS20
  .chk_cut_status = cutter_status
  IF .chk_cut_status>0 THEN
    IF cutter_life[.chk_cut_status]>max_cutt_life[.chk_cut_status] THEN
      PULSE y_cutter_end[.chk_cut_status],1
    ELSE
      SIGNAL -y_cutter_end[.chk_cut_status]
    END
  END
;
; +-- detect new parts on table (in case of Robot1 release part on table)
  IF UTIMER(@t_tab_status)>1 OR SIG(x_r1_start) THEN
    UTIMER @t_tab_status = 0
;   +-- posizione piano inclinato 1 da tagliare
    IF tab_part_status[1]==0 AND SIG(x_table_pres[1]) THEN
      tab_part_status[1] = 1
    END
    IF tab_part_status[1]<>0 AND SIG(-x_table_pres[1]) THEN
      tab_part_status[1] = 0
    END
;   +-- posizione piano inclinato 5 tagliato
    IF tab_part_status[5]==0 AND SIG(x_table_pres[5]) THEN
      tab_part_status[5] = 2
    END
    IF tab_part_status[5]<>0 AND SIG(-x_table_pres[5]) THEN
      tab_part_status[5] = 0
    END
  END
;
; +-- presenza anelli in posizione 5
  IF tab_part_status[5]==2 THEN     ;      SIG(m_raw_pres_d) AND clamp_status==0 AND spin_status==0 THEN
    PULSE y_r1_all_done,0.5
  ELSE
    SIGNAL -y_r1_all_done
  END
; +-- start presa anelli da RS20
  IF SIG(x_r1_direct) THEN
;   +-- presenza anello in pinza 1
    IF SIG(x_r1_part_pres[1]) THEN
      r1_part_status[1] = 1
    ELSE
      r1_part_status[1] = 0
    END
  END
;
; +-- 'raw parts presence' signal (zona 4)
  .presence_p = FALSE
  IF tab_part_status[1]==1 THEN
    .presence_p = TRUE
  END
  IF .presence_p==TRUE THEN
    PULSE m_raw_pres_p,0.5
  ELSE
    SIGNAL -m_raw_pres_p
  END
;
; +-- 'raw parts presence' signal (zona 1)
  .presence_d = FALSE
  IF tab_part_status[5]==2 THEN
    .presence_d = TRUE
  END
  IF .presence_d==TRUE THEN
    PULSE m_raw_pres_d,0.5
  ELSE
    SIGNAL -m_raw_pres_d
  END
;
; +-- verify part sensors on table
  IF SIG(-x_table_pres[1]) THEN
    PULSE m_tab_empty,0.5 ; barale prima non definito (3)
  END
;
; +-- safe position output to Robot1
  IF SIG(y_home1) THEN
    PULSE y_r1_safe_table,0.5 ; barale da non definito a 0.5
  ELSE
    SIGNAL -y_r1_safe_table
  END
;
; +-- inform Robot1 that Robot2 area is empty (no rings present)
  IF SIG(-x_table_pres[1]) AND SIG(-x_table_pres[5]) AND clamp_status==0 AND spin_status==0 THEN
    PULSE y_r1_area_empty,0.5 ; barale da non definito a 0.5
  ELSE
    SIGNAL -y_r1_area_empty
  END
;
; +-- reset cut life counters
  FOR .cutter = 1 TO max_cutters
    IF SIG(x_res_cutt_life[.cutter]) THEN
      cutter_life[.cutter] = 0
    END
  END
;
; +-- segnlazione stato pezzo scaricato a Robot rs20
  SIGNAL (-1+(2*ABS(b_scarto_cam_3d)))*y_cam_3d_scarto
;
; +-- reset tagli coltello 1
  IF SIG(x_res_cut_life[1]) THEN
    cutter_life[1] = 0
  END
;
; +-- reset tagli coltello 2
  IF SIG(x_res_cut_life[2]) THEN
    cutter_life[2] = 0
  END
;
  RETURN
.END
.PROGRAM pc1_rcp_server() #1814783726
;Recipe server (via Profibus I/O)
;
;Server ready
  PULSE y_rcs_ready,0.5
;
;Communication reset external command
  IF SIG(m_rcp_srv_reset) OR SIG(x_rcp_srv_reset) THEN
    phase_rcs = 0
    SIGNAL -m_rcp_srv_reset
  END
;
;Start Read/Write sequence
  IF phase_rcs==0 THEN
    IF SIG(x_rcs_read_req) THEN
      phase_rcs = 1
    ELSE
      IF SIG(x_rcs_write_req) THEN
        phase_rcs = 101
      ELSE
        SIGNAL -y_rcs_busy
        SIGNAL -y_rcs_data_rdy,-y_rcs_param_err,-y_rcs_integer,-y_rcs_negative
        BITS y_rcs_r_data,16 = 0
      END
    END
  ELSE
    PULSE m_pc1_no_twait,0.5
  END
;
;Start READ sequence
  IF phase_rcs==1 THEN
    SIGNAL y_rcs_busy
;Read and Verify ADDR value and rcp_buffer[addr] value
    rcs_addr = BITS(x_rcs_addr,16)
    .addr_ok = TRUE
    IF rcs_addr>9999 THEN
      .addr_ok = FALSE
    END
    $str = "rcp_buffer["+$ENCODE(/F0.0,rcs_addr)+"]"
    IF NOT EXISTREAL($str) THEN
      .addr_ok = FALSE
    END
    IF .addr_ok==TRUE THEN
;Prepare R_DATA
      IF rcp_buffer[rcs_addr]<0 THEN
        SIGNAL y_rcs_negative
      END
      rcs_r_data = ABS(rcp_buffer[rcs_addr])
      IF rcs_r_data>655.35 THEN
        SIGNAL y_rcs_integer
        rcs_r_data = INT(rcs_r_data)
        rcs_r_data = MINVAL(rcs_r_data,65535)
      ELSE
        rcs_r_data = rcs_r_data*100
      END
;Write R_DATA
      BITS y_rcs_r_data,16 = rcs_r_data
;Set 'data_ready' output
      SIGNAL y_rcs_data_rdy
;
    ELSE
;Reset R_DATA
      BITS y_rcs_r_data,16 = 0
;Set 'Param.not present' output
      SIGNAL y_rcs_param_err
;
    END
    phase_rcs = 2
  END
;
;Waiting for ACK
  IF phase_rcs==2 AND SIG(x_rcs_ack,-x_rcs_read_req) THEN
    phase_rcs = 0
  END
;
;
;
;Start WRITE sequence
  IF phase_rcs==101 THEN
    SIGNAL y_rcs_busy
;Read and verify ADDR value
    rcs_addr = BITS(x_rcs_addr,16)
    .addr_ok = TRUE
    IF rcs_addr>9999 THEN
      .addr_ok = FALSE
    END
    .$str = "rcp_buffer["+$ENCODE(/F0.0,rcs_addr)+"]"
    IF NOT EXISTREAL(.$str) THEN
      .addr_ok = FALSE
    END
    IF .addr_ok==TRUE THEN
;Read and elaborate W_DATA
      rcs_w_data = BITS(x_rcs_w_data,16)
      IF SIG(x_rcs_negative) THEN
        rcs_w_data = -rcs_w_data
      END
      IF SIG(-x_rcs_integer) THEN
        rcs_w_data = rcs_w_data/100
      END
;Write rcp_buffer[addr]
      rcp_buffer[rcs_addr] = rcs_w_data
;
;Prepare R_DATA to send back to client to verify
      rcs_addr = BITS(x_rcs_addr,16)
      IF rcp_buffer[rcs_addr]<0 THEN
        SIGNAL y_rcs_negative
      END
      rcs_r_data = ABS(rcp_buffer[rcs_addr])
      IF rcs_r_data>655.35 THEN
        SIGNAL y_rcs_integer
        rcs_r_data = INT(rcs_r_data)
        rcs_r_data = MINVAL(rcs_r_data,65535)
      ELSE
        rcs_r_data = rcs_r_data*100
      END
;Write R_DATA
      BITS y_rcs_r_data,16 = rcs_r_data
;Set 'data_ready' output
      SIGNAL y_rcs_data_rdy
;
    ELSE
;Reset R_DATA
      BITS y_rcs_r_data,16 = 0
;Set 'Param.not present' output
      SIGNAL y_rcs_param_err
;
    END
    phase_rcs = 102
  END
;
;Waiting for ACK
  IF phase_rcs==102 AND SIG(x_rcs_ack,-x_rcs_write_req) THEN
    phase_rcs = 0
  END
;
  RETURN
.END
.PROGRAM pc1_set_speed() #1747262831
;Speed adjustment calculation
;
; +-- program speed
  IF first_cycle==TRUE THEN
    critical_speed = hmi_prog_speed*spd_correction
  ELSE
    critical_speed = hmi_prog_speed*spd_correction
  END
  chng_area_speed = 90*spd_correction
  chng_cutt_speed = 10
;
; +-- monitor speed
  IF SWITCH(REPEAT ) THEN
    MC SPEED hmi_mon_speed
  END
;
;
  RETURN
.END
.PROGRAM pc1_spin_settle() #1814174603
;Ring settlement on spindle by opening an closing the spindle grip
;
  IF rcp_spin_mode==0 THEN
;Close4lock
    .y_spin_lock = y_close_spin
    .y_spin_unlock = y_open_spin
  ELSE
;Open4lock
    .y_spin_lock = y_open_spin
    .y_spin_unlock = y_close_spin
  END
;
;Ring settlement sequence
  IF phase_s==0 AND SIG(m_spin_settle) THEN
    SIGNAL -m_spin_settle
    UTIMER @t_spin_settle = 0
    phase_s = 1
  END
  IF phase_s==1 THEN
    IF UTIMER(@t_spin_settle)>0.3 THEN  ;T.opening
      UTIMER @t_spin_settle = 0
      phase_s = 2
    END
  END
  IF phase_s==2 THEN
    IF UTIMER(@t_spin_settle)>0.1 THEN
      UTIMER @t_spin_settle = 0
      phase_s = 3
    END
  END
  IF phase_s==3 THEN
    IF UTIMER(@t_spin_settle)>0.3 THEN   ;T.closing
      UTIMER @t_spin_settle = 0
      phase_s = 4
    END
  END
  IF phase_s==4 THEN
    IF UTIMER(@t_spin_settle)>0.1 THEN
      UTIMER @t_spin_settle = 0
      phase_s = 0
    END
  END
;
;Enabling outputs to spindle   
  CASE phase_s OF
   VALUE 1:
    PULSE .y_spin_unlock
   VALUE 3:
    PULSE .y_spin_lock
  END
;
  RETURN
.END
.PROGRAM pc1_start_robot() #1646292425
;Seleziona ed avvia programma robot
;
; +-- antiripetizione pulsante HOME
  IF NOT SIG(x_movehome_req) THEN
    .b_start = FALSE
  END
;	+-- se siamo in repeat
  IF SWITCH(REPEAT ) THEN
;   +-- richiesta seleziona muovihome
    IF SIG(x_man_enable,x_movehome_req) AND NOT .b_start AND NOT SWITCH(TEACH_LOCK ) THEN
      .b_start = TRUE
      i_fase_start = 1
      UTIMER .@r_tim_wait = 0
      MC HOLD
    END
;   +-- robot non in ciclo, error reset e prime robot
    IF i_fase_start==1 AND NOT SWITCH(CS ) THEN
      IF UTIMER(.@r_tim_wait)>1 THEN
        i_fase_start = 2
        UTIMER .@r_tim_wait = 0
        MC ERESET
        MC PRIME robot
      END
    END
;   +-- verifica programma robot selezionato
    IF i_fase_start==2 AND WHICHTASK("robot")==1 THEN
      IF UTIMER(.@r_tim_wait)>1 THEN
        i_fase_start = 3
        UTIMER .@r_tim_wait = 0
        MC ZPOW ON
      END
    END
;   +-- avvio ciclo
    IF i_fase_start==3 AND SWITCH(POWER ) THEN
      IF UTIMER(.@r_tim_wait)>1 THEN
        IF NOT SWITCH(CS ) THEN
          i_fase_start = 0
          MC continue
        ELSE
          i_fase_start = 0
        END
      END
    END
  ELSE
;
    i_fase_start = 0
  END
;
  RETURN
.END
.PROGRAM pc1_warning() #1790058875
;Avvisi a RS20
;
; +-- controllo richiesta reset allarme
  IF NOT SIG(x_res_alarm) THEN
    .b_alm_reset = FALSE
  END
;
  IF SIG(x_res_alarm) AND NOT .b_alm_reset THEN
    .b_alm_reset = TRUE
    b_set_warning = FALSE
    i_warning = 0
    BITS y_warn_code0,5 = 0
  END
;
  IF i_warning<>0 AND NOT b_set_warning THEN
    BITS y_warn_code0,5 = i_warning
    b_set_warning = TRUE
  END
;
  RETURN
.END
.PROGRAM pc1_whichtask() #1705098164
;Verifica programmi da non eseguire in automatico
;
  .b_reset = FALSE
  IF SWITCH(REPEAT ) THEN
    IF WHICHTASK("aaa_pos")==1 THEN
      .b_reset = TRUE
    END
    IF WHICHTASK("aaa_align")==1 THEN
      .b_reset = TRUE
    END
    IF WHICHTASK("set_frame")==1 THEN
      .b_reset = TRUE
    END
    IF .b_reset THEN
      MC HOLD
      MC KILL
    END
  END
;
  RETURN
.END
.PROGRAM pc2_chk_pc1_run() #2147483647
;Controllo parallelo 1 in run
;
  IF NOT SIG(m_pc1_run) AND TASK(1001)<>1 AND b_pc_run THEN
    PULSE m_pc1_run,1
    PCABORT
    PCEXECUTE autostart.pc
  END
;
  RETURN
.END
.PROGRAM prova_ing_mand() #0
  JMOVE #spin_appro
  JMOVE #fi_spindle_p
  JMOVE #fi_spindle_c
  JMOVE #cut_p0
.END
.PROGRAM res_clamp_eq() #320
;Reset signal 'x_chng_type_req'
;
  PULSE y_chng_type_ok,1
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(-x_chng_type_req) DO
    IF UTIMER(.@r_tim_wait)>5 THEN
      CALL alarm(29)
      UTIMER .@r_tim_wait = 0
      PULSE y_chng_type_ok,1
    END
    TWAIT 0.1
  END
;
  RETURN
.END
.PROGRAM res_cutt_chk() #5
;Reset signal 'x_cutt_chk_req'
;
  PULSE y_cutt_chk_ok,1
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(-x_cutt_chk_req) DO
    IF UTIMER(.@r_tim_wait)>5 THEN
      CALL alarm(30)
      UTIMER .@r_tim_wait = 0
      PULSE y_cutt_chk_ok,1
    END
    TWAIT 0.1
  END
;
  RETURN
.END
.PROGRAM res_cutt_rel() #284
;Reset signal 'x_cutt_rel_req'
;
  PULSE y_cutt_rel_ok,1
  UTIMER .@r_tim_wait = 0
  WHILE NOT SIG(-x_cutt_rel_req) DO
    IF UTIMER(.@r_tim_wait)>5 THEN
      CALL alarm(28)
      UTIMER .@r_tim_wait = 0
      PULSE y_cutt_rel_ok,1
    END
    TWAIT 0.1
  END
;
  RETURN
.END
.PROGRAM reset_coll() #2541452
;Reset collaudo anello
;
; +-- se richiesta collaudo anello attiva
  IF SIG(x_coll_pezzo) THEN
;   +-- reset collaudo anello
    PULSE y_res_col_pezzo,1
;   +-- attesa collaudo anello resettato
    UTIMER .@r_tim_wait = 0
    WHILE SIG(x_coll_pezzo) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        CALL alarm(40)
        UTIMER .@r_tim_wait = 0
        PULSE y_res_coll_pezz,1
      END
      TWAIT 0.02
    END
  END
;
  RETURN
.END
.PROGRAM reset_ini() #21954
;Initial reset of in/out
;
  SIGNAL -y_r1_ok_place,-y_r1_all_done
  BITS y_alm_code0,8 = 0
  act_alarm = 0
  $act_alarm = "none"
  $act_alarm_time = "--:--:--"
  IFPWPRINT 1=" "
  TWAIT 0.2
  first_cycle = TRUE
  UTIMER @cycle_time0 = 0
  IGNORE x_r1_safe_table
;
  RETURN
.END
.PROGRAM restore_spindle() #9351
;Riposizionamento mandrino
;
  IF rcp_fase_mand==1 AND SIG(x_spin_unlook,-x_spin_look) THEN
    CALL spindle_rot("STOP","ON","ON")
  END
;
.END
.PROGRAM ring_clamp(.$ope,.$brk) #3925218
;Ringer clamp management
;
;   .$ope = "UNLOCK"       : release ring
;           "LOCK"         : lock ring
;
  IF rcp_clamp_mode==0 THEN
;   +-- close4lock
    SCASE .$ope OF
     SVALUE "LOCK":
      .$ope2 = "CLOSE"
     SVALUE "UNLOCK":
      .$ope2 = "OPEN"
    END
  ELSE
;   +-- open4lock
    SCASE .$ope OF
     SVALUE "LOCK":
      .$ope2 = "OPEN"
     SVALUE "UNLOCK":
      .$ope2 = "CLOSE"
    END
  END
;
  IF .$brk=="BREAK" THEN
    BREAK
  END
;
  SCASE .$ope2 OF
   SVALUE "OPEN":
    SIGNAL -y_close_r_clamp
    PULSE y_open_r_clamp,1
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(-x_r_clamp_closd) DO
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(11)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.02
    END
    UTIMER .@r_tim_wait = 0
    WAIT SIG(x_r_clamp_open) OR UTIMER(.@r_tim_wait)>0.3
;
   SVALUE "CLOSE":
    SIGNAL -y_open_r_clamp
    PULSE y_close_r_clamp,1
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(-x_r_clamp_open) DO
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(12)
        UTIMER .@r_tim_waitt = 0
      END
      TWAIT 0.02
    END
    UTIMER .@r_tim_wait = 0
    WAIT SIG(x_r_clamp_closd) OR UTIMER(.@r_tim_wait)>0.3
;
  END
;
  RETURN
.END
.PROGRAM ring_transfer() #4477
;Direct transfer of raw rings from Robot 1 to table
;
; +-- porta il robot in posizione (ROBOT1)
  i_muove_verso = 25
  CALL muovi_r
;
; +-- verifica presenza pezzo da scaricare su rs20
  IF SIG(x_r1_part_pres[1]) THEN
;   +-- altezza anello campione
    .h_model = 14.7
;   +-- prelievo da robot rs20
    CALL mov_par(critical_speed,100,100,100)
    CALL ring_clamp("UNLOCK","")
    JAPPRO #robot1_2[1],130
    DECEL 50
    LAPPRO #robot1_2[1],(rcp_ext_height-.h_model)
    CALL ring_clamp("LOCK","BREAK")
    TWAIT 0.5
    CALL robot1_clamp(1,"UNLOCK")
    clamp_status = 1        ;raw
    r1_part_status[1] = 0  ;empty
    ACCEL 50
    LAPPRO #robot1_2[1],130
;
;   +-- controllo posizione libera
    CALL chk_pos_free(1)
;   +-- deposito su piano inclinato
    JMOVE #fi_table
    JMOVE #table3[1]
    LMOVE #table2place[1]
    CALL ring_clamp("UNLOCK","BREAK")
    clamp_status = 0
    LMOVE #table1[1]
    JMOVE #fi_table
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_table_pres[1]) DO
      IF UTIMER(.@r_tim_wait)>3 THEN
        i_muove_verso = 1
        CALL muovi_r
        CALL alarm(50)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.02
    END
    UTIMER .@r_tim_wait = 0
    tab_part_status[1] = 1
    i_muove_da = 22
  END
;
; +-- verifica presenza pezzo su piano inclinato
  IF SIG(x_table_pres[5]) THEN
;   +-- altezza anello campione
    .h_model = 14.7
;   +-- prelievo da piano inclianto 
    CALL mov_par(critical_speed,100,100,100)
    CALL ring_clamp("UNLOCK","")
    JMOVE #table3[5]
    LMOVE #table2pick[5]
    CALL ring_clamp("LOCK","BREAK")
    clamp_status = 1
    LMOVE #table1[5]
    JMOVE #fi_table
    tab_part_status[5] = 0
;
;   +-- deposrito su robot rs20
    CALL mov_par(critical_speed,100,100,100)
    JAPPRO #robot1_2[1],130
    DECEL 50
    LAPPRO #robot1_2[1],(rcp_ext_height-.h_model)
;   +-- deposito
    CALL robot1_clamp(1,"LOCK")
    CALL ring_clamp("UNLOCK","BREAK")
    clamp_status = 0        ;raw
    ACCEL 50
    LAPPRO #robot1_2[1],130
    JMOVE #fi_table
    i_muove_da = 22
  END
;
; +-- porta il robot in posizione (HOME)
;  i_muove_da = 22
;  i_muove_verso = 1
;  CALL muovi_r
;
  RETURN
.END
.PROGRAM robot() #0
;Main program
;
ini:
  CALL initialize
  CALL m_movehome
  CALL main
  CALL m_movehome
  PAUSE
  GOTO ini
;
  RETURN
.END
.PROGRAM robot1_clamp(.n_clamp,.$ope) #8643
;Robot 1 clamp open
;
  BREAK
  SCASE .$ope OF
   SVALUE "UNLOCK":
    PULSE y_r1_op_clamp[.n_clamp],1
    UTIMER .@time_out = 0
    WHILE NOT SIG(x_r1_part_rel[.n_clamp]) DO
      IF UTIMER(.@time_out)>10 THEN
        CALL alarm(16+.n_clamp)
        UTIMER .@time_out = 0
        PULSE y_r1_op_clamp[.n_clamp],1
      END
      TWAIT 0.02
    END
   SVALUE "LOCK":
    PULSE y_r1_op_depo[.n_clamp],1
    UTIMER .@time_out = 0
    WHILE NOT SIG(x_r1_part_pick[.n_clamp]) DO
      IF UTIMER(.@time_out)>10 THEN
        CALL alarm(16+.n_clamp);*** verificare numero allarme
        UTIMER .@time_out = 0
        PULSE y_r1_op_depo[.n_clamp],1
      END
      TWAIT 0.02
    END
  END
;
  RETURN
.END
.PROGRAM search_raw(.$source) #703491
;Search first raw part on table, set 'fpn' and 'lpn'
;
retry:
  fpn = 0
  lpn = 0
  SCASE .$source OF
   SVALUE "TABLE":
    FOR .part = 1 TO max_rings
      IF tab_part_status[.part]==1 THEN
        fpn = .part ;first part number
        .part = 99999
      END
    END
    FOR .part = max_rings TO 1 STEP -1
      IF tab_part_status[.part] THEN
        lpn = .part ;last part number
        .part = -99999
      END
    END
;
   SVALUE "ROBOT1":
    FOR .part = 1 TO max_rings
      IF SIG(x_r1_part_pres[.part]) THEN
        fpn = .part ;first part number
        .part = 99999
      END
    END
    FOR .part = max_rings TO 1 STEP -1
      IF SIG(x_r1_part_pres[.part]) THEN
        lpn = .part ;last part number
        .part = -99999
      END
    END
;
  END
  IF fpn==0 OR lpn==0 THEN
    IF .$source=="TABLE" THEN
      CALL alarm(22)
    ELSE
      CALL alarm(39)
    END
    GOTO retry
  END
;
  IF fpn>lpn THEN
    PAUSE
    GOTO retry
  END
;
  RETURN
.END
.PROGRAM set_frame() #0
;Manual position teaching
;
  HALT
  CALL m_tab_spin
  TOOL t_cutter
  BASE new_base
  JMOVE #fi_spindle_c
  JMOVE #orient_spin
;
;Frame 'fr_spin' teaching
  JAPPRO #fsx1,20
  LMOVE #fsx1;[A]+[POS-MOD] (H.06 Xbase-)
  LAPPRO #fsx1,20
  LAPPRO #fsx2,20
  LMOVE #fsx2;[A]+[POS-MOD] (H.12 Xbase+)
  LAPPRO #fsx2,20
  LAPPRO #fsy1,20
  LMOVE #fsy1;[A]+[POS-MOD] (H.09 Ybase-)
  LAPPRO #fsy1,20
  LAPPRO #fsy2,20
  LMOVE #fsy2;[A]+[POS-MOD] (H.03 Ybase+)
  LAPPRO #fsy2,20
  PAUSE
;
;Calculate spindle posture and frame
  TOOL t_cutter
  BASE new_base
  POINT fsx1 = #fsx1
  POINT fsx2 = #fsx2
  POINT fsy1 = #fsy1
  POINT fsy2 = #fsy2
;
  ddx = DEXT(fsx2,1)-DEXT(fsx1,1)
  ddz1 = DEXT(fsx2,3)-DEXT(fsx1,3)
  rot_y = ATAN2(ddz1,ddx)
;
  ddy = DEXT(fsy2,2)-DEXT(fsy1,2)
  ddz2 = DEXT(fsy2,3)-DEXT(fsy1,3)
  rot_x = ATAN2(ddz2,ddy)
;
  POINT spin_posture = RX(180)+RX(rot_x)+RY(rot_y)
  POINT fr_spin = CCENTER(fsy1,fsx2,fsy2,spin_posture)
  POINT org_cut = RZ(90)+RY(180) ;origin positition for cutter on spindle
  POINT org_clamp = RZ(90)+RY(180) ;origin positition for clamp on spindle
  PAUSE
;
  JMOVE #fi_spindle_c
;
  RETURN
;
;Lines for testing
  PAUSE
  BASE new_base
  TOOL t_clamp
  POINT zzpresa = fr_spin+org_clamp
  JAPPRO zzpresa,20
  LMOVE zzpresa
;
.END
.PROGRAM set_id_taglio(.id_tag) #1577
;Definisce il primo taglio abilitato per la presa coltello ad inizio ciclo
;
; +-- taglio 1 abilitato
  IF rcp_cut_ena[1]==1 THEN
;   +-- set ID taglio
    .id_tag = 1
  ELSE
;
;   +-- taglio 2 abilitato
    IF rcp_cut_ena[2]==1 THEN
;     +-- set ID taglio
      .id_tag = 2
    ELSE
;
;     +-- taglio 3 abilitato
      IF rcp_cut_ena[3]==1 THEN
;       +-- set ID taglio
        .id_tag = 3
      ELSE
;
;       +-- taglio 4 abilitato
        IF rcp_cut_ena[4]==1 THEN
;         +-- set ID taglio
          .id_tag = 4
        END
      END
    END
  END
;
  RETURN
.END
