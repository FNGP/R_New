  rcp_buffer[83] = 0    ; rcp_cut_thick[4]   Cutting thickness (along cutter axis) [mm]                              
  rcp_buffer[84] = 0    ; rcp_cut_height[4]  Cutting height (from spindle plane) [mm]                                
  rcp_buffer[85] = 0    ; rcp_cut_lift[4]    Cutting lift distance [mm]                                              
  rcp_buffer[86] = 0    ; rcp_cut_appro[4]   Cutting start point approach distance [mm]                              
