.PROGRAM m_cutting() #7782
;Cutting movements
  JMOVE #fi_spindle_c
;
;                   +----------------- start mandrino cappuccio
;                   |       +--------- comando start mandrino cappuccio
;                   |       |    +---- controllo mandrino in movimento
  CALL spindle_rot("START","ON","OFF")
;
  CALL mov_par(critical_speed,200,100,100)
;
  DO
;
;   +--search for the next enabled 'acn' (active cut number)
    DO
      IF rcp_cut_ena[acn]<>1 THEN
        acn = acn+1
      END
    UNTIL rcp_cut_ena[acn]==1 OR acn=>4
;
;.... and execute it
    IF rcp_cut_ena[acn]==1 THEN
;     +-- set speed mandrino
      CALL set_spin_speed
;     +-- set TOOL
      CALL set_tool("CUTTER")
;     +-- calcolo posizioni di taglio
      CALL calc_cut
;
      JMOVE #cut_p0
;                       +----------------- start mandrino cappuccio
;                       |       +--------- comando start mandrino cappuccio
;                       |       |     +--- controllo mandrino in movimento
      CALL spindle_rot("START","OFF","ON")
;
      JMOVE #cut_p1
      CALL mov_par(critical_speed/10,5,100,100)
      spin_status = 90+acn  ;91/92/93/94 single cut NOT completed
      ABS.SPEED ON
      SPEED rcp_cut_sp[acn] MM/S
      LMOVE #cut_p2
      BREAK
      IF rcp_cut_lift[acn]==0 THEN
        SPEED 100 MM/S
        LMOVE #cut_p3
        ABS.SPEED OFF
      ELSE
        SPEED 50 MM/S
        LMOVE #cut_p3
        BREAK
        ABS.SPEED OFF
      END
      CALL mov_par(critical_speed,200,100,100)
      LMOVE #cut_p4
      JMOVE #cut_p0
;
      spin_status = 10+acn  ;11/12/13/14 single cut completed
      last_cutter = cutter_status
;     +-- controllo fine vita coltelli
      cutter_life[cutter_status] = cutter_life[cutter_status]+1
      IF cutter_life[cutter_status]>max_cutt_life[cutter_status] THEN
        i_warning = cutter_status
      END
;
      CASE acn OF
       VALUE 1:
        IF rcp_mand_soff[1]>0 AND (rcp_cut_ena[2]==1 OR rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1) THEN
          IF rcp_mand_stop[1]==1 THEN
            CALL spindle_rot("STOP","ON","ON")
          END
          JMOVE #fi_spindle_c
          BREAK
          PULSE y_mand_soffio,rcp_mand_soff[1]
          TWAIT rcp_mand_soff[1]
          IF rcp_mand_stop[1]==1 THEN
            CALL spindle_rot("START","ON","ON")
          END
        END
       VALUE 2:
        IF rcp_mand_soff[2]>0 AND (rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1) THEN
          IF rcp_mand_stop[2]==1 THEN
            CALL spindle_rot("STOP","ON","ON")
          END
          JMOVE #fi_spindle_c
          BREAK
          PULSE y_mand_soffio,rcp_mand_soff[2]
          TWAIT rcp_mand_soff[2]
          IF rcp_mand_stop[2]==1 THEN
            CALL spindle_rot("START","ON","ON")
          END
        END
       VALUE 3:
        IF rcp_mand_soff[3]>0 AND rcp_cut_ena[4]==1 THEN
          IF rcp_mand_stop[3]==1 THEN
            CALL spindle_rot("STOP","ON","ON")
          END
          JMOVE #fi_spindle_c
          BREAK
          PULSE y_mand_soffio,rcp_mand_soff[3]
          TWAIT rcp_mand_soff[3]
          IF rcp_mand_stop[3]==1 THEN
            CALL spindle_rot("START","ON","ON")
          END
        END
      END
;
      JMOVE #cut_p0
      acn = acn+1
    END
;
  UNTIL acn=>4
;
;                   +--------------- stop mandrino cappuccio
;                   |      +-------- comando stop mandrino cappuccio
;                   |      |    +--- controllo mandrino fermo
  CALL spindle_rot("STOP","ON","OFF")
;
  CALL mov_par(critical_speed,200,100,100)
  JMOVE #fi_spindle_c
;
; +-- seleziona la prossima "prima" velocita' mandrino da utilizzare
  IF rcp_cut_ena[1]==1 THEN
    acn = 1
    CALL set_spin_speed
  ELSE
;
    IF rcp_cut_ena[2]==1 THEN
      acn = 2
      CALL set_spin_speed
    ELSE
;
      IF rcp_cut_ena[3]==1 THEN
        acn = 3
        CALL set_spin_speed
      ELSE
;
        IF rcp_cut_ena[4]==1 THEN
          acn = 4
          CALL set_spin_speed
        END
      END
    END
  END
;
  RETURN
.END
.PROGRAM m_movehome() #35976
;Move the arm back to nest position
;
  BREAK
  PULSE y_spin_stop,1
  BITS y_alm_code0,8 = 0
  ABS.SPEED OFF
  POINT actual = HERE
  POINT mh[1] = #phome
  POINT mh[2] = #fi_table
  POINT mh[3] = #m1_22_01
  POINT mh[4] = #m1_22_02
  POINT mh[5] = #fi_spindle_p
  POINT mh[6] = #spin_pick
  POINT mh[7] = #fi_spindle_c
  POINT mh[8] = #fi_cutters
  POINT mh[9] = #c_holder1001
  POINT mh[10] = #c_holder2001
  POINT mh[11] = #clamp_eq1
  POINT mh[12] = #reject2
  POINT mh[13] = #c_holder1002
  POINT mh[14] = #c_holder2002
  POINT mh[15] = #robot1_2[1]
  POINT mh[16] = #fi_start_cam
  POINT mh[17] = #avv_cam3d_01
  POINT mh[18] = #cam_3d_start
  POINT mh[19] = #table1[1]
  POINT mh[20] = #fi_end_cam
  POINT mh[21] = #all_cam3d_01
  POINT mh[22] = #cam_3d_stop
  POINT mh[23] = #table2pick[1]
  POINT mh[24] = #m5_11_03
  POINT mh[25] = #m5_11_05
  POINT mh[26] = #m12_22_04
  POINT mh[27] = #m1_5_01
  POINT mh[28] = #m1_5_02
  POINT mh[29] = #table3[5]
  POINT mh[30] = #m5_11_02
  POINT mh[31] = #m5_11_04
  POINT mh[32] = #m5_11_06
  POINT mh[33] = #table2place[5]
  POINT mh[34] = #cam_3d_01
  POINT mh[35] = #cam_3d_02
  POINT mh[36] = #cam_3d_03
  POINT mh[37] = #cam_3d_04
  POINT mh[38] = #cam_3d_05
  POINT mh[39] = #phome
  POINT mh[40] = #phome
  POINT mh[41] = #phome
  POINT mh[42] = #phome
  POINT mh[43] = #phome
  POINT mh[44] = #phome
  POINT mh[45] = #phome
  POINT mh[46] = #phome
  POINT mh[47] = #phome
  .max_id = 47
;
;Create distance array (mh_d[])
  FOR .id_pos = 1 TO .max_id
    mh_d[.id_pos] = DISTANCE(HERE,mh[.id_pos])
  END
;
;Search for lesser distance mh_d[]
  .id_pos = 0
  mh_d[.id_pos] = 99999
  FOR .p = 1 TO .max_id
    IF mh_d[.p]<mh_d[.id_pos] THEN
      .id_pos = .p
    END
  END
;
;Select the appropriate path
  PRINT "MOVEHOME: path n.",.id_pos
  last_movehome = .id_pos
  SPEED 10,10 ALWAYS
  SPEED 4,4
  CASE .id_pos OF
   VALUE 1:
    JMOVE #phome
    TDRAW 0,0,10
    BREAK
    LMOVE #phome
;
   VALUE 2:
    JMOVE #fi_table
    JMOVE #m1_22_02
    JMOVE #m1_22_01
    JMOVE #phome
;
   VALUE 3:
    JMOVE #m1_22_01
    JMOVE #phome
;
   VALUE 4:
    JMOVE #m1_22_02
    JMOVE #m1_22_01
    JMOVE #phome
;
   VALUE 5:
    JMOVE #fi_spindle_p
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 6:
    JMOVE #spin_appro
    JMOVE #fi_spindle_p
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 7:
    DRAW 0,0,-10
    JMOVE #fi_spindle_c
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 8:
    JMOVE #fi_cutters
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 9:
    LMOVE #c_holder1001
    JMOVE #fi_cutters
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 10:
    LMOVE #c_holder2001
    JMOVE #fi_cutters
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 11:
    JMOVE #clamp_eq1
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 12:
    JMOVE #reject1
    JMOVE #fi_spindle_p
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 13:
    SPEED 2
    LMOVE #c_holder1001
    JMOVE #fi_cutters
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 14:
    SPEED 2
    LMOVE #c_holder2001
    JMOVE #fi_cutters
    JMOVE #m1_5_02
    JMOVE #m1_5_01
    JMOVE #phome
;
   VALUE 15:
    CALL ring_clamp("UNLOCK","BREAK")
    JAPPRO #robot1_2[1],130
    JMOVE #fi_table
    JMOVE #m1_22_02
    JMOVE #m1_22_01
    JMOVE #phome
;
   VALUE 16:
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 17:
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 18:
    JMOVE #cam_3d_start
    JMOVE #avv_cam3d_01
    JMOVE #fi_start_cam
    JMOVE #m11_1_01
    JMOVE #m11_1_02
    JMOVE #m11_1_03
    JMOVE #m11_1_04
    JMOVE #phome
;
   VALUE 19,23:
    LMOVE #table1[1]
    JMOVE #fi_table
    JMOVE #m1_22_02
    JMOVE #m1_22_01
    JMOVE #phome
;
   VALUE 20:
    JMOVE #fi_end_cam
    JMOVE #m12_1_01
    JMOVE #m12_1_02
    JMOVE #m12_1_03
    JMOVE #phome
;
   VALUE 21:
    JMOVE #all_cam3d_01
    JMOVE #fi_end_cam
    JMOVE #m12_1_01
    JMOVE #m12_1_02
    JMOVE #m12_1_03
    JMOVE #phome
;
   VALUE 22:
    JMOVE #cam_3d_stop
    LMOVE #cam_3d_11
    LMOVE #cam_3d_10
    LMOVE #cam_3d_09
