.END
.PROGRAM alarm(.alarm) #131008
;Alarm management
;
  last_alarm = act_alarm
  $last_alarm_time = $act_alarm_time
  act_alarm = .alarm
  $act_alarm_time = $TIME
;
  BITS y_alm_code0,8 = act_alarm
  WAIT SIG(x_res_alarm) OR SIG(x_movehome_req)
  BITS y_alm_code0,8 = 0
  IFPWPRINT 1,2,,4=" "
  IF SIG(x_movehome_req) THEN
    PAUSE
  END
;
  act_alarm = 0
  $act_alarm_time = "--:--:--"
;
  RETURN
.END
.PROGRAM arresta() #0
;Arresto immediato robot se mancano le condizioni di sicurezza
;
  HALT
.END
.PROGRAM autostart.pc() #0
;Process Control Task n.1
;
  QTOOL OFF
  CP ON
  STP_ONCE OFF
  ABS.SPEED OFF
  SLOW_START OFF
  AUTOSTART.PC ON
  AUTOSTART2.PC ON
  AUTOSTART3.PC OFF
  AUTOSTART4.PC OFF
  AUTOSTART5.PC OFF
  PREFETCH.SIGINS OFF
  CALL set_io
  CALL set_var
;
  max_t_pc1 = 0
  t0_pc1 = TIMER(0)
  UTIMER @t_tab_status = 0
  phase_rcs = 0           ;counter for Recipe-Server sequence
  phase_tab_ver = 0       ;counter for Check-table-sensors sequence
  t_spin_settle = 0       ;timer for ring settlement on spindle
  phase_s = 0             ;counter for ring settlement on spindle
  i_fase_start = 0        ;reset fase avvio ciclo
  i_fase_soffio = 0       ;reset fase soffio mandrino 1
;
  PULSE m_rcp_srv_reset,1 ;Reset communication with robot 1 on restart
  b_pc_run = TRUE
  DO
    rcp_buffer0 = rcp_buffer[0] ;for IFP display
    rcp_buffer1 = rcp_buffer[1]
    rcp_buffer2 = rcp_buffer[2]
    rcp_buffer3 = rcp_buffer[3]
    rcp_buffer4 = rcp_buffer[4]
    rcp_buffer5 = rcp_buffer[5]
    rcp_buffer6 = rcp_buffer[6]
;
;   +-- controllo pc2 in run
    CALL pc1_chk_pc2_run
;   +-- server rcp
    CALL pc1_rcp_server
;   +-- muovihome in esecuzione
    CALL pc1_muovih_run
;   +-- blocco programmi da eseguire solo manualmente		
    CALL pc1_whichtask
;   +-- update recipe data from rcp_buffer[]
    CALL pc1_get_rcp_par
;   +-- PLC (robot1) interface
    CALL pc1_plc_if
;   +-- ring settlement on spindle
    CALL pc1_spin_settle
;   +-- display informations on IFP
    CALL pc1_ifp_disp
;   +-- arm speed settings
    CALL pc1_set_speed
;   +-- controllo batterie
    CALL pc1_chk_battery
;   +-- warnings
    CALL pc1_warning
;   +-- seleziona ed avvia programma robot
    CALL pc1_start_robot
;   +-- gestione soffio mandrino
    CALL pc1_mand_soffio
;   +-- gestione telecamera 3D
    CALL pc1_cam_3d(SIG(m_cam3d_prep),SIG(m_cam3d_start),SIG(m_cam3d_stop))
;   +-- cutter on-board security signal to PLC
    IF cutter_status<>0 AND SIG(-y_home1) THEN
      PULSE y_cutt_on_board,0.5
    END
;
    IF NOT SIG(m_pc1_no_twait) THEN
      TWAIT 0.05
    END
    t_pc1 = TIMER(0)-t0_pc1
    t0_pc1 = TIMER(0)
    IF t_pc1>max_t_pc1 THEN
      max_t_pc1 = t_pc1
    END;
  UNTIL b_pc_run==FALSE
;
  RETURN
.END
.PROGRAM autostart2.pc() #0
;Processo parallelo 2
;
  b_pc_run = TRUE
  DO
;   +-- controllo parallelo 1 in run
    CALL pc2_chk_pc1_run
    TWAIT 0.02
  UNTIL b_pc_run==FALSE
;
  RETURN
.END
.PROGRAM calc_cut() #1273649
;Caculate cutting positions
;
  IF debug THEN
    TYPE 3: "CALC_CUT(",/F3.0,acn,/F0.2,"): diam=",rcp_cut_diam[acn],"  region=",rcp_region[acn],"  height=",rcp_cut_height[acn],"  thick=",rcp_cut_thick[acn]
    TYPE 3: /F0.2,"                ang1=",rcp_cut_ang1[acn],"  ang2=",rcp_cut_ang2[acn],"  lift=",rcp_cut_lift[acn],"  jig=",rcp_jig_height
    TYPE 3: /F0.2,"                blade_ang=",rcp_blade_ang,"  appro=",rcp_cut_appro[acn],"  RZ=",rcp_spin_rz[acn],"   BladeCorr=",rcp_blad_corr[acn]
    TYPE 3: /F0.2,"                blade_len=",rcp_blade_len
    TYPE 3: " "
  END
;
  IF rcp_region[acn]==0 THEN
;   +-- region 0: External
    POINT p1a = TRANS(rcp_cut_diam[acn]/2,0,rcp_jig_height+rcp_cut_height[acn])
  ELSE
;
;   +-- region 1: Internal
    POINT p1a = TRANS(-rcp_cut_diam[acn]/2,0,rcp_jig_height+rcp_cut_height[acn])
  END
  POINT p1 = p1a+RY(rcp_cut_ang1[acn]-rcp_blad_corr[acn])+org_cut+TRANS(,,-rcp_cut_appro[acn])+RZ(rcp_cut_ang2[acn]) ;start of cut
  POINT p0 = TRANS(0,0,45)+p1+TRANS(0,0,-30)
;
  cut_thick2 = rcp_cut_thick[acn]+rcp_cut_appro[acn]
  POINT p2 = p1+TRANS(0,cut_thick2*SIN(rcp_blad_corr[acn]),cut_thick2*COS(rcp_blad_corr[acn])) ;end of cut
;
  IF rcp_cut_lift[acn]>0 THEN
    POINT p3 = p2+TRANS(0,rcp_cut_lift[acn]*SIN(45),-rcp_cut_lift[acn]*COS(45))
  ELSE
;
    IF rcp_cut_lift[acn]<0 THEN
      POINT p3 = p2+TRANS(0,rcp_cut_lift[acn]*SIN(45),rcp_cut_lift[acn]*COS(45))
    ELSE
;
      POINT p3 = p1+TRANS(0,0,-10)
    END
  END
;
  IF rcp_region[acn]==0 THEN
;   +-- region 0: External
    POINT p4 = TRANS(0,0,30)+p3+TRANS(0,0,-rcp_cut_thick[acn]-10)
  ELSE
;
;   +-- region 1: Internal
    POINT p4 = TRANS(0,0,10)+p3+TRANS(0,0,-rcp_cut_thick[acn]-10)
  END
;
  POINT corr_fr_spin = TRANS(hmi_corr_spin_x,hmi_corr_spin_y,hmi_corr_spin_z)
;
  POINT #cut_p0t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p0,#fi_spindle_c ;over
  POINT #cut_p1t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p1,#fi_spindle_c ;start
  POINT #cut_p2t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p2,#fi_spindle_c ;end
  POINT #cut_p3t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p3,#fi_spindle_c ;lift
  POINT #cut_p4t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[acn])+p4,#fi_spindle_c ;retract
;
  CALL optimize(#cut_p0t,#cut_p0,#fi_spindle_c);1
  CALL optimize(#cut_p1t,#cut_p1,#fi_spindle_c);2
  CALL optimize(#cut_p2t,#cut_p2,#fi_spindle_c);3
  CALL optimize(#cut_p3t,#cut_p3,#fi_spindle_c);4
  CALL optimize(#cut_p4t,#cut_p4,#fi_spindle_c);5
;
  IF DEXT(#cut_p1,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p1,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
  IF DEXT(#cut_p2,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p2,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
  IF DEXT(#cut_p3,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p3,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
  IF DEXT(#cut_p4,6)>DEXT(#cut_p0,6)+90 OR DEXT(#cut_p4,6)<DEXT(#cut_p0,6)-90 THEN
    CALL alarm(31)
    HALT
  END
;
  POINT zz_p1[acn] = p1
  POINT zz_p0[acn] = p1+TRANS(0,0,-15)
  POINT zz_p2[acn] = p2
  POINT zz_p3[acn] = p3
;
  RETURN
.END
.PROGRAM calc_pos_spin(.#appro,.#pick,.#place) #676605
;Calculate picking/placing position on the spindle
;
  .rot_clamp = 30 ;clamp rotation amount around Z
;
  ddh = rcp_ext_height
;
  TOOL t_clamp
;
; +-- search for the most distant appros position to spindle
  .mm = 1
  POINT .appro = NULL
  DO
    POINT .tmp = fr_spin+RZ(.rot_clamp)+TRANS(,,ddh+.mm)+org_clamp+RX(-2)
    IF INRANGE(.tmp,#fi_spindle_p)==0 THEN
      POINT .appro = .tmp
      .mm = .mm+1
    ELSE
      .mm = -9999 ;exit_loop   
    END
  UNTIL .mm==-9999
;
  POINT corr_fr_spin = TRANS(hmi_corr_spin_x,hmi_corr_spin_y,hmi_corr_spin_z)
;
  POINT .pick = fr_spin+corr_fr_spin+RZ(.rot_clamp)+TRANS(,,ddh)+org_clamp
  POINT .place = fr_spin+corr_fr_spin+RZ(.rot_clamp)+TRANS(,,ddh-hmi_jig_push)+org_clamp
;
  POINT .#appro = .appro,#fi_spindle_p
  POINT .#pick = .pick,#fi_spindle_p
  POINT .#place = .place,#fi_spindle_p
;
  RETURN
.END
.PROGRAM calc_pos_tab(.#rif,.#pos1,.#pos2,.#pos3,.#pos4,.#pos5) #6181378
;Calcolo posizioni presa/deposito piano inclinato
;
; +-- diametro anello capione
  .d_model = 106.8
; +-- altezza anello campione
  .h_model = 14.7
;
; +-- set tool pinza carico
  CALL set_tool("CLAMP")
; +-- riferimento
  POINT f_pincl+.rif = .#rif
; +-- avvicinamento
  POINT .#pos1 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60)),(rcp_ext_height-.h_model)+40),.#rif
; +-- presa anello
  POINT .#pos2 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60)),(rcp_ext_height-.h_model)),.#rif
; +-- posa anello
  POINT .#pos3 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60))+2,(rcp_ext_height-.h_model)+1),.#rif
; +-- allontanamento
  POINT .#pos4 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60))+10,(rcp_ext_height-.h_model)+10),.#rif
; +-- allontanamento
  POINT .#pos5 = f_pincl+SHIFT(.rif BY 0,(rcp_ext_diam-.d_model)/(2*SIN(60))+20,(rcp_ext_height-.h_model)+40),.#rif
;
  RETURN
.END
.PROGRAM cam_3d_risult() #1790
;Controllo risultati camera 3d
;
; +-- reset iniziale
  SIGNAL -m_cam3d_prep
  SIGNAL -m_cam3d_start
  IF SIG(y_cam_3d_trig) THEN
    b_cam_stop = TRUE
  END
  UTIMER @r_tim_result = 0
  UTIMER @r_tim_wait = 0
; +-- attesa pezzo buono/scarto o timeout
  WAIT SIG(x_cam_3d_buono) OR SIG(x_cam_3d_scarto) OR UTIMER(@r_tim_wait)>10 OR act_alarm==46
; +-- se sono in timeout
  IF UTIMER(@r_tim_wait)>10 THEN
    UTIMER @r_tim_wait = 0
;   +-- set pezzo scarto
    b_scarto_cam_3d = TRUE
;   +-- reset acquisizione a telecamera
    PULSE y_cam_3d_res,2
;   +-- segnalazione a op
    CALL alarm(47)
    RETURN
  END
;
; +-- Verifica risultato buono o scarto
  IF SIG(x_cam_3d_buono) THEN
    b_scarto_cam_3d = FALSE
  ELSE
    b_scarto_cam_3d = TRUE
  END
; +-- reset acquisizione a telecamera
  PULSE y_cam_3d_res,2
;	
.END
.PROGRAM camera_3d() #2047
; +-- Controllo telecamera 3d (sopra robot rs5)
;
; +-- inizializza variabili gestione telecamera
  SIGNAL -m_cam3d_prep
  SIGNAL -m_cam3d_start
  SIGNAL -m_cam3d_stop
;
; +-- set tool pinza
  CALL set_tool("CLAMP")
; +-- richiesta preparazione telecamera
  SIGNAL m_cam3d_prep
; +-- porta il robot in posizione (FUORI INGOMBRO DA TELCAMERA)
  i_muove_verso = 11
  CALL muovi_r
; +-- avvicinamento a telecamera
  JMOVE #avv_cam3d_01
; +-- posizione start controllo telecamera
  JMOVE #cam_3d_start
; +-- start acquisizione dati da telecamera
  BREAK
  SIGNAL m_cam3d_start
;
; +-- setto i valori di movimento (movimenti lenti)
  ABS.SPEED ON
  SPEED MINVAL(rcp_speed_lin,2),MINVAL(15,20) ALWAYS ; prove eseguite con velocita lin=0.7, circ=15
  ACCURACY 0.1 ALWAYS
  BREAK
  LMOVE #cam_3d_01a
  LMOVE #cam_3d_01b
  LMOVE #cam_3d_01
  BREAK
; +-- setto i valori di movimento (movimenti veloci)
  SPEED 10,35 ALWAYS
  LMOVE #cam_3d_02
  LMOVE #cam_3d_03
  LMOVE #cam_3d_04
  LMOVE #cam_3d_05
  BREAK
; +-- setto i valori di movimento (movimenti lenti)
  SPEED MINVAL(rcp_speed_circ,2),MINVAL(15,20) ALWAYS
; +-- posizione finale controllo telecamera
  LMOVE #cam_3d_stop
; +-- stop acquisizione dati da telecamera
  BREAK
  SIGNAL m_cam3d_stop
  BREAK
  ABS.SPEED OFF
;
; +-- set valori collision detection
  CALL mov_par(critical_speed,300,100,100)
; +-- allontanamento da telecamera
;LMOVE #all_cam3d_01
; +-- fuori ingombro da telecamera (finale)
  JMOVE #fi_end_cam
  i_muove_da = 12
;
  RETURN
.END
.PROGRAM chk_align(.#p1,.#p2) #5016323
;Check robot alignment in linear motion
;
  IF DEXT(.#p1,5)>0 AND DEXT(.#p2,5)<0 THEN
    b_align = ON
  END
  IF DEXT(.#p1,5)<0 AND DEXT(.#p2,5)>0 THEN
    b_align = ON
  END
  IF DEXT(.#p1,5)>-0.5 AND DEXT(.#p1,5)<0.5 THEN
    b_align = ON
  END
  IF DEXT(.#p2,5)>-0.5 AND DEXT(.#p2,5)<0.5 THEN
    b_align = ON
  END
  IF b_align==ON THEN
    CASE chk_acn OF
     VALUE 1:
      CALL alarm(35)
     VALUE 2:
      CALL alarm(36)
     VALUE 3:
      CALL alarm(37)
     VALUE 4:
      CALL alarm(38)
     ANY :
      HALT
    END
  END
;
  RETURN
.END
.PROGRAM chk_calc_cut() #764139
;Controllo posizioni di taglio
;
ini: b_align = OFF
  FOR chk_acn = 1 TO 4
    IF rcp_cut_ena[chk_acn]==1 THEN
;
      IF rcp_region[chk_acn]==0 THEN
;       +-- region 0: External
        POINT p1a = TRANS(rcp_cut_diam[chk_acn]/2,0,rcp_jig_height+rcp_cut_height[chk_acn])
      ELSE
;
;       +-- region 1: Internal
        POINT p1a = TRANS(-rcp_cut_diam[chk_acn]/2,0,rcp_jig_height+rcp_cut_height[chk_acn])
      END
      POINT p1 = p1a+RY(rcp_cut_ang1[chk_acn]-rcp_blad_corr[chk_acn])+org_cut+TRANS(,,-rcp_cut_appro[chk_acn])+RZ(rcp_cut_ang2[chk_acn]) ;start of cut
      POINT p0 = TRANS(0,0,30)+p1+TRANS(0,0,-30)
;
      cut_thick2 = rcp_cut_thick[chk_acn]+rcp_cut_appro[chk_acn]
      POINT p2 = p1+TRANS(0,cut_thick2*SIN(rcp_blad_corr[chk_acn]),cut_thick2*COS(rcp_blad_corr[chk_acn])) ;end of cut
;
      IF rcp_cut_lift[chk_acn]>0 THEN
        POINT p3 = p2+TRANS(0,rcp_cut_lift[chk_acn]*SIN(45),-rcp_cut_lift[chk_acn]*COS(45))
      ELSE
;
        IF rcp_cut_lift[chk_acn]<0 THEN
          POINT p3 = p2+TRANS(0,rcp_cut_lift[chk_acn]*SIN(45),rcp_cut_lift[chk_acn]*COS(45))
        ELSE
;
          POINT p3 = p1+TRANS(0,0,-10)
        END
      END
      IF rcp_region[chk_acn]==0 THEN
;       +-- region 0: External
        POINT p4 = TRANS(0,0,30)+p3+TRANS(0,0,-rcp_cut_thick[chk_acn]-10)
      ELSE
;
;       +-- region 1: Internal
        POINT p4 = TRANS(0,0,10)+p3+TRANS(0,0,-rcp_cut_thick[chk_acn]-10)
      END
;
      POINT corr_fr_spin = TRANS(hmi_corr_spin_x,hmi_corr_spin_y,hmi_corr_spin_z)
;
      POINT #cut_p0t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p0,#fi_spindle_c ;over
      POINT #cut_p1t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p1,#fi_spindle_c ;start
      POINT #cut_p2t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p2,#fi_spindle_c ;end
      POINT #cut_p3t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p3,#fi_spindle_c ;lift
      POINT #cut_p4t = fr_spin+corr_fr_spin+RZ(rcp_spin_rz[chk_acn])+p4,#fi_spindle_c ;retract
;
      CALL optimize(#cut_p0t,#cut_p0,#fi_spindle_c)
      CALL optimize(#cut_p1t,#cut_p1,#fi_spindle_c)
      CALL optimize(#cut_p2t,#cut_p2,#fi_spindle_c)
      CALL optimize(#cut_p3t,#cut_p3,#fi_spindle_c)
      CALL optimize(#cut_p4t,#cut_p4,#fi_spindle_c)
      CALL chk_align(#cut_p1,#cut_p2)
      CALL chk_align(#cut_p2,#cut_p3)
      CALL chk_align(#cut_p3,#cut_p4)
    END
  END
  IF b_align==ON GOTO ini
;
  RETURN
.END
.PROGRAM chk_cutt_clamp() #26081
;Verify cutter clamp status
;
  CASE cutter_status OF
   VALUE 0:
    CALL cutter_clamp("UNLOCK","BREAK")
    CALL cutter_holder(1,"LOCK","BREAK")
    CALL cutter_holder(2,"LOCK","BREAK")
   VALUE 1:
    CALL cutter_clamp("LOCK","BREAK")
    CALL cutter_holder(1,"UNLOCK","BREAK")
    CALL cutter_holder(2,"LOCK","BREAK")
   VALUE 2:
    CALL cutter_clamp("LOCK","BREAK")
    CALL cutter_holder(1,"LOCK","BREAK")
    CALL cutter_holder(2,"UNLOCK","BREAK")
  END
;
  RETURN
.END
.PROGRAM chk_cutter_n(.cutter) #44139
;Check cutter number
;
  WHILE (rcp_cutter_n[.cutter]==0 AND rcp_cut_ena[.cutter]) DO
    CALL alarm(34)
  END
;
  RETURN
.END
.PROGRAM chk_pc1_run() #205355
;Controllo parallelo 1 in run
;
  IF NOT SIG(m_pc1_run) AND TASK(1001)<>1 AND b_pc_run THEN
    PULSE m_pc1_run,1
    PCABORT
    PCEXECUTE autostart.pc
  END
;
  RETURN
.END
.PROGRAM chk_pos_free(.id_pos) #699546
;Controllo posizione piano inclinato libera
;
  IF SIG(x_table_pres[.id_pos]) THEN
    BREAK
    WHILE SIG(x_table_pres[.id_pos]) DO
      CALL alarm(45+.id_pos)
    END
  END
;
  RETURN
.END
.PROGRAM chk_spindle(.$ope) #1415342
;Verify spindle status before access
;
  IF rcp_spin_mode==0 THEN
    SCASE .$ope OF
     SVALUE "PICK":
      WHILE NOT SIG(-x_spin_rotating) DO
        CALL alarm(25)
      END
     SVALUE "PLACE":
      WHILE NOT SIG(-x_spin_rotating,x_spin_open) DO
        CALL alarm(26)
      END
    END
  ELSE
    SCASE .$ope OF
     SVALUE "PICK":
      WHILE NOT SIG(-x_spin_rotating) DO
        CALL alarm(25)
      END
     SVALUE "PLACE":
      WHILE NOT SIG(-x_spin_rotating,x_spin_closed) DO
        CALL alarm(26)
      END
    END
  END
;
  RETURN
.END
.PROGRAM cut_sequence() #629172
;Sequenza di taglio anello
;
; +-- set timer per tempo ciclo
  UTIMER @cycle_time0 = 0
; +-- se la pinza e' vuota preleva dal piano inclinato
  IF clamp_status==0 THEN
;   +-- controllo RS20 in fuoriingombro
    CALL wait_r1_safe
;   +-- porta il robot in posizione (PIANO INCLINATO)
    i_muove_verso = 22
    CALL muovi_r
;   +-- presa anello da piano inclinato
    CALL m_pick_table
  END
;
; +-- controllo RS20 in fuoriingombro
  CALL wait_spindle_sa
  SIGNAL -y_ficapp_rs20
; +-- porta il robot in posizione (MANDRINO)
  i_muove_verso = 5
  CALL muovi_r
; +-- reset controllo collisione con RS20 su tavola
  IGNORE x_r1_safe_table
; +-- deposito anello su mandrino
  CALL m_place_spindle
;
; +-- verifica tipo di coltello per il taglio
  CALL cutter_change(rcp_cutter_n[apn])
;
; +-- controllo visivo coltello
  IF SIG(x_cutt_chk_req) THEN
    CALL m_cutter_check
    CALL res_cutt_chk
  END
;
; +-- esegue i tagli attivi per questo anello
  CALL m_cutting
;
; +-- set tagli completati
  spin_status = 2
;
; +-- presa anello da mandrino
  CALL m_pick_spindle
;
; +-- se richiesta collaudo pezzo o posizione gia occupata
  IF SIG(x_coll_pezzo) OR SIG(x_table_pres[5]) THEN
;   +-- deposito anello negli sfridi
    CALL m_ring_reject
;   +-- reset stato posizione piano inclinato
    tab_part_status[5] = 0
;
;   +-- se non ho nessun pezzo da prelevare o non ho lo start, vado in home
    IF (NOT SIG(m_raw_pres_p) AND NOT SIG(x_r1_direct)) OR NOT SIG(x_start_seq_req) THEN
;     +-- porta il robot in posizione (HOME)
      SIGNAL y_ficapp_rs20
      i_muove_verso = 1
      CALL muovi_r
;     +-- reset controllo collisione con RS20 da tavola e mandrino
      IGNORE x_r1_safe_table
      IGNORE x_r1_safe_spind
    ELSE
;
;     +-- controllo RS20 in fuoriingombro
      CALL wait_r1_safe
;     +-- porta il robot in posizione (PIANO INCLINATO)
      SIGNAL y_ficapp_rs20
      i_muove_verso = 22
      CALL muovi_r
      IGNORE x_r1_safe_spind
    END
;
;   +-- reset collaudo pezzo
    CALL reset_coll
  ELSE
;
    IF SIG(x_cam_3d_inc) THEN
;     +-- porta il robot in posizione telecamera 3d
      CALL camera_3d
    END
;
;   +-- controllo RS20 in fuoriingombro
    CALL wait_r1_safe
;   +-- porta il robot in posizione (PIANO INCLINATO)
    SIGNAL y_ficapp_rs20
    i_muove_verso = 22
    CALL muovi_r
    IGNORE x_r1_safe_spind
;
;   +-- deposito su piano inclinato
;                      +-- 2 = anello finito
    CALL m_place_table(2)
;
;   +-- se non ho nessun pezzo da prelevare o non ho lo start, vado in home
    IF (NOT SIG(m_raw_pres_p) AND NOT SIG(x_r1_direct)) OR NOT SIG(x_start_seq_req) THEN
;     +-- porte il robot in fuori ingombro tavola
      JMOVE #fi_table
;     +-- reset controllo collisione con RS20
      IGNORE x_r1_safe_table
      IGNORE x_r1_safe_spind
    END
  END
; +-- lettura tempo ciclo
  CALL get_cycle_time
;
; +-- reset primo ciclo
  first_cycle = FALSE
;
  RETURN
.END
.PROGRAM cut_single() #295
;Automatic cutting sequence for one ring
;
; +-- set timer per tempo ciclo
  UTIMER @cycle_time0 = 0
; +-- se la pinza e' vuota preleva dal piano inclinato
  IF clamp_status==0 THEN
;   +-- controllo RS20 in fuoriingombro
    CALL wait_r1_safe
;   +-- porta il robot in posizione (PIANO INCLINATO)
    i_muove_verso = 22
    CALL muovi_r
;   +-- presa anello da piano inclinato
    CALL m_pick_table
  END
;
; +-- controllo RS20 in fuoriingombro
  CALL wait_spindle_sa
; +-- porta il robot in posizione (MANDRINO)
  i_muove_verso = 5
  CALL muovi_r
; +-- reset controllo collisione con RS20
  IGNORE x_r1_safe_table
; +-- deposito anello su mandrino
  CALL m_place_spindle
;
; +-- verifica tipo di coltello per il taglio
  CALL cutter_change(rcp_cutter_n[apn])
;
; +-- controllo visivo coltello
  IF SIG(x_cutt_chk_req) THEN
    CALL m_cutter_check
    CALL res_cutt_chk
  END
;
; +-- execute all the (remaining) enabled cuts on this ring
  CALL m_cutting
;
  spin_status = 2  ;all cuts are completed
;
;                   +--------------- stop mandrino cappuccio
;                   |      +-------- comando stop mandrino cappuccio
;                   |      |    +--- controllo mandrino fermo
  CALL spindle_rot("STOP","ON","ON")
;
; +-- presa anello da mandrino
  CALL m_pick_spindle
;
; +-- deposito anello negli sfridi
  CALL m_ring_reject
;
; +-- porta il robot in posizione (HOME)
  i_muove_verso = 1
  CALL muovi_r
  IGNORE x_r1_safe_spind
; +-- lettura tempo ciclo
  CALL get_cycle_time
;
  RETURN
.END
.PROGRAM cutter_change(.n_cutter_req) #709767
;Pick the requested cutter (1 or 2, 0=release any cutter)
;
; +-- verify 'cutter_status' variable coherence
  CALL cutter_verify
;
; +-- motions for cutter release/change
  c_cut_op = 10*cutter_status+.n_cutter_req  ;change_cut_operation
  CASE c_cut_op OF
   VALUE 0,11,22:
;   +-- nothing to do
;
   VALUE 1:
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- prelievo coltello 1
    CALL m_pick_cutter1
;
   VALUE 2:
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- prelievo coltello 2
    CALL m_pick_cutter2
;
   VALUE 10:
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- deposito coltello 1
    CALL m_place_cutter1
;
   VALUE 20:
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- deposito coltello 2
    CALL m_place_cutter2
;
   VALUE 12:
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- deposito coltello 1
    CALL m_place_cutter1
;   +-- prelievo coltello 2
    CALL m_pick_cutter2
;
   VALUE 21:
;   +-- porta il robot in posizione (MANDRINO)
    i_muove_verso = 5
    CALL muovi_r
;   +-- deposito coltello 2
    CALL m_place_cutter2
;   +-- prelievo coltello 1
    CALL m_pick_cutter1
  END
;
; +-- verify 'cutter_status' variable coherence
  CALL cutter_verify
;
  RETURN
.END
.PROGRAM cutter_clamp(.$ope,.$brk) #28483
;Operate cutter clamp
;
;   .$ope = "UNLOCK"       : release ring
;           "LOCK"         : lock ring
;   .$brk = "BREAK"        : BREAK instruction before operation
;         = ""             : no BREAK instruction before operation
;
  IF .$brk=="BREAK" THEN
    BREAK
  END
;
  SCASE .$ope OF
   SVALUE "UNLOCK":
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_c_clamp_open,-x_c_clamp_closd) DO
      SIGNAL -y_close_c_clamp
      PULSE y_open_c_clamp
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(5)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.1
    END
;
   SVALUE "LOCK":
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(-x_c_clamp_open,x_c_clamp_closd) DO
      SIGNAL -y_open_c_clamp
      PULSE y_close_c_clamp
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(6)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.1
    END
;
   ANY :
    PAUSE
    HALT
  END
;
  RETURN
.END
.PROGRAM cutter_holder(.holder_numb,.$ope,.$brk) #54167
;Operate cutter holder
;
;   .holder_numb = 1 or 2  : holder number   
;   .$ope = "UNLOCK"       : release ring
;           "LOCK"         : lock ring
;   .$brk = "BREAK"        : BREAK instruction before operation
;         = ""             : no BREAK instruction before operation
;
  IF .$brk=="BREAK" THEN
    BREAK
  END
;
  $ope2 = .$ope+"_"+$ENCODE(/F1.0,.holder_numb)
  SCASE $ope2 OF
   SVALUE "UNLOCK_1":
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(-x_cutt_lock[1],x_cutt_unlock[1]) DO
      SIGNAL -y_lock_cutt[1]
      PULSE y_unlock_cutt[1]
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(7)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.1
    END
;
   SVALUE "LOCK_1":
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_cutt_lock[1],-x_cutt_unlock[1]) DO
      SIGNAL -y_unlock_cutt[1]
      PULSE y_lock_cutt[1]
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(8)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.1
    END
;
   SVALUE "UNLOCK_2":
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(-x_cutt_lock[2],x_cutt_unlock[2]) DO
      SIGNAL -y_lock_cutt[2]
      PULSE y_unlock_cutt[2]
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(9)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.1
    END
;
   SVALUE "LOCK_2":
    UTIMER .@r_tim_wait = 0
    WHILE NOT SIG(x_cutt_lock[2],-x_cutt_unlock[2]) DO
      SIGNAL -y_unlock_cutt[1]
      PULSE y_lock_cutt[2]
      IF UTIMER(.@r_tim_wait)>3 THEN
        CALL alarm(10)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.1
    END
;
   ANY :
    PAUSE
    HALT
  END
;
  RETURN
.END
.PROGRAM cutter_select() #0
;Choose cutter and generate 'cutter_req'
;
; NOTE: all cutters must be present in the cell!
;
;Verify the last used cutter
  IF last_cutter==0 THEN
    IF cutter_status>0 THEN
      last_cutter = cutter_status
    ELSE
      last_cutter = 1
    END
  END
;
;Search for a new cutter
  cutter_req = last_cutter
  CASE last_cutter OF
   VALUE 1:
    IF cutter_life[1]<max_cutt_life[1] THEN
      cutter_req = 1
    ELSE
      IF cutter_life[2]<max_cutt_life[2] THEN
        cutter_req = 2
      END
    END
   VALUE 2:
    IF cutter_life[2]<max_cutt_life[2] THEN
      cutter_req = 2
    ELSE
      IF cutter_life[1]<max_cutt_life[1] THEN
        cutter_req = 1
      END
    END
  END
;
  RETURN
.END
.PROGRAM cutter_verify() #170438768
;Verify 'cutter_status' variable coherence
;
; NOTE: all cutters must be present in the cell!
;
retry:
  IF cutter_status<>0 THEN
    IF SIG(x_cutt_pres[1],x_cutt_pres[2]) THEN
      cutter_status = 0
    END
  END
;
  CASE cutter_status OF
   VALUE 0:
    IF SIG(-x_cutt_pres[1]) THEN
      CALL alarm(1)
      GOTO retry
    END
    IF SIG(-x_cutt_pres[2]) THEN
      CALL alarm(2)
      GOTO retry
    END
;
   VALUE 1:
    IF SIG(x_cutt_pres[1]) THEN
      CALL alarm(3)
      GOTO retry
    END
    IF SIG(-x_cutt_pres[2]) THEN
      CALL alarm(2)
      GOTO retry
    END
;
   VALUE 2:
    IF SIG(-x_cutt_pres[1]) THEN
      CALL alarm(1)
      GOTO retry
    END
    IF SIG(x_cutt_pres[2]) THEN
      CALL alarm(4)
      GOTO retry
    END
  END
;
  RETURN
.END
.PROGRAM display() #0
;Display some information on terminal
;
  TYPE " "
  TYPE "=== DISPLAY ================================================================"
  TYPE " Clamp_status .............. = ",clamp_status
  TYPE " Spin_status ............... = ",spin_status
  TYPE " Cutter_status ............. = ",cutter_status
  TYPE " Tab_part_status[1,2,3,4] .. = ",tab_part_status[1],tab_part_status[2],tab_part_status[3],tab_part_status[4]
  TYPE " Tab_part_status[5,6,7,8] .. = ",tab_part_status[5],tab_part_status[6],tab_part_status[7],tab_part_status[8]
  TYPE " R1_part_status[1,2,3,4] ... = ",r1_part_status[1],r1_part_status[2],r1_part_status[3],r1_part_status[4]
  TYPE " Cutter_life[1] (max) ...... = ",/F6.0,cutter_life[1],"  (",max_cutt_life[1],")"
  TYPE " Cutter_life[2] (max) ...... = ",/F6.0,cutter_life[2],"  (",max_cutt_life[2],")"
  TYPE " Last_cutter ............... = ",last_cutter
  TYPE " Mission ID (act/last) ..... = ",miss_exe," /",miss_last
  TYPE " Last Movehome ID .......... = ",last_movehome
  TYPE " "
  TYPE " Actual alarm .............. = ",$act_alarm_time," (",act_alarm,") "
  TYPE " Last alarm ................ = ",$last_alarm_time," (",last_alarm,") "
  TYPE " Idle time ................. = ",idle_time
  TYPE " "
  TYPE " apn (fpn/lpn) ............. = ",apn,"  (",fpn,"/",lpn,")"
  TYPE " acn ....................... = ",acn
  TYPE "============================================================================"
;
  RETURN
.END
.PROGRAM get_cycle_time() #706457
;Calculate cycle-time
;
  cycle_time = UTIMER(@cycle_time0)
  IF debug THEN
    TYPE /F6.2,"Cycle-time = ",cycle_time,"     (",/F1.0,apn,")"
  END
;
  RETURN
.END
.PROGRAM initialize() #26081
;Initialization
;
  CALL set_io
  CALL set_var
  CALL reset_ini
  CALL origins
  FOR .ii = 1 TO 4
    IF rcp_cut_ena[.ii]==1 THEN
      CALL chk_cutter_n(.ii)
    END
  END
  CALL chk_pc1_run
  CALL initpoints
  CALL cutter_verify
  CALL chk_cutt_clamp
;
  RETURN
  CALL link
.END
.PROGRAM initpoints() #859946
;Locations initialization
;
; +-- picking positions on ring table
  CALL calc_pos_tab(#rif_table1,#table1[1],#table2pick[1],#table2place[1],#table3[1],#table4[1])
; +-- placing positions on ring table
  CALL calc_pos_tab(#rif_table5,#table1[5],#table2pick[5],#table2place[5],#table3[5],#table4[5])
; +-- picking/placing position on spindle
  CALL calc_pos_spin(#spin_appro,#spin_pick,#spin_place)
;
  RETURN
.END
.PROGRAM link() #0
;
  CALL display
  CALL aaa_align
  CALL aaa_pos
  CALL aaa_sw_version
  CALL set_frame
  CALL zzz_recipe
  CALL cutter_select
  CALL muove_da1_a1
  CALL muove_da1_a5
  CALL muove_da1_a22
  CALL muove_da1_a25
  CALL muove_da5_a1
  CALL muove_da5_a5
  CALL muove_da5_a22
  CALL muove_da22_a1
  CALL muove_da22_a5
  CALL muove_da22_a22
  CALL muove_da25_a1
  CALL muove_da25_a25
;
  RETURN
.END
