.END
.PROGRAM set_out() #0
;Set uscite ad inizio ciclo
;
  SIGNAL -y_arb_ok_ch,-y_arb_st_free,-y_arb_est_ind,-y_arb_est_ava
  SIGNAL -y_nsca_start,-y_abilita_esa,-y_rs5_okpre
  SIGNAL y_rs5_fi,-y_luce_tele_on,-y_sp_tele_ap,-y_sp_tele_ch
  SIGNAL m_ok_read_cam1
;	+-- discesa sollevatore
  CALL soll_dw("BREAK","ON","ON",0)
  PULSE y_sp_tele_ch,1
  PULSE y_res_fine,1
;
  RETURN
.END
.PROGRAM set_spin_speed() #2108881
;Ask correct spindle speed setting to Robot1/KLogic
;
  IF acn>0 AND acn<5 THEN
    BITS y_analogica_ch1,16 = MINVAL(32760,((32760/3600)*(rcp_speed_mand[acn]*2)))
  ELSE
;
    BITS y_analogica_ch1,16 = MINVAL(32760,((32760/3600)*(rcp_speed_mand[1]*2)))
  END
;
  RETURN
.END
.PROGRAM set_tool(.$tool_name) #8159706
;Update tool dimension
;
  SCASE .$tool_name OF
   SVALUE "CUTTER":
    POINT .tool_req = t_cutter0+TRANS(,,rcp_blade_len) ;tool on blade tip
   SVALUE "CLAMP":
    POINT .tool_req = t_clamp
  END
;
  POINT .tool_act = TOOL
  IF DISTANCE(.tool_act,.tool_req)>1 THEN
    TOOL .tool_req
  END
;
  RETURN
.END
.PROGRAM set_var() #27143
;Setting of system constant
;
  max_rings = 1             ;Number of rings on the table (placed by robot 1)
  ena_rc_sensors = OFF      ;Enable ring clamp sensors
  max_cutters = 2           ;Number of cutters in the holder
  ena_simula = OFF          ;Enable simulated cycle
  b_cam_prep = FALSE        ;Start camera data
  b_cam_start = FALSE       ;Start camera acquisition
  b_cam_chk = FALSE         ;Start camera result
  b_cam_result = FALSE      ;Confirm camera result
;
  RETURN
;
;
;Subsequent lines are inserted only as a reference
;
; - speed setting variables
  chng_cutt_speed = 10      ;Change-cutter speed (%)
  chng_area_speed = 90      ;Change-area (table/spindle/table) speed (%)
  critical_speed = 50       ;Critical operation (cut/pick/place) speed (%)
  spd_correction = 1        ;Speed correction (for adaptative speed) (0..2)
;
; - status variables -
  clamp_status = 0          ;Ring clamp status [0=empty  1=raw ring    2=cutted ring]
  spin_status = 0           ;Spindle status [0=empty  1=raw ring  11/12/13/14=partially cutted ring  2=cutted ring  91/92/93/94=interrupted cut (reject)]
  cutter_status = 0         ;Cutter on board status [0=empty  1=cutter n.1  2=cutter n.2]
  tab_part_status[1] = 0    ;Table part1 status [0=empty  1=raw]
  tab_part_status[2] = 0    ;Table part2 status [0=empty  1=raw]
  tab_part_status[3] = 0    ;Table part3 status [0=empty  1=raw]
  tab_part_status[4] = 0    ;Table part4 status [0=empty  1=raw]
  tab_part_status[5] = 0    ;Table part5 status [0=empty  2=finished]
  tab_part_status[6] = 0    ;Table part6 status [0=empty  2=finished]
  tab_part_status[7] = 0    ;Table part7 status [0=empty  2=finished]
  tab_part_status[8] = 0    ;Table part8 status [0=empty  2=finished]
  r1_part_status[1] = 0     ;Robot1 part1 status [0=empty  1=raw]
  r1_part_status[2] = 0     ;Robot1 part2 status [0=empty  1=raw]
  r1_part_status[3] = 0     ;Robot1 part3 status [0=empty  1=raw]
  r1_part_status[4] = 0     ;Robot1 part4 status [0=empty  1=raw]
  apn = 1                   ;Actual Part Number [1...4]
  fpn = 1                   ;First Part Number [1...4]
  lpn = 1                   ;Last Part Number [1...4]
  acn = 1                   ;Actual Cut Number [1...4]
;
  cutter_life[1] = 0        ;Number of cuts operated by cutter 1
  cutter_life[2] = 0        ;Number of cuts operated by cutter 2
  max_cutt_life[1] = 1000   ;Max number of cuts for cutter 1
  max_cutt_life[2] = 1000   ;Max number of cuts for cutter 2
  hmi_jig_push = 1.5        ;Pushing movement of ring clamp on spindle
  hmi_corr_spin_x = 0       ;Spindle center correction X
  hmi_corr_spin_y = 0       ;Spindle center correction Y
  hmi_corr_spin_z = 0       ;Spindle center correction Z
;
; - recipe variables -
  rcp_source = 0            ;Raw rings source [0=Ring_table  1=Robot1_clamps]
  rcp_jig_height = 15       ;Mounting jig height [mm]
  rcp_blade_ang = 20        ;Blade angle (total - assuming symmetrical) [deg]
  rcp_blade_len = 15        ;Blade lenght [mm]
  rcp_clamp_mode = 0        ;Clamp ring locking mode [0=close4lock  1=open4lock]
  rcp_spin_mode = 0         ;Spindle ring locking mode [0=close4lock  1=open4lock]
  rcp_ext_diam = 150        ;Ring external diameter [mm]
  rcp_ext_height = 30       ;Ring external height [mm]
;
  FOR .x = 1 TO 4
    rcp_cut_ena[.x] = 0     ;Cut enable [0=disabled  1=enabled]
    rcp_region[.x] = 0      ;Ring cutting region [0=foreward   1=backward]
    rcp_cut_sp[.x] = 5      ;Cutting speed (robot movement) [mm/s]
    rcp_cut_ang1[.x] = 30   ;Cutting angle 1 (around cutting direction) [deg]
    rcp_cut_ang2[.x] = 3    ;Cutting angle 2 (around cutter axis) [deg]
    rcp_cut_diam[.x] = 120  ;Cutting diameter (2x radius from spindle center) [mm]
    rcp_cut_thick[.x] = 3   ;Cutting thickness (along cutter axis) [mm]
    rcp_cut_height[.x] = 10 ;Cutting height (from spindle plane) [mm]
    rcp_cut_lift[.x] = 4    ;Cutting lift distance [mm]
    rcp_cut_appro[.x] = 15  ;Cutting start point approach distance [mm]
    rcp_spin_rz[.x] = 0     ;Spindle (frame) rotation around Z axis [deg]
    rcp_blad_corr[.x] = -10 ;Blade angle correction [+/-deg]
    rcp_wait[.x] = 0        ;Waiting time after cutting [sec]
    rcp_wait_pre = 0        ;attesa prima della presa da mandrino
    rcp_mand_stop[.x] = 0
  END
;
  cutter_req = 0            ;Cutter choosed for the next cut [1 or 2]
  last_cutter = 0           ;Last cutter used by robot
;
.END
.PROGRAM spindle_clamp(.$ope,.$break,.$comm,.$sens) #2117841
;Spindle clamp management
;
;   .$ope = "UNLOCK"  : release ring
;           "LOCK"    : lock ring
;
  IF .$break=="BREAK" THEN
    BREAK
  END
  IF rcp_spin_mode==0 THEN
;Close4lock
    SCASE .$ope OF
     SVALUE "LOCK":
      .$ope2 = "CLOSE"
     SVALUE "UNLOCK":
      .$ope2 = "OPEN"
    END
  ELSE
;Open4lock
    SCASE .$ope OF
     SVALUE "LOCK":
      .$ope2 = "OPEN"
     SVALUE "UNLOCK":
      .$ope2 = "CLOSE"
    END
  END
;
  IF NOT SWITCH(REPEAT ) THEN
    DELAY 0.5
    BREAK
    RETURN
  END
;
  SCASE .$ope2 OF
   SVALUE "OPEN":
    IF .$comm=="ON" THEN
;     +-- controllo pompa arburg accesa
      WHILE NOT SIG(x_arb_pompa) DO
        CALL alarm(21)
      END
      SIGNAL -y_close_spin
      PULSE y_open_spin,1
    END
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(x_spin_open,-x_spin_closed) DO
        SIGNAL -y_close_spin
        PULSE y_open_spin
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(13)
          UTIMER .@time_out = 0
        END
        TWAIT 0.02
      END
    END
;
   SVALUE "CLOSE":
    IF .$comm=="ON" THEN
;     +-- controllo pompa arburg accesa
      WHILE NOT SIG(x_arb_pompa) DO
        CALL alarm(21)
      END
      SIGNAL -y_open_spin
      PULSE y_close_spin,1
    END
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(-x_spin_open,x_spin_closed) DO
        SIGNAL -y_open_spin
        PULSE y_close_spin
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(14)
          UTIMER .@time_out = 0
        END
        TWAIT 0.02
      END
    END
  END
;
  RETURN
.END
.PROGRAM spindle_rot(.$ope,.$comm,.$sens) #3516830
;Spindle operation
;
  SCASE .$ope OF
   SVALUE "START":
;   +-- comando
    IF .$comm=="ON" THEN
      IF rcp_fase_mand==1 THEN
        PULSE y_spin_unlook,1
        UTIMER .@time_out = 0
        WHILE SIG(x_spin_unlook,-x_spin_look) DO
          IF UTIMER(.@time_out)>5 THEN
            CALL alarm(23)
            UTIMER .@time_out = 0
          END
          TWAIT 0.02
        END
        WAIT SIG(x_spin_unlook)
      ELSE
        IF SIG(-x_spin_unlook,x_spin_look) THEN
          PULSE y_spin_unlook,1
          UTIMER .@time_out = 0
          WHILE SIG(x_spin_unlook,-x_spin_look) DO
            IF UTIMER(.@time_out)>5 THEN
              CALL alarm(23)
              UTIMER .@time_out = 0
            END
            TWAIT 0.02
          END
          WAIT SIG(x_spin_unlook)
        END
      END
;	
      PULSE y_spin_start,1
    END
;   +-- attesa
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(x_spin_rotating) DO
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(23)
          UTIMER .@time_out = 0
          PULSE y_spin_start,1
        END
        TWAIT 0.02
      END
    END
;
   SVALUE "STOP":
;   +-- comando
    IF .$comm=="ON" THEN
      PULSE y_spin_stop,0.3
    END
;   +-- attesa
    IF .$sens=="ON" THEN
      UTIMER .@time_out = 0
      WHILE NOT SIG(-x_spin_rotating) DO
        IF UTIMER(.@time_out)>5 THEN
          CALL alarm(24)
          UTIMER .@time_out = 0
          PULSE y_spin_stop,0.3
        END
        TWAIT 0.02
      END
    END
;
;   +-- controllo mandrino fermo e blocco orientato
    IF rcp_fase_mand==1 AND NOT SIG(x_spin_look) THEN
      UTIMER .@time_out = 0
      WAIT SIG(x_orient_spin,-x_spin_rotating)
      TWAIT 2 ; modifica qui!!
      TYPE 3: "twait"
      BREAK
      PULSE y_spin_look,1
;
      UTIMER .@time_out = 0
      WHILE SIG(x_spin_unlook,-x_spin_look) DO
        IF UTIMER(.@time_out)>10 THEN
          CALL alarm(23)
          UTIMER .@time_out = 0
        END
        TWAIT 0.02
      END
    END
;	
  END
;
  RETURN
.END
.PROGRAM spindle_verify(.spindle_empty) #2586436
;Verify spindle empty at start cycle
;
  .spindle_empty = TRUE
;
  IF rcp_spin_mode==0 THEN
;Close4lock
    IF NOT SIG(x_spin_open,-x_spin_closed) THEN
      .spindle_empty = FALSE
    END
  ELSE
;Open4lock
    IF NOT SIG(-x_spin_open,x_spin_closed) THEN
      .spindle_empty = FALSE
    END
  END
;
  RETURN
.END
.PROGRAM tacche_zero() #0
;porta il robot sullo zero encoder per verificare le tacche meccaniche
;posizione di home
  JMOVE #phome
;posizione vicina a zero encoder
  JMOVE #pos
;calcolo posizione di "zeri" precisi
  DECOMPOSE .pos[1] = #pos
;assegno ad ogni angolo il valore di zero gradi
  .pos[1] = 0
  .pos[2] = 0
  .pos[3] = 0
  .pos[4] = 0
  .pos[5] = 0
  .pos[6] = 0
;riconpongo precision point
  POINT #pos_zero = #PPOINT(.pos[1],.pos[1],.pos[1],.pos[1],.pos[1],.pos[1])
  JMOVE #pos_zero
;	
.END
.PROGRAM wait_mission() #838214
;Attesa operazioni da eseguire
;
  miss_last = miss_exe
  miss_exe = 0
  BREAK
  DO
;   +-- verificare coerenza variabile 'cutter_status'
    CALL cutter_verify
;
;
;   +-- manuale
    IF SIG(x_man_enable) THEN
;
;
;     +-- posa coltello (manuale)
      IF SIG(x_cutt_rel_req,x_r1_safe_table,y_home1) AND cutter_status>0 THEN
        miss_exe = 13
        RETURN
      END
;
;
;     +-- cambio tipo (manuale)
      IF SIG(x_chng_type_req,x_r1_safe_table,y_home1) THEN
        miss_exe = 14
        RETURN
      END
;
;
;     +-- controllo visivo coltello (manuale)
      IF SIG(x_cutt_chk_req,x_r1_safe_table,y_home1) AND cutter_status>0 THEN
        miss_exe = 25
        RETURN
      END
;
;
;     +-- movements to home position
      IF SIG(x_movehome_req) THEN
        miss_exe = 15
        RETURN
      END
;
    END
;
;
;   +-- automatico
    IF SIG(x_auto_enable) THEN
;
;     +-- ciclo automatico
      IF SIG(x_start_seq_req) AND SIG(x_r1_safe_table) AND SIG(x_r1_safe_spind) THEN
        IF rcp_cut_ena[1]==1 OR rcp_cut_ena[2]==1 OR rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1 THEN
          IF cutter_status<>0 AND SIG(m_raw_pres_p) THEN
            miss_exe = 2 ;taglio
            RETURN
          END
          IF cutter_status==0 THEN
            miss_exe = 5 ;cambio coltello
            RETURN
          END
        END
      END
    END
;
;   +-- operazioni manuali
    IF SIG(x_man_enable,x_r1_safe_table) THEN
      IF rcp_cut_ena[1]==1 OR rcp_cut_ena[2]==1 OR rcp_cut_ena[3]==1 OR rcp_cut_ena[4]==1 THEN
;
;       +-- ciclo solo taglio
        IF SIG(x_start_seq_req) THEN
          IF cutter_status<>0 AND SIG(m_raw_pres_p) THEN
            miss_exe = 11 ;taglio
            RETURN
          END
          IF cutter_status==0 THEN
            miss_exe = 16 ;cambio coltello
            RETURN
          END
        END
;
;       +-- richiesta taglio singolo
        IF SIG(x_tag_sing,y_home1) THEN
;         +-- se ho il coltello a bordo ed ho pezzi sul piano inclinato
          IF cutter_status<>0 AND SIG(m_raw_pres_p) THEN
;           +-- eseguo la sequenza di taglio
            miss_exe = 23
            RETURN
          END
;         +-- se non ho il coltello a bordo
          IF cutter_status==0 THEN
;           +-- presa coltello
            miss_exe = 24
            RETURN
          END
;         +-- se non ho pezzi sul piano inclinato
          IF NOT SIG(m_raw_pres_p) THEN
;           +-- reset richiesta taglio singolo
            PULSE y_res_tag_sing,1
;           +-- attesa reset taglio singolo
            UTIMER .@r_tim_wait = 0
            WHILE SIG(x_tag_sing) DO
              IF UTIMER(.@r_tim_wait)>5 THEN
                CALL alarm(41)
              END
              TWAIT 0.02
            END
          END
        END
      END
;
;     +-- ciclo test telecamera 3d
      IF SIG(x_test_cam,y_home1) THEN
;	    +-- se ho pezzi sul piano inclinato
        IF SIG(m_raw_pres_p) THEN
;           +-- eseguo la sequenza di controllo telecamera
          miss_exe = 26
          RETURN
        ELSE
;           +-- reset richiesta controllo telecamera
          PULSE y_res_test,3
;           +-- attesa reset taglio singolo
          UTIMER .@r_tim_wait = 0
          WHILE SIG(x_test_cam) DO
            IF UTIMER(.@r_tim_wait)>5 THEN
              CALL alarm(48)
            END
            TWAIT 0.02
          END
        END
      END
    END
;
;   +-- muove in home		
    IF SIG(x_end_cycle_req) AND NOT SIG(y_home1) THEN
      miss_exe = 4 ;muovihome
      RETURN
    END
;
;   +-- transfer ring from Robot1 to table
    IF SIG(m_tab_empty) THEN
      IF SIG(x_r1_direct) AND miss_last<>1 THEN
        miss_exe = 1
        RETURN
      END
    END
;
    TWAIT 0.05
  UNTIL miss_exe>0
;
  RETURN
.END
.PROGRAM wait_r1_safe() #1653473
;Wait for Robot1 in safe area from Table position
;
  UTIMER .@r_tim_wait = 0
  IF NOT SIG(x_r1_safe_table) THEN
    WHILE NOT SIG(x_r1_safe_table) DO
      IF UTIMER(.@r_tim_wait)>5 THEN
        CALL alarm(27)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.02
    END
  END
; +-- abilito controllo anticollisione con RS20
  ONI -x_r1_safe_table CALL arresta
;
  RETURN
.END
.PROGRAM wait_spindle_sa() #4318
;Wait for Robot1 in safe area from spindle position
;
  UTIMER .@r_tim_wait = 0
  IF NOT SIG(x_r1_safe_spind) THEN
    WHILE NOT SIG(x_r1_safe_spind) DO
      IF UTIMER(.@r_tim_wait)>20 THEN
        CALL alarm(32)
        UTIMER .@r_tim_wait = 0
      END
      TWAIT 0.02
    END
  END
; +-- abilito controllo anticollisione con RS20
  ONI -x_r1_safe_spind CALL arresta
;
  RETURN
.END
.PROGRAM x_move(.$target,.x_sens,.sp,.err) #708673
;Simulate an XMOVE command using LMOVE+BRAKE
;
;Preceeding error state
  IF .err==TRUE THEN
    IF debug THEN
      TYPE 3: "X_MOVE: error param already active"
    END
    RETURN
  END
;
;Signal is already active ---> error
  IF SIG(.x_sens) THEN
    .err = TRUE
    IF debug THEN
      TYPE 3: "X_MOVE: signal already active"
    END
    RETURN
  END
;
;Joint or location
  $pref = $LEFT(.$target,1)
  IF $pref=="#" THEN
    POINT .#target = STRTOPOS(.$target)
    POINT .target = .#target
  ELSE
    POINT .target = STRTOPOS(.$target)
    POINT .#target = .target
  END
;
;Motion
;ABS.SPEED ON
  DO
    ACCURACY 1
    SPEED .sp MM/S
    LMOVE .#target
    DO
      IF SIG(.x_sens) THEN
        .err = TRUE
        BRAKE
        ABS.SPEED OFF
        RETURN
      END
    UNTIL DISTANCE(.target,HERE)<1
  UNTIL DISTANCE(.target,HERE)<1
  ABS.SPEED OFF
;
  RETURN
.END
.PROGRAM zzz_recipe() #0
;Manually fill the recipe buffer (for machine test)
;
; - recipe variables -
  rcp_buffer[0] = -99999
  rcp_buffer[1] = 1    ; rcp_source         Raw rings source [0=Ring_table  1=Robot1_clamps]
  rcp_buffer[2] = 1.5  ; hmi_jig_push       Pushing movement of ring clamp on spindle
  rcp_buffer[3] = 0    ; hmi_corr_spin_x    Spindle center correction X
  rcp_buffer[4] = 0    ; hmi_corr_spin_y    Spindle center correction Y
  rcp_buffer[5] = 0    ; hmi_corr_spin_z    Spindle center correction Z
  rcp_buffer[6] = 30   ; Monitor Speed
  rcp_buffer[7] = 10   ; Program speed                        
  rcp_buffer[8] = 55   ; Max number of cuts for cutter 1 and 2
;rcp_buffer[ 9] = 0   ; Actual number of cuts of cutter 1 (read-only)
;rcp_buffer[10] = 0   ; Actual number of cuts of cutter 2 (read-only)
  rcp_buffer[11] = -99999
  rcp_buffer[12] = -99999
  rcp_buffer[13] = -99999
  rcp_buffer[14] = -99999
  rcp_buffer[15] = -99999
  rcp_buffer[16] = 15   ; rcp_jig_height     Mounting jig height [mm]                              
  rcp_buffer[17] = 20   ; rcp_blade_ang      Blade angle (total - assuming symmetrical) [deg]      
  rcp_buffer[18] = 15   ; rcp_blade_len      Blade lenght [mm]                                     
  rcp_buffer[19] = 0    ; rcp_clamp_mode     Clamp ring locking mode [0=close4lock  1=open4lock]   
  rcp_buffer[20] = 0    ; rcp_spin_mode      Spindle ring locking mode [0=close4lock  1=open4lock] 
  rcp_buffer[21] = 80   ; rcp_ext_diam       Ring external diameter [mm]                           
  rcp_buffer[22] = 15.5 ; rcp_ext_height     Ring external height [mm]   
;	
  rcp_buffer[23] = 0.2   ; rcp_wait[1] waiting time after cutting [sec]
  rcp_buffer[24] = -99999
  rcp_buffer[25] = 500  ; rcp_speed_mand[1]  Speed mandrino taglio 1
  rcp_buffer[26] = 1    ; rcp_cut_ena[1]     Cut enable [0=disabled  1=enabled]
  rcp_buffer[27] = 0    ; rcp_region[1]      Ring cutting region [0=foreward   1=backward]
  rcp_buffer[28] = 11   ; rcp_cut_sp[1]      Cutting speed (robot movement) [mm/s]                                   
  rcp_buffer[29] = 31   ; rcp_cut_ang1[1]    Cutting angle 1 (around cutting direction) [deg]                        
